"// SimpleCtrlScriptStart {invalid}
// SimpleCtrlScript {ChildPanelOnCentral}
// filename {examples/testchildpanel}
// panelname {examples/testchildpanel}
// anzahl_parameter {2}
// parameter1 {$parameter1:zentriertes}
// parameter2 {$parameter2:Childpanel}
// SimpleCtrlScriptEnd
main()
{
  string lan=getLocale(getActiveLang());

  ChildPanelOnCentral(\"examples/testchildpanel\",
    \"examples/testchildpanel\",
    makeDynString((strpos(lan,\"de\")==0)?\"$parameter1:zentriertes\":\"$parameter1:centered\",
                  (strpos(lan,\"de\")==0)?\"$parameter2:Childpanel\":\"$parameter2:childpanel\"));
}

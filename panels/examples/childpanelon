LANG:10001 34 Open child panel at fixed position
LANG:10004 30 在固定位置打开子面板
LANG:10000 35 Öffne Childpanel an fixer Position
LANG:10027 60 Откр.дочерн.панель на указ.позиц.
"// SimpleCtrlScriptStart {invalid}
// SimpleCtrlScript {ChildPanelOn}
// filename {examples/testchildpanel}
// panelname {examples/testchildpanel}
// anzahl_parameter {2}
// parameter1 {$parameter1:an fixer Position}
// parameter2 {$parameter2:(0,0)}
// x {0}
// y {0}
// SimpleCtrlScriptEnd
main()
{
  string lan=getLocale(getActiveLang());

  ChildPanelOn(\"examples/testchildpanel\",
    \"examples/testchildpanel\",
    makeDynString((strpos(lan,\"de\")==0)?\"$parameter1:an fixer Position\":\"$parameter1:at fixed position\",
                  \"$parameter2:(0,0)\"),
    0,0);
}

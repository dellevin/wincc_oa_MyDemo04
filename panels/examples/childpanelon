LANG:10001 35 Open child panel relative to object
LANG:10004 30 相对于对象打开子面板
LANG:10000 36 Öffne Childpanel relativ zum Objekt
LANG:10027 65 Откр.дочерн.панель  относит.объекта
"// SimpleCtrlScriptStart {invalid}
// SimpleCtrlScript {ChildPanelOnRelativ}
// filename {examples/testchildpanel}
// panelname {examples/testchildpanel}
// anzahl_parameter {2}
// parameter1 {$parameter1:relativ}
// parameter2 {$parameter2:zum Objekt}
// x {10}
// y {10}
// SimpleCtrlScriptEnd
main()
{
  string lan=getLocale(getActiveLang());

  ChildPanelOnRelativ(\"examples/testchildpanel\",
    \"examples/testchildpanel\",
    makeDynString((strpos(lan,\"de\")==0)?\"$parameter1:relativ\":\"$parameter1:relative\",
                  (strpos(lan,\"de\")==0)?\"$parameter2:zum Objekt\":\"$parameter2:to object\"),
    10,10);
}

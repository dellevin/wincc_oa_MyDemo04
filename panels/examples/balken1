1 8 91 E "main()
{

  dpConnect( \"work\", \"ExampleDP_bt.x1:_original.._value\",
                     \"ExampleDP_bt.x2:_original.._value\",
                     \"ExampleDP_bt.y1:_original.._value\",
                     \"ExampleDP_bt.y2:_original.._value\" );
}

work( string dp1, dyn_float x1,
      string dp2, dyn_float x2,
      string dp3, dyn_float y1,
      string dp4, dyn_float y2 )
{
                     
  dyn_string      col1, col2;
  string          s;
  int       i;
  int       n = dynlen( x1 );

  for(i = 1; i <= n; i++ )
  {
      col1[i] = \"\";
      col2[i] = \"green\";
  }
  
 
  setValue( \"balken\", \"data\", x1, x2, y1, y2, col1, col2 );

  setValue( \"balken\", \"flush\" );


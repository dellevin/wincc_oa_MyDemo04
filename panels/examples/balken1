1 367 16 E "main()
{
  int i;
  float	 xRange;
  dyn_float       x1, x2, y1, y2;

  getValue( \"balken\", \"xRange\", xRange );

   
   x1[1] = 0.0;
      x2[1] = 1.0;
      y1[1] = 10.0;
      y2[1] = 14.0;
      for(i = 2; i <= 4; i++ )
      {
        x1[i] = x2[i-1];
        x2[i] = x1[i] + i;
        y1[i] = y1[i-1] + i;
        y2[i] = y2[i-1] + i;
      }
      for(i = 5; i <= 8; i++ )
      {
        x1[i] = x2[i-1];
        x2[i] = x1[i] + 1.;
        y1[i] = y1[i-1] - 2.0;
        y2[i] = 8.;
      }
      for(i = 9; i <= 20; i++ )
      {
        x1[i] = x2[i-1];
        x2[i] = x1[i] + 1.;
        y1[i] = y1[i-1] + 2.0;
        y2[i] = i;
      }
      for(i = 21; i <= xRange; i++ )
      {
        x1[i] = x2[i-1];
        x2[i] = x1[i] + 1.;
        y1[i] = y1[i-1] - 2.0;
        y2[i] = i;
      }

  dpSet( \"ExampleDP_bt.x1:_original.._value\", x1,
         \"ExampleDP_bt.x2:_original.._value\", x2,
         \"ExampleDP_bt.y1:_original.._value\", y1,
         \"ExampleDP_bt.y2:_original.._value\", y2 );


0 0 0 0 0
1
"main()
{
  int number;


  getValue( \"balken\", \"xArrayShow\", number );
  setValue(\"\",\"number\",number);	

  if (number == 0)
  {
      setValue( \"xarraygrid\", \"visible\", 0 );
      setValue( \"texthoehe\", \"visible\", 0 );
      setValue( \"werthoehe\", \"visible\", 0 );
  }

}" 0
 "main()
{
  int i, j, number;
  dyn_float       x;
  dyn_string      farbe, bezeich;
  float     xRange;


  getValue(\"\",\"number\", number);	

  switch(number)
  {
    case  0:
      setValue( \"balken\", \"xArrayShow\", 0 );
      setValue( \"xarraygrid\", \"visible\", 0 );
      setValue( \"texthoehe\", \"visible\", 0 );
      setValue( \"werthoehe\", \"visible\", 0 );
      break;
    case  1:
      
      getValue( \"balken\", \"xRange\", xRange );
      setValue( \"xarraygrid\", \"visible\", 1 );
      setValue( \"texthoehe\", \"visible\", 1 );
      setValue( \"werthoehe\", \"visible\", 1 );
 
      x[1] = 0;
      for(i = 1; i <= xRange; i++ )
      {
       x[i+1] = x[i] + 1;
       if ( i%2 == 0 )
          farbe[i] = \"weiss\";
        else
          farbe[i] = \"Green\";
        if ( i%8 == 0 )
        {
          j = x[i];
          bezeich[i] = j;
        }
        else
          bezeich[i] = \"\";
      }
  
   
      setValue( \"balken\", \"xArray\", x, farbe, bezeich,
			  \"xArrayShow\", 1 );
      break;
    	
  }

  setValue( \"balken\", \"flush\" );

}" 0


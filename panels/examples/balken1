LANG:10001 13 Random values
LANG:10004 9 随机值
LANG:10000 12 Zufallswerte
LANG:10027 25 Случ.значения
"main()
{
  int i;
  float	 xRange;
  dyn_float       x1, x2, y1, y2;

  getValue( \"balken\", \"xRange\", xRange );


      x1[1] = 0.0;
      x2[1] = 0.8;
      y1[1] = 11.2;
      y2[1] = 10.8;
      for(i = 2; i <= 6; i++ )
      {
         x1[i] = x2[i-1];
         x2[i] = x1[i] + 1./(7-i)*4.;
         y1[i] = 12. - (rand()/32767.)*4.0;
         y2[i] = 9.;
      }
      for(i = 7; i <= 26; i++ )
      {
         x1[i] = x2[i-1];
         x2[i] = x1[i] + 4.;
         y1[i] = 12. - (rand()/32767.)*4.0;
         y2[i] = 12. - (rand()/32767.)*4.0;
         y2[i] = 11.;

     }
     for(i = 27; i <= xRange; i++ )
     {
       x1[i] = x2[i-1];
       x2[i] = x1[i] + 1./(i-25)*4.;
       y1[i] = 12. - (rand()/32767.)*5.0;
       y2[i] = 12. - (rand()/32767.)*5.0;
     }

  dpSet( \"ExampleDP_bt.x1:_original.._value\", x1,
         \"ExampleDP_bt.x2:_original.._value\", x2,
         \"ExampleDP_bt.y1:_original.._value\", y1,
         \"ExampleDP_bt.y2:_original.._value\", y2 );

}" 0

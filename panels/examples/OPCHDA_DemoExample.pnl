"main()
{
  dpOPCHDAResponse = cbResponseDP.selectedText;
  if(txtItem.text == \"\")
  {
    ChildPanelOnCentral(\"vision/MessageWarning\", \"History Read Failed\", makeDynString(getCatStr(\"opchdaClient\", \"histRead6\")));
    return;
  }
  if(!dpExists(dpOPCHDAResponse))
  {
    ChildPanelOnCentral(\"vision/MessageWarning\", \"History Read Failed\", makeDynString(getCatStr(\"opchdaClient\", \"histRead7\")));
    return;
  }
  clearData();
  if(!historyReadConnected)
  {
    dpConnect(\"getDataCB\", false, dpOPCHDAResponse + \".RequestId\");
    historyReadConnected = true;
  }
  int status = 0;
  lastHistoryReadRequest = \"REQUEST-\" + nextRequestId++;
  
  status = opchda_setHistoryRead(\"_\" + cbServers.selectedText, lastHistoryReadRequest, txtItem.text, 
                                 getStartTime(),getEndTime(), sbMaxValues.text, (int)getBounds() , dpOPCHDAResponse);
  if(status != 0)
    ChildPanelOnCentral(\"vision/MessageWarning\", \"History Read Failed\", makeDynString(getCatStr(\"opchdaClient\", \"histRead8\") + status));
  else
    wait_modul.visible = TRUE;
}

void getDataCB(string dp, string val)
{
  if(val == lastHistoryReadRequest)
  {
    fillData(dpOPCHDAResponse);
    dpDisconnect(\"getDataCB\", dp);
    historyReadConnected = false;
    
    wait_modul.visible = FALSE;
  }
}




"main()
{
  for(int i = 1; i <= 100; ++i)
  {
    if(dpExists(\"_opchdaResponse\"+i))
      continue;
    if(dpCreate(\"_opchdaResponse\"+i, \"_OPCHDAHistoryReadResponse\") == -1)
    {
      DebugN(getLastError());
      return;
    }
    
    while(!dpExists(\"_opchdaResponse\" + i))
      delay(0, 100);
    
    dpSet(\"_opchdaResponse\" + i + \".DataType:_original.._last_value_storage_off\", TRUE,
          \"_opchdaResponse\" + i + \".RequestId:_original.._last_value_storage_off\", TRUE,
          \"_opchdaResponse\" + i + \".Timestamps:_original.._last_value_storage_off\", TRUE,
          \"_opchdaResponse\" + i + \".DataValues:_original.._last_value_storage_off\", TRUE,
          \"_opchdaResponse\" + i + \".ValueStatusCodes:_original.._last_value_storage_off\", TRUE,
          \"_opchdaResponse\" + i + \".ReturnCode:_original.._last_value_storage_off\", TRUE);
    
    dyn_string items = cbResponseDP.items;
    dynAppend(items, \"_opchdaResponse\"+i);
    cbResponseDP.items = items;
    return;
  }

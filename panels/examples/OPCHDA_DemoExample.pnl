E "#uses \"opchdaClient.ctl\"
string dpOPCHDAClient;
string dpOPCHDAResponse;
string serverConnInfo = \"\";
uint serverState = 0;
int nextRequestId = 1;
string lastHistoryReadRequest = \"\";
int lastBrowseRequestNumber = 0;
int browseCBs = 0;
bool historyReadConnected = false;
bool g_bFlatBrowse = FALSE;
dyn_anytype values;
dyn_time timeStamps;

void updateClient()
{
  dyn_string dsServers = opchda_getServersFromClient(\"_\" + cbClients.selectedText);
  
  cbServers.items = dsServers;
  
  dpOPCHDAClient = \"_\" + cbClients.selectedText;
  updateServerData(\"_\" + cbServers.selectedText);
}

void updateServerData(string dpOPCHDAServer)
{
  if(dpOPCHDAServer == \"\")
    return;
  
  dpGet(dpOPCHDAServer + \".Config.ConnInfo\", serverConnInfo,
        dpOPCHDAServer + \".State.ServerState\", serverState);
  
  txtServerState.text = (serverState == 0 ? \"disconnected\" : \"connected\");
}

void clearData()
{
  lblCount.text = \"0\";
  table.deleteAllLines();
  trend.curveRemoveData(\"#1\", makeTime(3000,12,31));
}

void fillData(string dpOPCHDAResponse)
{
  string requestId;
  dyn_string dsTodo, dsDataType;  
  string datatype;
  int returnCode;
  dyn_string dataValues;
  dyn_bit32 valueStatusCodes;
  string dataType;
  dynClear(timeStamps);
  dynClear(values);
  int status = 0;
  status = opchda_getHistoryReadValues(dpOPCHDAResponse, requestId, returnCode, timeStamps,
                                       dataValues, valueStatusCodes, dataType);
  
  //not our last request
  if(requestId != lastHistoryReadRequest)
    return;
  
  tfReturnCode.text = returnCode;
  tfReturnCode.backCol = (returnCode == 0 ? \"green\" : \"red\");
  
  lblCount.text = dynlen(timeStamps);
  
  string error = \"\";  
  
  if(status != 0)
    error = getCatStr(\"opchdaClient\", \"histRead1\");
  
  if(returnCode != 0)
    error = getCatStr(\"opchdaClient\", \"histRead2\") + returnCode;
  
  if(dynlen(timeStamps) != dynlen(dataValues) || dynlen(timeStamps) != dynlen(valueStatusCodes))
    error = getCatStr(\"opchdaClient\", \"histRead3\");

  if(dynlen(timeStamps) == 0)
    error = getCatStr(\"opchdaClient\", \"histRead4\");

  if(error != \"\")
  {
    ChildPanelOnCentral(\"vision/MessageWarning\", \"History Read Failed\", makeDynString(error));
    return;
  }  
  
  switch(dataType)
  {
    
    case \"VT_BOOL\":
      datatype = \"bool\";
      for(int i = 1; i <= dynlen(dataValues); ++i)
        dynAppend(values, (bool)dataValues[i]);
      break;
    
    case \"VT_I4\":
      datatype = \"int\";
      for(int i = 1; i <= dynlen(dataValues); ++i)
        dynAppend(values, (int)dataValues[i]);
      break;
      
    case \"VT_UI4\":
      datatype = \"uint\";
      for(int i = 1; i <= dynlen(dataValues); ++i)
        dynAppend(values, (uint)dataValues[i]);
      break;
      
    case \"VT_R8\":
      datatype = \"real\";
      for(int i = 1; i <= dynlen(dataValues); ++i)
      {
        strreplace(dataValues[i], \",\", \".\");
        dynAppend(values, (float)dataValues[i]);
      }
      break;
    
    case \"VT_DATE\":
      datatype = \"date\";
      for(int i = 1; i <= dynlen(dataValues); ++i)
        dynAppend(values, (time)dataValues[i]);
      break;
      
    default:
      ChildPanelOnCentral(\"vision/MessageWarning\", \"History Read Failed\", makeDynString(getCatStr(\"opchdaClient\", \"histRead5\") + dataType));
      return;
  }
  
//   for(int i = 1; i <= dynlen(timeStamps); i++)
//   {
//     dynAppend(dsTodo, \"todo\");
//     dynAppend(dsDataType, datatype);
//   }

  dyn_string valueQualities;
  for(int i = 1; i <= dynlen(valueStatusCodes); ++i)
  {
    dyn_string tmp;
    opchda_getQualityBitInformation(valueStatusCodes[i], tmp);
    dynAppend(valueQualities, (string)tmp);
  }
    
  
  table.appendLines(dynlen(timeStamps), \"#1\", timeStamps, \"#2\", values, \"#3\", valueStatusCodes, \"#4\", valueQualities);
  
  for(int i = 1; i <= dynlen(values); ++i)
  {
    time t = (time)timeStamps[i];//scanTimeUTC(timeStamps[i]);
    trend.curveVal(\"#1\", (float)values[i], t);
//     table.appendLine(\"#1\", t, \"#2\", values[i], \"#3\", \"todo\", \"#4\", datatype);
  }
  trend.timeBegin(timeStamps[1] < timeStamps[dynlen(timeStamps)] ? timeStamps[1] : timeStamps[dynlen(timeStamps)]);
  trend.pointType(\"#1\", 5);
}

string getStartTime()
{
  bool b;
  string str;
  time t;
  
  b = cbNoStart.state(0);

  if (!b)
  {
    t = makeTime(tib_year.text, tib_month.text, tib_day.text,
                 tib_hour.text, tib_minute.text, tib_second.text);
    str = t;
  }
  else
  {
    str = \"\";
  }
  return str;
}

string getEndTime()
{
  bool b;
  string str;
  time t;
  
  b = cbNoEnd.state(0);
  if (!b)
  {
    t = makeTime(tie_year.text, tie_month.text, tie_day.text,
                 tie_hour.text, tie_minute.text, tie_second.text);
    str = t;
  }
  else
  {
    str = \"\";
  }
  return str;
}

int getBounds()
{
  bool b;
  
  b = cbWithBounds.state(0);
  
  return (int)b;
}
























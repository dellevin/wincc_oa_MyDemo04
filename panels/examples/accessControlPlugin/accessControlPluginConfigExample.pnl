selectionChanged(string id)
{

  dyn_string keys;
  string path = id;

  string dp;

  cnsGetId(path, dp);

  dp_text.text(dp);

  // check/debug correctly set properties
  bool ok = cnsGetPropertyKeys(path, keys);

  settings.text(\"\");
  config.text(path);

  dp_settings.state(0, false);
  dp_settings.state(1, false);
  dp_settings.state(2, false);

  warning.visible(true);


  for (int i = 1; i <= dynlen(keys); i++)
  {
    anytype retVal;
    string key = keys[i];
    cnsGetProperty(path, key, retVal);

    switch(key)
    {
      case \"Read\":
        if ( retVal == 6)
        {
          dp_settings.state(0, true);
          warning.visible(false);
          settings.text(settings.text + \"Read: 6 \");
        }
        else
        {
          settings.text(settings.text + \"Read: 0 \");
        }
      break;

      case \"Write\":
        if ( retVal == 6)
        {
          dp_settings.state(1, true);
          warning.visible(false);
          settings.text(settings.text + \"Write: 6 \");
        }
        else
        {
          settings.text(settings.text + \"Write: 0 \");
        }
      break;

      case \"View\":
        if ( retVal == 6)
        {
          dp_settings.state(2, true);
          warning.visible(false);
          settings.text(settings.text + \"View: 6 \");
        }
        else
        {
          settings.text(settings.text + \"View: 0 \");
        }
      break;
    }
  }


  if ( ok )
    dp_settings.enabled(true);
  else
    dp_settings.enabled(false);

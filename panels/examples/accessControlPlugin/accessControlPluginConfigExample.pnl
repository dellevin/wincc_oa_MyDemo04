E"main(int button, int state)
{
  string selctedLine = DP_TREE1.selectedItem();
  anytype hex;
  bool r,w,x;
  int set;

  selctedLine = dpSubStr(selctedLine, DPSUB_DP);

  if ( configName == \"CNSPlugin\")
  {
    selctedLine = cnsTreeObject.selectedItem();
  }
  else if ( !dpExists(selctedLine) )
  {
    return;
  }

  getValue(\"\", \"state\", 0, r);
  getValue(\"\", \"state\", 1, w);
  getValue(\"\", \"state\", 2, x);

  if ( !r && !w && !x )
  {
    warning.visible(true);
  }
  else
  {
    warning.visible(false);
  }

  switch(configName)
  {
    case \"GeneralConfigPlugin\":

      hex = \"0X\";
      hex = buildHex(hex, r);
      hex = buildHex(hex, w);
      hex = buildHex(hex, x);

      sscanf(hex, \"%x\", set);
      settings.text(set);
      dpSet(selctedLine + \".:_general.._int_01\", set);

      break;

    case \"CommonConfigPlugin\":

      hex = buildHex(hex, r);
      hex = buildHex(hex, w);
      hex = buildHex(hex, x);

//       hex += \"00\";
      if ( hex == \"000000\" )
      {
        hex = \"\";
      }

      dpSetDescription(selctedLine + \".\", hex);
      hex = dpGetDescription(selctedLine + \".\");
      settings.text(hex);

      break;

    case \"CNSPlugin\":

      settings.text(\"\");

      if ( !x && !r && !w )
      {
        setRight(selctedLine, \"View\",  -1);
        setRight(selctedLine, \"Read\",  -1);
        setRight(selctedLine, \"Write\",  -1);
      }
      else
      {
        if ( x )
        {
          setRight(selctedLine, \"View\",  6);
          settings.text(\"View: 6 \");
        }
        else
        {
          setRight(selctedLine, \"View\",  0);
          settings.text(\"View: 0 \");
        }

        if ( r )
        {
          setRight(selctedLine, \"Read\",  6);
          settings.text(settings.text + \"Read: 6 \");
        }
        else
        {
          setRight(selctedLine, \"Read\",  0);
          settings.text(settings.text + \"Read: 0 \");
        }

        if ( w )
        {
          setRight(selctedLine, \"Write\", 6);
          settings.text(settings.text + \"Write: 6 \");
        }
        else
        {
          setRight(selctedLine, \"Write\", 0);
          settings.text(settings.text + \"Write: 0 \");
        }
      }


      break;
  }
}

string buildHex(string hex, bool state)
{
  if ( state )
  {
    hex += \"06\";
  }
  else
  {
    hex += \"00\";
  }

  return hex;
}

bool setRight(string path, string key, int value)
{
  bool result;
  dyn_string keys;

  cnsGetPropertyKeys(path, keys);

  if (value >= 0)
  {
    result = cnsSetProperty(path, key, value);
  }
  else if (dynContains(keys, key) > 0)
  {
    result = cnsRemoveProperty(path, key);
  }
  else
  {
    result = true;
  }

  delay(0, 50);  // take care: the CNS functions don't wait for messages to be acomplished

  return result;
}


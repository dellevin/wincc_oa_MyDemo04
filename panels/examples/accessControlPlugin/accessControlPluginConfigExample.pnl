"#uses \"CtrlCNS\"
#uses \"panel\"
#uses \"CtrlPv2Admin\"

main()
{
  bool dfltConfig;
  langString text;

//   warning.visible(true);
  warning.visible(false);

  //check for config
  configName = paCfgReadValueDflt(getPath(CONFIG_REL_PATH, \"config\"), \"general\", \"accessControlPlugin\", \"\", dfltConfig); //accessControlPlugin

  //check for root
  if (getUserId() != 0 || dfltConfig )
  {
    //disable panel
    preview.enabled(FALSE);
    PANEL_REF2.enabled(FALSE);
    DP_TREE1.enabled(FALSE);

    if ( getUserId() != 0 )
    {
      //dialog when not root
      ChildPanelOnCentralModal(\"vision/messages/MessageInfo1\", \"Info\",
                             makeDynString(\"$1:Current user does not have the required permissions.\"));
    }

    if ( dfltConfig )
    {
      //dialog when no config is found
      ChildPanelOnCentralModal(\"vision/messages/MessageInfo1\", \"Info\",
                               makeDynString(\"$1:Config entry \\\"accessControlPlugin\\\" was not found.\"));
    }

    return;
  }

  //check for guest & permission bit

  dyn_float fresult;
  dyn_string sresult;
  bool loop = true;

  while(loop)
  {
    unsigned guestId = getUserId(\"guest\");

    if ( guestId == 65535 || !getUserPermission( 6, guestId) || !getUserPermission( 1, guestId) )
    {
      //disable panel
      preview.enabled(FALSE);
      PANEL_REF2.enabled(FALSE);
      DP_TREE1.enabled(FALSE);

      //dialog when not root

      ChildPanelOnCentralModalReturn(\"examples/accessControlPlugin/PermissionInfo.pnl\", \"Permission\",
                                     makeDynString(\"\"), fresult, sresult);

      if ( dynlen(sresult) <= 0 || sresult[1] == \"false\")
      {
        PanelOff();
      }

    }
    else
    {
      preview.enabled(TRUE);
      PANEL_REF2.enabled(TRUE);
      DP_TREE1.enabled(TRUE);
      loop = false;
    }
  }

  lblHeader.text = lblHeader.text + configName;
  setValue(configName, \"visible\", TRUE);

  string view = strrtrim(getSystemName(), \":\") + \".Permissions\";
  string dp;

  //check for dps/cns
  switch(configName)
  {
    case \"CommonConfigPlugin\":
    case \"GeneralConfigPlugin\":

      if(!dpExists(\"Example_a\") || !dpExists(\"Example_b\") || !dpExists(\"Example_c\"))
      {
        createData();
      }
      break;

    case \"CNSPlugin\":

      PANEL_REF2.visible(true);
      DP_TREE1.visible(false);

      //create CNS
      if(!cns_nodeExists(view + \":ExampleNode_a\") ||!cns_nodeExists(view + \":ExampleNode_b\") || !cns_nodeExists(view + \":ExampleNode_c\"))
      {
        createData();
      }

      //missing datapoint entry for node
      cnsGetId(view + \":ExampleNode_a\", dp);
      if ( dp == \"\" )
      {
        cnsChangeNodeData(view + \":ExampleNode_a\" , \"Example_a.\");
      }

      cnsGetId(view + \":ExampleNode_b\", dp);
      if ( dp == \"\" )
      {
        cnsChangeNodeData(view + \":ExampleNode_b\" , \"Example_b.\");
      }

      cnsGetId(view + \":ExampleNode_c\", dp);
      if ( dp == \"\" )
      {
        cnsChangeNodeData(view + \":ExampleNode_c\" , \"Example_c.\");
      }

      break;
  }


}

void createData()
{
  //dialog for creation
  dyn_float resultFloat;
  dyn_string resultText;
  string view;

  ChildPanelOnCentralModalReturn(\"vision/messages/MessageInfo3\", \"Create Data\",
                                 makeDynString(\"$1:Would you like to create new data points or CNS views automatically?\",\"$2:Yes\",\"$3:No\",\"$4:Cancel\"),
                                 resultFloat, resultText);

  if ( resultText[1] == \"abbruch\" || resultText[1] == \"false\" )
  {
    return;
  }

  //create data
  if(!dpExists(\"Example_a\"))
  {
    createGeneralConfigDP(\"Example_a\");
  }

  if(!dpExists(\"Example_b\"))
  {
    createGeneralConfigDP(\"Example_b\");
  }

  if(!dpExists(\"Example_c\"))
  {
    createGeneralConfigDP(\"Example_c\");
  }

  if ( configName == \"CNSPlugin\")
  {
    view = strrtrim(getSystemName(), \":\") + \".Permissions\";

    if(!cns_viewExists(view))
    {
      cnsCreateView(view, \"Permission\");
    }

    if(!cns_nodeExists(view + \":ExampleNode_a\"))
    {
      cns_createTreeOrNode(view + \":\", \"ExampleNode_a\", \"ExampleNode_a\", \"Example_a\", 2);
    }

    if(!cns_nodeExists(view + \":ExampleNode_b\"))
    {
      cns_createTreeOrNode(view + \":\", \"ExampleNode_b\", \"ExampleNode_b\", \"Example_b\", 2);
    }

    if(!cns_nodeExists(view + \":ExampleNode_c\"))
    {
      cns_createTreeOrNode(view + \":\", \"ExampleNode_c\", \"ExampleNode_c\", \"Example_c\", 2);
    }
  }
}

createGeneralConfigDP(string name)
{
  dpCreate(name, \"ExampleDP_Int\");
  delay(0, 100);
  dpSet(name + \".:_general.._type\", DPCONFIG_GENERAL);
  dpSet(name + \".:_general.._int_01\", 394758);
}





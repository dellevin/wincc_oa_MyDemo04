}

/*
  @author Bernhard Reichl
  Set value of a variable
  @param sF Is the value to be set
*/
public setVal(float sF)
{
  val = sF;
  VAL.text = var2Str(format,val);
}

/*
  @author Bernhard Reichl
  Set units label
  @param sU Is the untis label
*/
public setUnit(string sU)
{
  unit = sU;
}

/*
  @author Bernhard Reichl
  Set title
  @param sT Is the title
*/
public setTitle(string sT)
{
  title = sT;
  TITLE.text = sT + unit;
}

/*
  @author Bernhard Reichl
  Get the given variable in the given format
  @params
    - sFormat Is the format of the variable
    - var Is the variable value
  @return title
*/
string var2Str(string sFormat, anytype var)
{
  string s;
  sprintf(s, sFormat, var);
  return s;
}

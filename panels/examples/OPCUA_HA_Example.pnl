E "string g_ResponseDp;
string g_ResponseDpOld;
bool g_bMulti;

void enableBegin(bool bEnabled)
{
  setMultiValue(\"tib_year\",   \"enabled\", bEnabled,
                \"tib_month\",  \"enabled\", bEnabled,
                \"tib_day\",    \"enabled\", bEnabled,
                \"tib_hour\",   \"enabled\", bEnabled,
                \"tib_minute\", \"enabled\", bEnabled,
                \"tib_second\", \"enabled\", bEnabled,
                \"tib_today\",  \"enabled\", bEnabled);
}

void enableEnd(bool bEnabled)
{
  setMultiValue(\"tie_year\",   \"enabled\", bEnabled,
                \"tie_month\",  \"enabled\", bEnabled,
                \"tie_day\",    \"enabled\", bEnabled,
                \"tie_hour\",   \"enabled\", bEnabled,
                \"tie_minute\", \"enabled\", bEnabled,
                \"tie_second\", \"enabled\", bEnabled,
                \"tie_today\",  \"enabled\", bEnabled);
}

void refreshConnect()
{
  if (g_ResponseDp != g_ResponseDpOld)
  {
    if (g_ResponseDpOld != \"\")
    {
      dpDisconnect(\"processResponse\",
                   g_ResponseDpOld + \".RequestId\",
                   g_ResponseDpOld + \".ReturnCode\",
                   g_ResponseDpOld + \".ResultCodes\",
                   g_ResponseDpOld + \".NodeStatusCodes\",
                   g_ResponseDpOld + \".SourceTimestamps\",
                   g_ResponseDpOld + \".DataValues\",
                   g_ResponseDpOld + \".ValueStatusCodes\",
                   g_ResponseDpOld + \".DpList\");
    }
    g_ResponseDpOld = g_ResponseDp;
    dpConnect(\"processResponse\", FALSE,
              g_ResponseDp + \".RequestId\",
              g_ResponseDp + \".ReturnCode\",
              g_ResponseDp + \".ResultCodes\",
              g_ResponseDp + \".NodeStatusCodes\",
              g_ResponseDp + \".SourceTimestamps\",
              g_ResponseDp + \".DataValues\",
              g_ResponseDp + \".ValueStatusCodes\",
              g_ResponseDp + \".DpList\");
  }
}

void processResponse(string dpe1, string reqId,
                     string dpe2, int returnCode,
                     string dpe3, dyn_int resultCodes,
                     string dpe4, dyn_bit32 nodeStatusCodes,
                     string dpe5, dyn_string sourceTs,
                     string dpe6, dyn_string dataValues,
                     string dpe7, dyn_bit32 valueStatusCodes,
                     string dpe8, dyn_string dpList)
{
  string strNodeStatus;
  int sumRc = returnCode;
  shape lshape;
 
  if(!g_bMulti)
    lshape = getShape(\"resultTab\");
  else
    lshape = getShape(\"tblAddress\");
  
  lshape.deleteAllLines();
  
  for(int i = 1; i <= (g_bMulti ? dynlen(dpList) : dynlen(sourceTs)); i++)
  {
    string hexStatus;
    sprintf(hexStatus, \"%08x\", (g_bMulti ? (uint)nodeStatusCodes[i] : (uint)valueStatusCodes[i]));
    
    if(!g_bMulti)
      lshape.appendLine (\"time\", sourceTs[i],
                         \"value\", dataValues[i],
                         \"status\", hexStatus);
    else
    {
      lshape.appendLine(\"dpe\", dpList[i],
                        \"status\", hexStatus);
      
      if(nodeStatusCodes[i][31] == 0 && nodeStatusCodes[i][30] == 0)
        lshape.cellBackColRC(i - 1, \"status\", \"green\");
      else
        lshape.cellBackColRC(i - 1, \"status\", \"red\");
    }
  }
  
  // generate a summary return code
  for (int i = 1; i <= dynlen(resultCodes); i++)
  {
     if (sumRc == 0 && resultCodes[i] != 0)
       sumRc = resultCodes[i];
  }
  
  tfReturnCode.text(sumRc);
  if (sumRc == 0)
    tfReturnCode.backCol(\"green\");
  else
    tfReturnCode.backCol(\"red\");
  
  if(g_bMulti)
    lblCount.text = dynlen(dpList);
  else
    lblCount.text = dynlen(sourceTs);
  
  if(!g_bMulti)
  {
    tfNodeStatus.visible = TRUE;
    lblNodeStatus.visible = TRUE;
    if (dynlen(nodeStatusCodes) > 0)
    {
      sprintf(strNodeStatus, \"%08x\", (uint)nodeStatusCodes[1]);
  
      if (nodeStatusCodes[1][31] == 0 && nodeStatusCodes[1][30] == 0)
        tfNodeStatus.backCol(\"green\");
      else
        tfNodeStatus.backCol(\"red\");
    }
    tfNodeStatus.text(strNodeStatus);
  }
  else
  {
    tfNodeStatus.visible = FALSE;
    lblNodeStatus.visible = FALSE;
  }
  
  wait_modul.visible = FALSE;
}

string getItemDescriptor()
{
  string str = txtItem.text;
  
  return str;
}

void setItemDescriptor(string str)
{
  txtItem.text(str);
  
}

string getStartTime()
{
  bool b;
  string str;
  time t;
  
  b = cbNoStart.state(0);

  if (!b)
  {
    t =makeTime(tib_year.text, tib_month.text, tib_day.text,
                tib_hour.text, tib_minute.text, tib_second.text);
    str = t;
  }
  else
  {
    str = \"\";
  }
  return str;
}

string getEndTime()
{
  bool b;
  string str;
  time t;
  
  b = cbNoEnd.state(0);
  if (!b)
  {
    t =makeTime(tie_year.text, tie_month.text, tie_day.text,
                tie_hour.text, tie_minute.text, tie_second.text);
    str = t;
  }
  else
  {
    str = \"\";
  }
  return str;
}

int getBounds()
{
  bool b;
  
  b = cbWithBounds.state(0);
  
  return (int)b;
}

void setBounds(int v)
{
  if (v)
    cbWithBounds.state(0, 1);
  else
    cbWithBounds.state(0, 0);
}

void switchTables()
{
  int iNumber = radVariant.number;
  
  if(iNumber == 2)
  {
    tblAddress.visible = TRUE;
    resultTab.visible = FALSE;
    g_bMulti = TRUE;
  }
  else
  {
    tblAddress.visible = FALSE;
    resultTab.visible = TRUE;
    g_bMulti = FALSE;
  }
}



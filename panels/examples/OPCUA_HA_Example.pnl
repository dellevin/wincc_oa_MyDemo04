"main()
{
  wait_modul.backCol(\"WF_EwoWaitBackground\");
  wait_modul.ModuleName = \"pls_wait\";
  wait_modul.visible = TRUE;
  
  dyn_int diSize = getPanelSize(panelFileName(myModuleName(), myPanelName()));
  wait_modul.size(diSize[1],diSize[2]);
  RootPanelOnModule(\"vision/wf/objects/Ewo_PlsWait.pnl\", \"pls_wait\", \"pls_wait\", makeDynString(\"$1:\"+diSize[1],\"$2:\"+diSize[2]));
  
  resultTab.alternatingRowColors(makeDynString(\"_AlternatingRowColorLight\", \"_AlternatingRowColorDark\"));
  tblAddress.alternatingRowColors(makeDynString(\"_AlternatingRowColorLight\", \"_AlternatingRowColorDark\"));
  
  int i;
  // fill server list
  
  dyn_string dsServers = dpNames(\"*\", \"_OPCUAServer\");
  dyn_string dsRespDps = dpNames(\"*\", \"_OPCUAHistoryReadResponse\");
  // fill response DP list
  
  // remove system names
  // and leading '_'
  for (i = 1; i <=dynlen(dsServers); i++)
  {
    dsServers[i] = substr(dpSubStr(dsServers[i], DPSUB_DP), 1);
  }
  
  // remove only system name
  for (i = 1; i <=dynlen(dsRespDps); i++)
    dsRespDps[i] = dpSubStr(dsRespDps[i], DPSUB_DP);

  cmbEquipment.items(dsServers);
  cmbRespDps.items(dsRespDps);
  lblCount.text = \"\";
  
  wait_modul.visible = FALSE;
  tblAddress.visible = FALSE;
}

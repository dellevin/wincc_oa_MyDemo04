string getTagType(anytype aCtlValue, string sTagType)
{
  string sRet;

  if ( sTagType == \"Dbpos\" )
    sRet = getCatStr(\"iec61850\", \"TagTypeDbposState\" + aCtlValue);
  else
    sRet = aCtlValue;

  return sRet;
}


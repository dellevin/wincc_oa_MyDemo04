"float mf_Min;
float mf_Max;
float mfNewValue;
string mDPE;
main()
{
 dyn_errClass err;
 bool b_PvRange=$b_PvRange;
 int iPvType=DPCONFIG_NONE;
 
 mDPE=$dpe_value;
 
 if(b_PvRange)
 dpGet($dpe_value + \":_pv_range.._type\", iPvType);
 
 if(iPvType!=DPCONFIG_NONE)
    dpConnect(\"pointer_scaleCB\",$dpe_value +\":_pv_range.._min\",
                              $dpe_value +\":_pv_range.._max\");                           
 else
   STD_pointer_Text($f_Min,$f_Max);

 
 
 dpConnect(\"STD_textFieldInCB\",
            $dpe_value+\":_online.._value\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"Value\", \"color\", \"_dpdoesnotexist\");
 
 dpConnect(\"STD_setRotationCB\",
            $dpe_value+\":_online.._value\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"pointer1\", \"color\", \"_dpdoesnotexist\");


 
 dpConnect(\"STD_setBackColorCB\", 
            $dpe_value+\":_alert_hdl.._act_state_color\");


}

pointer_scaleCB(string dp1,float f_Min, string dp2, float f_Max)
{
 STD_pointer_Text(f_Min,f_Max);
}

STD_pointer_Text(float fMin,float fMax)
{
 int iRanges,iTyp;
 float fLimit;
 float fstep=(fMax-fMin)/4;
 mf_Min=fMin;
 mf_Max=fMax;
 
 
  setMultiValue(\"T1\",\"text\",fMin,
               \"T2\",\"text\",fMin+fstep,
               \"T3\",\"text\",fMin+2*fstep,
               \"T4\",\"text\",fMin+3*fstep,
               \"T5\",\"text\",fMax);
               
  dpGet(mDPE + \":_alert_hdl.._type\", iTyp);
  if(iTyp!=DPCONFIG_NONE)
   {          
    dpGet(mDPE + \":_alert_hdl.._num_ranges\", iRanges);
    dpGet(mDPE + \":_alert_hdl.\"+iRanges+\"._l_limit\", fLimit);
    STD_pointer_alert_scale(fMin,fMax,fLimit);
   }

}



STD_setRotationCB(string dp1, float fNewValue)
{
float MIN_VALUE = mf_Min;
float MAX_VALUE = mf_Max;
float MIN_ROTATION=0;
float MAX_ROTATION=90;

float fRotation;
  fRotation = ( -1.0 * (MAX_ROTATION - MIN_ROTATION) / (MAX_VALUE - MIN_VALUE)) * 
              (fNewValue - MIN_VALUE) + MIN_ROTATION;
  
  if(fRotation>0) fRotation=0;
  if(fRotation<-90) fRotation=-90;

  setValue(\"pointer1\", \"rotation\", fRotation);
  setValue(\"pointer2\", \"rotation\", fRotation);
  mfNewValue=fNewValue;
}


STD_setBackColorCB(string dpSource, string sNewColor)
{ 
   setValue(\"Value\", \"backCol\", sNewColor);
}


STD_textFieldInCB(string dp, float fNewValue)
{
  setValue(\"Value\", \"text\", dpValToString(dp, fNewValue, FALSE) );
}

STD_pointer_alert_scale(float fMin,float fMax,float fLimit)

{
float fShow;
int i,iVisible;


 fShow=9-(((fLimit-fMin)/(fMax-fMin))*9);
 iVisible=fShow;
 if(iVisible>9)
  {
   iVisible=9;
   fShow=0;
  }
 else if(iVisible<0)
  {
   iVisible=0;
   fShow=0;
  }
 else
  fShow=fShow-iVisible;

 for(i=iVisible+1;i<=9;i++)
  setValue(\"w\"+i,\"visible\",0,\"rotation\",0);
 
 
 if(iVisible>0)
 for(i=1;i<=iVisible;i++)
  setValue(\"w\"+i,\"visible\",1,\"rotation\",0);

 
 if(fShow>0)
  setValue(\"w\"+(iVisible+1),\"visible\",1,\"rotation\",fShow*10-10);
 
 STD_setRotationCB(\"Dummy\", mfNewValue);

 
}




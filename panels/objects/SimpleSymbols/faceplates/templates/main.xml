   <prop name="ru_RU.utf8"></prop>
  </prop>
  <prop name="Size">191 442</prop>
  <prop name="BackColor">_3DFace</prop>
  <prop name="RefPoint">5 5</prop>
  <prop name="InitAndTermRef">True</prop>

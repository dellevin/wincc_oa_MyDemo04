////////////////////////////////////////////////////////////////////////////////////////////////////

sendCommand(string sCommand)
{
  string sConnection = cmbConnection.text;
  anytype aValue;

  if (sConnection == \"\")
    return;

  switch (sCommand)
  {
    case \"GQ\":
      aValue = (int)0;
      break;
    default:
      DebugN(\"OPCUA Client Command: case not implemented!\",sCommand);
      return;
      break;
  }

  if (dpExists(getSystemName() + OPCUADRIVER_sConnectionDpPrefix + sConnection + \".Command.\"+sCommand))
  {
    dpSet(getSystemName() + OPCUADRIVER_sConnectionDpPrefix + sConnection + \".Command.\"+sCommand, aValue);
  }
  else
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\", \"\", makeDynString(\"$1:\" + getCatStr(\"opcua\", \"applyBeforEditSubscr\")));
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

/*
 IM 106046 mPokorny 06.02.2012 : dont show connection if is DPT \"_OPCUAServer\" without DPs
   + show server states in fields \"ServerState\", \"tfReduServerState\" ,\"tfServerState_redu\" ,\"tfReduServerState_redu\"
*/
setTxtServers()//IM 104864
{
  if(!mappingHasKey(gOpcUaConn_mData[\"Connection\"],cmbConnection.text()))
    return;

  string s1a = gOpcUaConn_mData[\"Connection\"][cmbConnection.text()][\"ConnInfo\"];
  string s1b = gOpcUaConn_mData[\"Connection\"][cmbConnection.text()][\"ConnInfo\"];
  string s2a = gOpcUaConn_mData[\"Connection\"][cmbConnection.text()][\"ReduConnInfo\"];
  string s2b = gOpcUaConn_mData[\"Connection\"][cmbConnection.text()][\"ReduConnInfo\"];
  bool   bOPCRedu = gOpcUaConn_mData[\"Connection\"][cmbConnection.text()][\"ReduActive\"];

  bool   bIsRedundant = isRedundant();

  s1a = substr(s1a,strpos(s1a,\"//\")+2);//disacard opc.tcp://
  s1b = substr(s1b,strpos(s1b,\"//\")+2);
  s2a = substr(s2a,strpos(s2a,\"//\")+2);
  s2b = substr(s2b,strpos(s2b,\"//\")+2);

  int i1a = strpos(s1a,\":\");//discard :<portNo>
  int i1b = strpos(s1b,\":\");
  int i2a = strpos(s2a,\":\");
  int i2b = strpos(s2b,\":\");

  int j1a = strpos(s1a,\"/\");//discard /Pathinfo
  int j1b = strpos(s1b,\"/\");
  int j2a = strpos(s2a,\"/\");
  int j2b = strpos(s2b,\"/\");

  if(i1a > 0) s1a = substr(s1a,0,i1a);
  else if(j1a > 0) s1a = substr(s1a,0,j1a);
  if(i1b > 0) s1b = substr(s1b,0,i1b);
  else if(j1b > 0) s1b = substr(s1b,0,j1b);
  if(i2a > 0) s2a = substr(s2a,0,i2a);
  else if(j2a > 0) s2a = substr(s2a,0,j2a);
  if(i2b > 0) s2b = substr(s2b,0,i2b);
  else if(j2b > 0) s2b = substr(s2b,0,j2b);

  if(!bIsRedundant)
  {
    s1b = s2b = \"\";
  }
  if(!bOPCRedu)
  {
    s2a = s2b = \"\";
  }

  Host11.text      = host1;
  Host12.text      = host2;
  Host21.text      = host1;
  Host22.text      = host2;

  status12.enabled = bIsRedundant;
  status22.enabled = bIsRedundant;

  // IM 106046 mPokorny 06.02.2012
  tfServerState.text = s1a;
  tfReduServerState.text = s2a;
  tfServerState_redu.text = s1b;
  tfReduServerState_redu.text = s2b;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

bool clientDpExists(int nr)
{
  if(dpExists(\"_Driver\" + nr))
  {
    string driverType;
    dpGet(\"_Driver\" + nr + \".DT\", driverType);
    if(driverType == \"OPCUAC\")
      return true;
  }
  dyn_string ds;
  dyn_float df;
  ChildPanelOnCentralModalReturn(\"vision/MessageInfo\",
                                 getCatStr(\"para\", \"information\"),
                                 makeDynString(getCatStr(\"opcua\", \"opcuaDrvNumNotRun\"),
                                               getCatStr(\"para\", \"yes\"),
                                               getCatStr(\"para\", \"no\")),
                                 df, ds);

  if ( dynlen(df) < 1 || df[1] == 0 )
  {
    return false;
  }
  return true;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

bool opcuaDpExists(int nr)
{
  if(dpExists(\"_OPCUA\" + nr))
  {
    return true;
  }

  string text = getCatStr(\"opcua\", \"opcuaDpNotExist\");

  strreplace(text, \"$1\", \"_OPCUA\" + nr);

  ChildPanelOnCentralModal(\"vision/MessageWarning\",
                           getCatStr(\"para\", \"warning\"),
                           makeDynString(text));

  return false;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

bool checkConnectionDataSaved()
{
  if ( cmbConnection.text != \"\" && !dpExists(\"_\" + cmbConnection.text) )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
                             \"\",
                             makeDynString(\"$1:\" + getCatStr(\"opcua\", \"applyBeforEditSubscr\")));

    return 0;
  }
  else
    return 1;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void addOpcUaConnGlobals()
{
  if ( !globalExists(\"gOpcUaConn_mData\") )
    addGlobal(\"gOpcUaConn_mData\", MAPPING_VAR);
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void removeOpcUaConnGlobals()
{
  if ( globalExists(\"gOpcUaConn_mData\") )
    removeGlobal(\"gOpcUaConn_mData\");
}


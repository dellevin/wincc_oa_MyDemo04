"main()
{
  string sConnectionName = cmbConnection.text;

  dyn_float  df;
  dyn_string ds;

  if ( sConnectionName == \"\" )
    return;
  else
  {
    dyn_string dsSubs;
    dpGet(\"_\" + sConnectionName + \".Config.Subscriptions\", dsSubs);

    if ( dynlen(dsSubs) )
    {
      ChildPanelOnCentralModalReturn(\"para/opcuaRemoveDp.pnl\",
                                     getCatStr(\"general\", \"info\"),
                                     makeDynString(\"$1:\" + getCatStr(\"opcua\", \"applyDeleteConn\"),
                                                   \"$2:\" + getCatStr(\"general\", \"yes\"),
                                                   \"$3:\" + getCatStr(\"general\", \"no\"),
                                                   \"$4:\" + dynStringToString(dsSubs)),
                                     df, ds);
    }
    else
    {
      ChildPanelOnCentralModalReturn(\"vision/MessageInfo\",
                                     getCatStr(\"general\", \"info\"),
                                     makeDynString(getCatStr(\"opcua\", \"applyDeleteConn\"),
                                                   getCatStr(\"general\", \"yes\"),
                                                   getCatStr(\"general\", \"no\")),
                                     df, ds);
    }

    if ( dynlen(df) < 1 || df[1] < 1 )
      return;
    else
    {
      if ( dynlen(dsSubs) && df[1] == 2 && ds[1] == \"true\" )
      {
        for ( int i = dynlen(dsSubs); i >= 1; i-- )
        {
          if ( isRedundant() && dpExists(dsSubs[i] + \"_2\") )
            dpDelete(dsSubs[i] + \"_2\");

          if ( dpExists(dsSubs[i]) )
            dpDelete(dsSubs[i]);
        }
      }

      opcuaDriver_removeConnection(sConnectionName, gOpcUaConn_mData);

      delay(0,500);

      opcuaDriver_panelToMapping(gOpcUaConn_mData);
      opcuaDriver_saveDataFromMapping(gOpcUaConn_mData, \"\", cmbConnection.text);
    }
  }
}


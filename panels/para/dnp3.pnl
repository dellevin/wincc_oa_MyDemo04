  int i, j, pos;
  dyn_bool db;
  db = makeDynBool(false, false);
  getMultiValue(\"cbClearIIN\", \"state\", 0, db[1],
                \"cbClearIIN\", \"state\", 1, db[2]);
  j = 0;
  for (i = 1; i <= dynlen(db); i++)
  {
    if (db[i])
    {
      j++;
      pos = i -1;
    }
  }
  if (j == 1)
  {
    // deactivate
    setValue(\"cbClearIIN\", \"itemEnabled\", pos, false);
  }
  else
  {
    // activate all
    setMultiValue(\"cbClearIIN\", \"itemEnabled\", 0, true,
                  \"cbClearIIN\", \"itemEnabled\", 1, true);
  }

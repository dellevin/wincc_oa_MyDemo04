dnp3CreateNewStation(string dp)
{
  dyn_string ds;
  ds = strsplit(dp, \":\");
  string sSystem = ds[1]+\":\";
  dp = ds[2];
  int iSystem = getSystemId(sSystem);
  int iAnswer;
  dyn_errClass dErr;
   
  isRemoteSystemRedundant(iAnswer,sSystem);
  
  dpCreate(dp, \"_Dnp3Station\", iSystem);
  dErr = getLastError();
  if ( dynlen(dErr)>0)
  {
     throwError(dErr);
     errorDialog(dErr); 
  }
  
  if ( iAnswer == 1 )
  {
    dpCreate(dp + \"_2\", \"_Dnp3Station\", iSystem);
    dErr = getLastError();
    if ( dynlen(dErr)>0)
    {
       throwError(dErr);
       errorDialog(dErr); 
    }
  }
}

readDpWritePanel(string sPlc)
{
  langString ls;
  dyn_string ds;
  string config;
  bool active;
  unsigned usResp, gqclasses, usClearIIN;
  
  ls = dpGetComment(sPlc+\".\");
  t1.text  = ls;
  ds = ls;
  gLangList = ds;
  
  // read data from DP
  dpGet(sPlc + \".Configuration\", config,
        sPlc + \".Command.UnsolicitedResponse\", usResp,
        sPlc + \".Command.ClearIIN\", usClearIIN,
        sPlc + \".Command.Active\", active,
        sPlc + \".Command.GQ\", gqclasses);
  
  if (gqclasses == 0)
    gqclasses = 15;
  if (usClearIIN == 0)
    usClearIIN = 3;
  
  // parse the configuration string
  ds = strsplit(config, ':');
  if (dynlen(ds) == 0)
    dynAppend(ds,\"\"); // index 1 must not fail
    
  // there might be a valid configuration 
  if (ds[1] == \"IP\" && dynlen(ds) == 4)
  {
    // it is an IP configuration
    setMultiValue(\"rbMode\", \"number\", 0,
                  \"tfHost\", \"text\", ds[2],
                  \"sbPort\", \"text\", ds[3],
                  \"sbLinkAddress\", \"text\", ds[4]);
    setParamMode (0);
  }
  else if (ds[1] == \"SI\" && dynlen(ds) == 3)
  {
    // it is a serial configuration
    setMultiValue(\"rbMode\", \"number\", 1,
                  \"tfHost\", \"text\", ds[2],
                  \"sbLinkAddress\", \"text\", ds[3]);
    setParamMode (1);
  }
  else
  {
    // default 
    setMultiValue(\"rbMode\", \"number\", 0,
                  \"tfHost\", \"text\", \"\",
                  \"sbPort\", \"text\", 0,
                  \"sbLinkAddress\", \"text\", 0);
    setParamMode (0);
  }
    
  setMultiValue(\"cbActive\", \"state\", 0, active,
                \"cbUnSolicited\", \"state\", 0, usResp & 1,
                \"cbUnSolicited\", \"state\", 1, usResp & 2,
                \"cbUnSolicited\", \"state\", 2, usResp & 4,
                \"cbUnSolicited\", \"state\", 3, usResp & 256,
                \"cbClasses\", \"state\", 0, gqclasses & 1,
                \"cbClasses\", \"state\", 1, gqclasses & 2,
                \"cbClasses\", \"state\", 2, gqclasses & 4,
                \"cbClasses\", \"state\", 3, gqclasses & 8,
                \"cbClearIIN\", \"state\", 0, usClearIIN & 1,
                \"cbClearIIN\", \"state\", 1, usClearIIN & 2);
  
}

void setParamMode (int mode)
{
  if (mode == 0)
  {
    // IP
    setMultiValue(\"sbPort\", \"visible\", true,
                  \"ptxtPort\", \"visible\", true,
                  \"ptxtHost\", \"visible\", true,
                  \"ptxtDevice\", \"visible\", false);
  }
  else
  {
    // serial
    setMultiValue(\"sbPort\", \"visible\", false,
                  \"ptxtPort\", \"visible\", false,
                  \"ptxtHost\", \"visible\", false,
                  \"ptxtDevice\", \"visible\", true);
  }
}

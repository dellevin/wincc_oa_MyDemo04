"main()
{
  int        i;
  string     sPlc = \"\", sDp;
  dyn_string dsPlc = dpNames(getSystemName()+\"*\",\"_Dnp3Station\"), ds;
  
  string systemName = getSystemName();
  addGlobals();
  
  for ( i = dynlen(dsPlc); i > 0; i-- )
  {
    // don't display redundant datapoints
    if ( i > 0 && isReduDp(dsPlc[i]))
    {
      dynRemove(dsPlc, i);
      continue;
    }
    
    dsPlc[i] = dpSubStr(dsPlc[i],DPSUB_DP);
    if ( dsPlc[i][0] == \"_\" )
      dsPlc[i] = substr(dsPlc[i], 1, strlen(dsPlc[i])-1);
  }
  
  if ( dynlen(dsPlc) > 0 )
    sPlc = dsPlc[1];
  
  cmbPlc.items = dsPlc;
  cmbPlc.text = sPlc;
  if (sPlc != \"\")
  {
    readDpWritePanel(systemName + \"_\" +sPlc);
    connectState(systemName + \"_\" + sPlc);
  }
  if (isRedundant())
  {
    setReduStateVisible(true);
  }
  else
  {
    setReduStateVisible(false);
  }
      // dpConnect and disconnect must be in the same script
  // in the loop below nothing happens if the name of the sPlc
  // does not change
  while(1)
  {
    delay(0,20);
    sPlc = cmbPlc.text;
    if (sPlc != \"\")
      connectState(systemName + \"_\" + sPlc);
  }
}

void connectState (string sPlc)
{
  // to avoid error message when cloasing the panle check if 
  // variable exists
  if (!globalExists(\"gLastPlc\"))
    return;
  if (sPlc == gLastPlc )
    return;

  if (gLastPlc != \"\")
  {
    dpDisconnect(\"workIIN\", gLastPlc + \".State.IIN\");
    dpDisconnect(\"workConnState\", gLastPlc + \".State.ConnState\");
      
    if (isRedundant())
    {
      dpDisconnect(\"workIIN_2\", gLastPlc + \"_2.State.IIN\");
      dpDisconnect(\"workConnState_2\", gLastPlc + \"_2.State.ConnState\");    
    }
  }

  dpConnect(\"workIIN\", true, sPlc + \".State.IIN\");
  dpConnect(\"workConnState\", true, sPlc + \".State.ConnState\");
  if (isRedundant())
  {
    dpConnect(\"workIIN_2\", true, sPlc + \"_2.State.IIN\");
    dpConnect(\"workConnState_2\", true, sPlc + \"_2.State.ConnState\");    
  }
  gLastPlc = sPlc;
}



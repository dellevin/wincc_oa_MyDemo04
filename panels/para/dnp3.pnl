bool setDp()
{
  langString ls;
  string sPlc = cmbPlc.text;
  string config;
  bool usResp1, usResp2, usResp3, usResp4, active;
  string dp, host;
  unsigned port, usResp, la;
  int mode;
  
  if (sPlc == \"\" )
    return FALSE;

  dp = getSystemName() + \"_\"+sPlc;
  ls = gLangList;
  dpSetComment( dp+\".\", ls); 
  
  getMultiValue(\"cbActive\", \"state\", 0, active,
                \"cbUnSolicited\", \"state\", 0, usResp1,
                \"cbUnSolicited\", \"state\", 1, usResp2,
                \"cbUnSolicited\", \"state\", 2, usResp3,
                \"cbUnSolicited\", \"state\", 3, usResp4,
                \"rbMode\", \"number\", mode,
                \"tfHost\", \"text\", host,
                \"sbPort\", \"text\", port,
                \"sbLinkAddress\", \"text\", la);
  if (mode == 0)
  {
    config = \"IP:\" + host + \":\" + port + \":\" + la;
  }
  else if (mode == 1)
  {
    config = \"SI:\" + host + \":\" + la;
  }
  if (usResp1)
    usResp += 1;
  if (usResp2)
    usResp += 2;
  if (usResp3)
    usResp += 4;
  if (usResp4)
    usResp += 256;
  
  dpSet(dp + \".Configuration\", config,
        dp + \".Command.UnsolicitedResponse\", usResp,
        dp + \".Command.Active\", active);
  
  return TRUE;
}


void workIIN(string dp, unsigned iin)
{
  setValue(\"tfIin1\", \"text\", formatIIN(iin, 1));
  setValue(\"tfIin2\", \"text\", formatIIN(iin, 0));
}

void workConnState_2(string dp, int connState)
{
  tfState_2.text = getCatStr(\"dnp3\", \"ConnState\"+connState);
}

void workIIN_2(string dp, unsigned iin)
{
  setValue(\"tfIin1_2\", \"text\", formatIIN(iin, 1));
  setValue(\"tfIin2_2\", \"text\", formatIIN(iin, 0));
}

string formatIIN(unsigned iin, int byte)
{
  int i;
  bit32 b32;
  string str;
  
  while (byte > 0)
  {
    iin = iin / 256;
    byte--;
  }
  b32 = iin;
  str = b32;
  return substr(str, 24, 8);
}
void workConnState(string dp, int connState)
{
  tfState.text = getCatStr(\"dnp3\", \"ConnState\"+connState);
}


void addGlobals()
{
  addGlobal(\"gLangList\", DYN_STRING_VAR);
  addGlobal(\"gLastPlc\", STRING_VAR);
}

void removeGlobals()
{
  removeGlobal(\"gLangList\", DYN_STRING_VAR);
  removeGlobal(\"gLastPlc\", STRING_VAR);
}

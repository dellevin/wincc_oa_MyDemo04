"main()
{
  string keyFile;
  string pemFile;
  string csrfile;

  if ( CmbCertType.selectedPos == 1 )
  {
    keyFile = HostCertPath.text + myPathChar + txtCertName.text+\".key\";
    pemFile = HostCertPath.text + myPathChar + txtCertName.text+\".crt\";
  }
  else if ( CmbCertType.selectedPos == 2 )
  {
    keyFile = HostCertPath.text + myPathChar + \"host-key.pem\";
    pemFile = HostCertPath.text + myPathChar + \"host-cert.pem\";
  }
  else if ( CmbCertType.selectedPos == 3 )
  {
    keyFile = HostCertPath.text + myPathChar + \"privkey.pem\"; //TFS 36721 - Changed Name of the HTTP privKey-file
    pemFile = HostCertPath.text + myPathChar + \"certificate.pem\"; //TFS 36721 - Changed Name of the HTTP certificates
  }

  csrfile = pemFile;
  string replaceFileEnding;
  replaceFileEnding = substr(csrfile, strpos(csrfile, \".\"), 4);

  string srlFile = rootCA.text;
  string replaceFileEndingSRL;
  replaceFileEndingSRL = substr(srlFile, strpos(srlFile, \".\"), 4);

  if ( (access(rootCA.text, F_OK) != 0) ||
       (access(rootKeyFile.text, F_OK) != 0) )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\", \"couldNotCreateOPCUACert\",
                             makeDynString(getCatStr(\"SSLCerts\", \"createRootCert\")));

    return;
  }

  if ( (access(keyFile, F_OK) == 0) ||
       (access(pemFile, F_OK) == 0) )
  {
    dyn_float df;
    dyn_string ds;
    ChildPanelOnCentralModalReturn(\"vision/MessageInfo\", \"alreadyExists\",
                                   makeDynString(getCatStr(\"SSLCerts\", \"certAlreadyExists\"),
                                                 getCatStr(\"general\", \"yes\"), getCatStr(\"general\", \"no\")), df, ds);

    if ( (dynlen(df) == 0) || !df[1] )
    {
      return;
    }

    remove(keyFile);
    remove(pemFile);
    remove(csrfile);
  }

  setValue(\"grShapes\", \"enabled\", false);
  this.enabled = false;
  progress.visible = true;

  string param = createSSLCommand();

  string setOpenSSLPath = getPath(BIN_REL_PATH, \"openssl.cnf\");
  setOpenSSLPath = \"set OPENSSL_CONF=\" + setOpenSSLPath;
  string cmd = setOpenSSLPath + \"&\" + \"openssl genrsa -out \\\"\" + keyFile+\"\\\" 2048 \";
  string sOut, sErr;
  int rc;
  rc = system(cmd, sOut, sErr);
  if (rc == 0)
  {
    string subjectAltName;
    if (dynlen(cmbBxDNS.items) > 0) //should only be done when given
    {
      subjectAltName += \"authorityKeyIdentifier=keyid,issuer\\n\";
      subjectAltName += \"basicConstraints=CA:FALSE\\n\";
      subjectAltName += \"keyUsage = digitalSignature, nonRepudiation, keyEncipherment, dataEncipherment\\n\";
      subjectAltName += \"subjectAltName = @alt_names\\n\";
      subjectAltName += \"[alt_names]\\n\";
      dyn_string items = cmbBxDNS.items;
      string dnsEntry;
      for(int i = 1; i <= dynlen(items); i++)
      {
        dnsEntry = \"DNS.\" + i + \" = \" + items[i] + \"\\n\"; //for instance DNS.1 = localhost
        subjectAltName += dnsEntry;
      }
    }

    file fileSubjectAltName;
    fileSubjectAltName = fopen(HostCertPath.text + \"domains.ext\", \"w+\");
    rc = fputs(subjectAltName, fileSubjectAltName);
    (rc >= 0) ? (rc = 0) : (rc = -1); //fputs could return 0 or positive number when successfully executed
    fclose(fileSubjectAltName);
  }
  if (rc == 0)
  {
    cmd = setOpenSSLPath + \"&\" + \"openssl req -new -key \\\"\" + keyFile + \"\\\" -out \\\"\" + csrfile + \"\\\" \" + param;
    rc = system(cmd, sOut, sErr);
  }
  if (rc == 0)
  {
    cmd = setOpenSSLPath + \"&\" + \"openssl x509 -req -days \" + spDaysValid.text + \" -in \\\"\" + csrfile + \"\\\" -CA \\\"\" + rootCA.text +
                   \"\\\" -CAkey \\\"\" + rootKeyFile.text + \"\\\" -CAcreateserial -extfile \" + HostCertPath.text + \"domains.ext \" +
                   \"-passin pass:\" + rootKeyPwd.text + \" -out \\\"\" + pemFile+\"\\\"\";
    rc = system(cmd, sOut, sErr);
    remove(HostCertPath.text + \"domains.ext\");
  }

  if ( (rc != 0) && (sErr != \"\") )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\", getCatStr(\"SSLCerts\", \"warnCertCreate\"),
                             makeDynString(getCatStr(\"SSLCerts\", \"warnCertCreateLog\")));

    // throw output in logfile
    strreplace(sErr, \"\\n\", \"; \");
    throwError(makeError(\"SSLCerts\", PRIO_WARNING, ERR_CONTROL, 1, sErr));
  }
  else if ( (access(keyFile, F_OK) != 0) ||
            (access(pemFile, F_OK) != 0) )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\", \"couldNotCreateOPCUACert\",
                             makeDynString(getCatStr(\"SSLCerts\", \"couldNotCreateCert\")));

  }
  else
  {
    ChildPanelOnCentralModal(\"vision/MessageInfo1\", \"OPCUACertCreated\",
                             makeDynString(getCatStr(\"SSLCerts\", \"certCreated\")));
  }

  setValue(\"grShapes\", \"enabled\", true);
  this.enabled = true;
  progress.visible = false;
}


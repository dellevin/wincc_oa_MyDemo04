"main()
{
  dyn_string params;
  string cert, key;
  string path = makeUnixPath(HostCertPath.text);

  if(path[strlen(path) - 1] != '/')
  {
    path += '/';
  }

  switch(CmbCertType.selectedPos)
  {
    case 1: //free certificate
      cert = path + txtCertName.text + \".crt\";
      key = path + txtCertName.text + \".key\";
      break;
    case 2: //mxproxy
      cert = path + \"host-cert.pem\";
      key = path + \"host-key.pem\";
      break;
    case 3: //webserver
      cert = path + \"certificate.pem\";
      key = path + \"privkey.pem\";
      break;
  }

  if (isfile(rootCA.text) && isfile(cert) && isfile(key))
    params = makeDynString(rootCA.text, rootKeyFile.text, cert, key, txtCountry.text,
                           txtProvince.text, txtLocality.text, txtOrganization.text, txtDepartment.text);

  PanelOffReturn(makeDynFloat(0), params);
  ModuleOff(myModuleName());
}

//--------------------------------------------------------------------

string createSSLCommand()
{
   string param = \"-subj /C=\\\"\"+ txtCountry.text + \"\\\"/ST=\\\"\" + txtProvince.text + \"\\\"/L=\\\"\" + txtLocality.text +
                   \"\\\"/O=\\\"\" + txtOrganization.text + \"\\\"/OU=\\\"\" + txtDepartment.text + \"\\\"/CN=\\\"\" + txtMail.text + \"\\\"\";

   if(txtRoleOccupant.text() != \"\")
   {
     param = param + \"/roleOccupant=\\\"\" + txtRoleOccupant.text + \"\\\"\";
   }

   return param;
}

//--------------------------------------------------------------------

void changeCertType()
{
  if (CmbCertType.selectedPos() != 1)
  {
    switch( CmbCertType.selectedPos() )
    {
      case 2: //Proxy
        {
          txtCertName.enabled(false);
          txtCertKey.enabled(false);
          txtCertName.text(\"host-cert.pem & host-key.pem\");
          dyn_string texte;
          texte[1] = \"Produkt:\";
          texte[2] = \"Product:\";
          dyn_langString dstring = texte;
        }
        break;
      case 3: //HTTP
        {
          txtCertName.enabled(false);
          txtCertKey.enabled(false);
          txtCertName.text(\"certificate.pem & privkey.pem\");
          dyn_string texte;
          texte[1] = \"IP-Adresse:\";
          texte[2] = \"IP-Address:\";
          dyn_langString dstring = texte;
        }
        break;
      }

  }
  else
  {
    txtCertName.enabled(true);
    txtCertKey.enabled(true);
    txtCertName.text(\"\");
    txtCertKey.text(\"\");
    dyn_string texte;
    texte[1] = \"Produkt:\";
    texte[2] = \"Product:\";
    dyn_langString dstring = texte;
  }
}

//--------------------------------------------------------------------

string getTempDir()
{
  string tmpDir;
  if (_WIN32)
  {
    tmpDir = getenv(\"TEMP\");
    if (tmpDir == \"\")
      tmpDir = \"C:\";
    tmpDir = tmpDir + myPathChar;
  }
  else
  {
    tmpDir = \"/tmp/\";
  }

  string help = tmpDir + \"cert\";
  if ( isdir(help) || mkdir(help) )
    tmpDir = help + myPathChar;

  return tmpDir;
}

//--------------------------------------------------------------------

string getRootCertFilename()
{
  string path = getPath(CONFIG_REL_PATH);
  if (!isEvConnOpen())  // in PA
    path = getTempDir();

  if ( _WIN32 )
    strreplace(path, \"/\", \"\\\\\");
  string fileName;

  fileSelector(fileName, path, false);

  if ( _WIN32 )
    strreplace(fileName, \"/\", \"\\\\\");

  return fileName;
}

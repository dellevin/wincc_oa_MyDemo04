//--------------------------------------------------------------------------------
/**
 * @brief The function which checks if selected backend is redundant
**/
bool redundantBackend()
{
   string backendName = NgaBackendConfigProvider::selectedBackendName;
   if ( backendName.endsWith(\"_2\") || backendName.endsWith(\"_R\") )
     return TRUE;

  return FALSE;
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which does a shapes buffer values cleanup
**/
public void cleanup()
{
  oldValue.clear();
  oldAddress.clear();
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which updates states for the overview panel
 * @param states the given states to be taken
**/
public void updateStates(const mapping states)
{
  allStates = states;
  if ( !backendSelected.isEmpty() )
    updateOverview(backendSelected, FALSE);
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which does an overview update for the given backend name
**/
public synchronized void updateOverview(const string backendName, bool refreshSelection = TRUE)
{
  // update selected backend
  if ( refreshSelection )
  {
    backendSelected = backendName;

    // update main backend
    if ( !isRedundant() )
    {
      mainBackendSelected = backendSelected;
    }
    else
    {
      mainBackendSelected = backendSelected;
      if ( mainBackendSelected.endsWith(\"_R\") ||  mainBackendSelected.endsWith(\"_R_2\") )
        mainBackendSelected.remove(mainBackendSelected.lastIndexOf(\"_R\"), mainBackendSelected.length());
      else if ( mainBackendSelected.endsWith(\"_2\") )
        mainBackendSelected.remove(mainBackendSelected.lastIndexOf(\"_2\"), mainBackendSelected.length());
    }
  }

  shape updateNotReady = getShapeStrict(NgaBackendConfigProvider::overviewPanel, \"updateNotReady\");
  if ( allStates.isEmpty() || !mappingHasKey(allStates, mainBackendSelected) )
  {
    setValue(updateNotReady, \"visible\", TRUE);
    return;
  }
  else
  {
    setValue(updateNotReady, \"visible\", FALSE);
  }

  defineDiagram();  // with regard to inProc/out-ofProc

  bool frontendConnected = allStates[mainBackendSelected][\"frontend\"];
  if ( isRedundant() )
  {
    dyn_string reduPrefixes = makeDynString(\"_2\", \"_R\", \"_R_2\");
    for (int i = 1; i <= dynlen(reduPrefixes); ++i)
    {
      if ( frontendConnected && mappingHasKey(allStates, mainBackendSelected + reduPrefixes[i]) )
        frontendConnected = allStates[mainBackendSelected + reduPrefixes[i]][\"frontend\"];
    }

    startThread(\"frontendConnection\", frontendConnected);  // separate thread to reduce time
  }

  for (int i = 1; i <= dynlen(OVERVIEW_SHAPES); ++i)
  {
    // skip shapes related to redundant projects
    if ( !isRedundant() &&
         (OVERVIEW_SHAPES[i].contains(\"backend_2_\") || OVERVIEW_SHAPES[i].contains(\"backend_R_\")) )
    {
      continue;
    }

    startThread(\"redrawStates\", OVERVIEW_SHAPES[i], refreshSelection);  // separate thread to reduce time
  }
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which displays and hide the overview panel
 * @param show when TRUE the overview panel will be shown, FALSE will be hidden
**/
public synchronized void showOverview(bool show = TRUE)
{
  overviewModule.visible(show);
  if ( !show )
    return;

  int animGroup = createAnimation(\"parallel\");
  mapping options;
  options[\"duration\"] = 200;

  int width, height;
  int posX, posY;
  getValue(\"StatusFrame\", \"position\", posX, posY);
  getValue(\"StatusFrame\", \"size\", width, height);

  // Set an animation for shapes
  animate(animGroup, \"overviewModule\", \"sizeAsDyn\", makeDynInt(0, 0), makeDynInt(posX + width, posY + height), options);

  // Start animation and wait until finished
  startAnimationWait(animGroup);
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which defines the overview diagram view
**/
void defineDiagram()
{
  if ( !mappingHasKey(allStates, mainBackendSelected) )
    return;

  bool visible, inProc = allStates[mainBackendSelected][\"isInProc\"];
  string affectedShapeName = \"frontend_backend_con\";
  shape affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + affectedShapeName);
  getValue(affectedShape, \"visible\", visible);

  // update frontend icon
  setValue(getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".frontend1_ref.icon\"),
           \"fill\",
           \"[pattern,[fit,any,NGA/frontend_\" + (allStates[mainBackendSelected][\"frontend\"] ? \"active\" : \"notActive\") + \".svg]]\");

  // update frontend icon when redundant
  if ( isRedundant() && mappingHasKey(allStates, mainBackendSelected + \"_2\") )
    setValue(getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".frontend2_ref.icon\"),
             \"fill\",
             \"[pattern,[fit,any,NGA/frontend_\" + (allStates[mainBackendSelected + \"_2\"][\"frontend\"] ? \"active\" : \"notActive\") + \".svg]]\");

  if ( inProc != visible )  // no changes needed
    return;

  setValue(affectedShape, \"visible\", !inProc);
  if ( isRedundant() )
  {
    dyn_string reduPrefixes = makeDynString(\"_2\", \"_R\", \"_R_2\");
    for (int i = 1; i <= dynlen(reduPrefixes); ++i)
    {
      affectedShapeName = \"frontend_backend_con\";
      affectedShapeName.replace(\"backend_con\", \"backend\" + reduPrefixes[i] + \"_con\");
      affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + affectedShapeName);
      if ( hideMissingShape(affectedShape, mainBackendSelected + reduPrefixes[i]) )
        continue;

      inProc = allStates[mainBackendSelected + reduPrefixes[i]][\"isInProc\"];
      setValue(affectedShape, \"visible\", !inProc);
    }
  }

  startThread(\"moveDiagram\", inProc);  // separate thread to reduce time
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which updates the frontend connection state
**/
void frontendConnection(bool frontendConnected)
{
  string shapeFrontend = \"reduCon\";
  for (int i = 1; i <= 5; ++i)
  {
    shape theShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeFrontend + i);
    setValue(theShape, \"color\", frontendConnected ? \"STD_valve_open\" : \"\");
  }
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which shift diagram depending if it is in-Proc and out-of-Proc
**/
void moveDiagram(bool inProc)
{
  string affectedShapeName = \"frontend_backend_con\";
  shape affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + affectedShapeName);
  int w, h;
  getValue(affectedShape, \"size\", w, h);

  dyn_string all_shapes = OVERVIEW_SHAPES;
  dynAppend(all_shapes, makeDynString(\"database_ref\", \"database_2_ref\"));
  for (int i = 1; i <= dynlen(all_shapes); ++i)
  {
    // skip shapes related to redundant projects
    if ( !isRedundant() &&
         (all_shapes[i].contains(\"backend_2_\") || all_shapes[i].contains(\"backend_R_\") || all_shapes[i].contains(\"database_2_\") ) )
    {
      continue;
    }

    moveShape(all_shapes[i], inProc, h);
  }
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which shift individual shape to the given step
**/
void moveShape(const string shapeName, const bool inProc, const int step)
{
  shape affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeName);
  int x, y;
  getValue(affectedShape, \"position\", x, y);
  if ( inProc )
    y -= step;
  else
    y += step;

  setValue(affectedShape, \"position\", x, y);
  return;
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which redraws the given shape in the overview panel
**/
void redrawStates(const string &shapeName, bool refreshSelection = TRUE)
{
  shape affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeName);
  string backendPrefix = shapeName;
  string prefix = \"backend\";
  backendPrefix.remove(backendPrefix.indexOf(prefix), prefix.length());
  if ( shapeName.startsWith(prefix) && shapeName.endsWith(\"_ref\") )  // backend reference
  {
    backendPrefix.remove(backendPrefix.lastIndexOf(\"_ref\"), backendPrefix.length());

    string selectedIcon, activeIcon;
    if ( mainBackendSelected + backendPrefix == backendSelected )
      selectedIcon = \"_selected\";

    affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeName);
    if ( hideMissingShape(affectedShape, mainBackendSelected + backendPrefix, FALSE, refreshSelection) )
      return;

    if ( allStates[mainBackendSelected + backendPrefix][\"active\"] )
      activeIcon = \"_active\";
    else
      activeIcon = \"_notActive\";

    // update backend icon
    affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeName + \".icon\");
    setValue(affectedShape, \"fill\", \"[pattern,[fit,any,NGA/backend\" + activeIcon + selectedIcon + \".svg]]\");

    // update backend name
    affectedShape = getShapeStrict(NgaBackendConfigProvider::overviewPanel, NgaBackendConfigProvider::OVERVIEW_REF + \".\" + shapeName + \".name\");
    if ( backendPrefix.isEmpty() )
      setValue(affectedShape, \"text\", mainBackendSelected);
    else
      setValue(affectedShape, \"text\", \"   ...\" + backendPrefix);

    return;
  }
  else if ( shapeName.endsWith(\"_con\") )  // connection state
  {
    backendPrefix.remove(backendPrefix.lastIndexOf(\"_con\"), backendPrefix.length());

    bool state;
    if ( shapeName.startsWith(\"database_\") )
    {
      prefix = \"database_\";
      backendPrefix.remove(backendPrefix.indexOf(prefix), prefix.length());  // remove database_
      if ( hideMissingShape(affectedShape, mainBackendSelected + backendPrefix, FALSE, refreshSelection) )
        return;

      state = allStates[mainBackendSelected + backendPrefix][\"connectedToDatabase\"];
    }
    else if ( shapeName.startsWith(\"frontend_\") )
    {
      prefix = \"frontend_\";
      backendPrefix.remove(backendPrefix.indexOf(prefix), prefix.length());  // remove frontend_
      if(  hideMissingShape(affectedShape, mainBackendSelected + backendPrefix, TRUE, refreshSelection) )
        return;

      state = allStates[mainBackendSelected + backendPrefix][\"connectedToFrontend\"];
    }

    setValue(affectedShape, \"color\", state ? \"STD_valve_open\" : \"\");
    return;
  }
  else if ( shapeName.endsWith(\"_val\") )  // buffer state
  {
    backendPrefix.remove(backendPrefix.lastIndexOf(\"_val\"), backendPrefix.length());

    prefix = \"database_\";
    backendPrefix.remove(backendPrefix.indexOf(prefix), prefix.length());  // remove database_

    if ( hideMissingShape(affectedShape, mainBackendSelected + backendPrefix, FALSE, refreshSelection) )
      return;

    setValue(affectedShape, \"text\", allStates[mainBackendSelected + backendPrefix][\"buffer\"]);
    return;
  }
}

//--------------------------------------------------------------------------------
/**
 * @brief The function which hides non-existing shapes (related to backend with database handles redu)
**/
int hideMissingShape(const shape &affectedShape, const string &backendName, bool inProcRelated = FALSE)
{
  if ( !mappingHasKey(allStates, backendName) )
  {
    setValue(affectedShape, \"visible\", FALSE);
    return -1;
  }

  if ( inProcRelated )
    setValue(affectedShape, \"visible\", !allStates[backendName][\"isInProc\"]);
  else
    setValue(affectedShape, \"visible\", TRUE);

  return 0;
}

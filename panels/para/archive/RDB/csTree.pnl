//--------------------------------------------------------------------------------------
int getLevel(string id)
{
  int level=0;
  id = tvCompr.parent(id);
  while (tvCompr.itemExists(id))
  {
    level++;
    id = tvCompr.parent(id);
  }
  return level;
}
//--------------------------------------------------------------------------------------
// stores path in reverse order
dyn_string rememberPath(string id)
{
  dyn_string path;
  if (id != \"\")
  {
    int wayup = 1;
    do {
      path[wayup]=id;
      wayup++;
      id = tvCompr.parent(id);
    } while (tvCompr.itemExists(id));
  }  
  if (isDbgFlag(\"CS\"))
    DebugN(\"rememberPath\",path);
  return path;
}

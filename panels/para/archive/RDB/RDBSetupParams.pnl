"main()
{
  string sql = \"SELECT DISTINCT GROUP_NAME, REFERENCE FROM ARC_STATEMENT\";//\"SELECT GROUP_NAME FROM ARC_GROUP\";
  
  dyn_float resultFloat;
  dyn_string resultText;

  if (!schema_connected)
  {
	  ChildPanelOnCentralModalReturn(\"RDBPassword.pnl\", \"Passwort-Eingabe\", 
	       makeDynString(\"$Sys:0\", \"$Schema:1\", \"$App:0\", \"$Ado:1\"),
	       resultFloat, resultText);
	  if (resultFloat[1]==-1)
	    return;
	  string dsn = txt_service_name.text;
	  string pwd = resultText[2];
	  string user = txt_schema_user.text;
    schema_connected = connect2DB(dsn, user, pwd);
  }
  
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"Connected\", schema_connected);
  
  if (schema_connected)
  {
	  dyn_dyn_anytype table;
	  bool ok, isAlert;
	  int err = rdbSelect(conn, sql, table);
	  if (isDbgFlag(DBGFLAG_PARAMS))
      DebugTN(\"Table\",table);
	  dyn_string groups_DP, groups1, groups2, types1, types2;
	  groups1 = getDynString(table, 1);
	  types1 = getDynString(table, 2);
	  
	  groups_DP = dpNames(\"*\", \"_RDBArchiveGroups\");
	  for (int i=1;i<=dynlen(groups_DP);i++)
	  {
	    dpGet(groups_DP[i]+\".groupName\", groups2[i]);
	    dpGet(groups_DP[i]+\".isAlert\", isAlert);
	    types2[i]=isAlert?\"A\":\"E\";
	  }

	  ok = (groups1==groups2) && (types1==types2);
	  if (isDbgFlag(DBGFLAG_PARAMS))
      DebugTN(\"Groups and Types\", groups1, groups2, types1, types2, ok);
	  
	  if (ok)
	    anzeigeIntegrity.backCol=\"gruen\";
	  else
	    anzeigeIntegrity.backCol=\"rot\";
	}
}

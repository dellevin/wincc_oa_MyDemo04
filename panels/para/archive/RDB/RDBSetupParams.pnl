"main()
{
  string sql = \"SELECT SEQUENCE_OWNER, MIN_VALUE, MAX_VALUE \"
               \" FROM ALL_SEQUENCES WHERE SEQUENCE_NAME = 'SEQ_ARC_ARCHIVE' \"
               \" ORDER BY MIN_VALUE\";
  
  dyn_float resultFloat;
  dyn_string resultText;

  if (!sysconnected)
  {
	  ChildPanelOnCentralModalReturn(\"RDBPassword.pnl\", \"Passwort-Eingabe\", 
	       makeDynString(\"$Sys:1\", \"$Schema:0\", \"$App:0\", \"$Ado:1\"),
	       resultFloat, resultText);
	  if (resultFloat[1]==-1)
	    return;
	  string dsn = txt_service_name.text;
	  string syspwd = resultText[1];
	  string sysuser = txt_sysdba_user.text;
    sysconnected = connect2DB(dsn, sysuser, syspwd);
  }
  
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"Connected\", sysconnected);
  
  if (sysconnected)
  {
	  dyn_dyn_anytype table;
	  int err = rdbSelect(conn, sql, table);
    if (isDbgFlag(DBGFLAG_PARAMS))
      DebugTN(\"Table\",table,table[1],table[2]);
	  string flog=winPath(getPath(LOG_REL_PATH))+\"sequences.log\";
	  if (isDbgFlag(DBGFLAG_PARAMS))
      DebugTN(flog);
	  file f = fopen(flog, \"w\");
	  fprintf(f, \"%30s%10s%10s\\n\", \"Sequence Owner\", \"Minimum\", \"Maximum\");
	  for (int i=1;i<=dynlen(table);i++)
	  {
	    for (int j=1;j<=3;j++)
	    {
	      if (j==1)
	        fprintf(f, \"%30s\",table[i][j]);
	      else
	        fprintf(f, \"%10d\",table[i][j]);
	      //DebugN(i,j,table[i][j]);
	    }
	    fprintf(f, \"\\n\");
	  }
	  fclose(f);
    fileEditor(flog);
	}
}

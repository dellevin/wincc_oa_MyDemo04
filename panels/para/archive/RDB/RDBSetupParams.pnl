const string DBGFLAG_PARAMS = \"PARAMS\";

bool sysconnected;
bool schema_connected;
bool appconnected;
dbConnection conn;
mapping mSetupParams;
string pathRDBInstall;
bool login_strange_init;
bool try_odbc;

//--------------------------------------------------------------------------------------
  
bool connect2DB(string dsn, string user, string pwd, string host=\"\")
//returns true when ok
{
  int err;
  string mydsn;
  bool connected;

  if (_WIN32 )
  {
    //  OLE-DB - funktioniert lokal erst nach einmaliger Durchführung von ODBC-Connect!! - wieso??
    if (try_odbc)
    {
      mydsn = \"DSN=\"+dsn+\";UID=\"+user+\";PWD=\"+pwd; //ODBC
	  }
	  else
	  {
	    if (!login_strange_init)
	    {
		    mydsn = \"DSN='\"+dsn+\"';UID='\"+user+\"';PWD='\"+pwd+\"'\"; //ODBC
		    err = rdbOpen(conn, mydsn);
		    if (isDbgFlag(DBGFLAG_PARAMS))
          DebugTN(\"ODBC Init Open (\"+mydsn+\"): \"+err);
		    err = rdbClose(conn);
        if (isDbgFlag(DBGFLAG_PARAMS))
          DebugTN(\"ODBC Init Close: \"+err);
		    login_strange_init = true;
		  }
	    mydsn = \"Provider=MSDAORA;Data Source='\"+dsn+\"';User ID='\"+user+\"';Password='\"+pwd+\"'\"; 
	    //mydsn = \"Provider=MSDAORA;Data Source=\"+dsn+\";User ID=\"+user+\";Password=\"+pwd; 
	  }
  }
  else	// Linux, Solaris, ...
  {
    mydsn = \"driver=QOCI8;server=\"+host+\";database=\"+dsn+\";uid=\"+user+\";pwd=\"+pwd+\";\";
  }

  err = rdbOpen(conn, mydsn);
//	DebugN(\"Login (\"+mydsn+\"): \"+err);
  
  connected = !err;
//  if (connected)
//    anzeigeRDB.backCol=\"gruen\";
//  else
//    anzeigeRDB.backCol=\"rot\";
  return connected;
}

//--------------------------------------------------------------------------------------
  
bool checkConnection(string dsn, string user, string pwd, string host=\"\")
//returns true when ok
{
  int err;
  dyn_dyn_anytype table;
  bool b, opened4check;
    
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"checkConnection\", dsn, user, host);

  //if (!sysconnected && !schema_connected && !appconnected)
    if (!connect2DB(dsn, user, pwd, host))
      return false;
    else
      opened4check = true;
  
  err = rdbSelect(conn, \"select username, user_id, default_tablespace, created from user_users\", table);
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"checkConnection\", err, table);
  b = (dynlen(table)>0 && !err);
  if (opened4check)
    closeConnection();
  return b;
}

//--------------------------------------------------------------------------------------
  
bool closeConnection()
{
  int err=rdbClose(conn);
  DebugN(err);
  return (!err);
}

//--------------------------------------------------------------------------------------
  
int runSetup(string syspwd, string schemapwd, string apppwd)
{
  writeRDBConfig();

  string fsetup=pathRDBInstall+\"RDB_setup.sql\";
  string fconf=+\"config\";
  string sqlcommand = \"sqlplus /nolog @\"+winPath(fsetup)+\" \"+winPath(getPath(DATA_REL_PATH, \"RDBSetup/ora/\"))+\" \"+winPath(getPath(LOG_REL_PATH))+\" config \"+syspwd+\" \"+schemapwd+\" \"+apppwd;
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(sqlcommand);

  int err;
  string batfile = getPath(DATA_REL_PATH) + \"tempbat.bat\";
  file f = fopen(batfile, \"w\");
  fprintf(f, \"cd %s\\n%s\\n%s\", winPath(pathRDBInstall), sqlcommand, \"\");//\"pause\");
  fclose(f);
  if ( _UNIX )
    err = system(\"chmod 777 \" + batfile);
  err = system(\"start \"+batfile);
  return err;
}

//--------------------------------------------------------------------------------------
  
bool checkSetupLog()
{
  string line;
  int pos;
  string flog=winPath(getPath(LOG_REL_PATH))+\"RDB_setup.log\";
  file f = fopen(flog, \"r\");
  while (feof(f)==0)
  {
    fgets(line, 400, f);
    if (isDbgFlag(DBGFLAG_PARAMS))
      DebugTN(line);
    pos = strpos(line, \"FAILED\");
    if (pos >= 0)
      return false;
  }
  return true;
}    

//--------------------------------------------------------------------------------------
  
void loadSetupLog()
{
  string flog=winPath(getPath(LOG_REL_PATH, \"RDB_setup.log\"));
  fileEditor(flog);
}    

//--------------------------------------------------------------------------------------
  
int writeRDBConfig()
{
  string dir=getPath(DATA_REL_PATH, \"RDBSetup/ora\", \"\", 1);
  if (dir==\"\")
  {
	   dir = getPath(DATA_REL_PATH)+ \"RDBSetup/ora\";
	   mkdir(dir);
     if (isDbgFlag(DBGFLAG_PARAMS))
       DebugTN(\"Creating RDB directory: \"+dir);
  }
  string fconf=dir+ \"/RDB_config.sql\";
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"Writing to RDB config: \"+fconf);

  file f=fopen(fconf, \"w\");
  int state;
  
  getValue(\"chk_yesno_newuser\", \"state\", 0, state);
  string str_yesno_newuser = state ? \"yes\" : \"no\";
  getValue(\"chk_occi_upgrade\", \"state\", 0, state);
  string str_occi_upgrade = state ? \"yes\" : \"no\";
  getValue(\"chk_zip_backup\", \"state\", 0, state);
  string str_zip_backup = state ? \"yes\" : \"no\";
  getValue(\"chk_use_xpisso\", \"state\", 0, state);
  string str_use_xpisso = state ? \"yes\" : \"no\";
  getValue(\"chk_use_java\", \"state\", 0, state);
  string str_use_java = state ? \"yes\" : \"no\";

  fputs(\"define connect_identifier = '\"+txt_connect_identifier.text  +\"'\\n\", f);
  fputs(\"define sysdba_user        = '\"+txt_sysdba_user.text         +\"'\\n\", f);
  fputs(\"define yesno_newuser      = '\"+str_yesno_newuser            +\"'\\n\", f);
  fputs(\"define schema_user        = '\"+txt_schema_user.text         +\"'\\n\", f);
  fputs(\"define app_user           = '\"+txt_app_user.text            +\"'\\n\", f);
  fputs(\"define use_rman           = '\"+strtolower(cbo_use_rman.text)+\"'\\n\", f);
  fputs(\"define os_sys             = '\"+strtolower(cbo_os_sys.text)  +\"'\\n\", f);
  fputs(\"define zip_backup         = '\"+str_zip_backup               +\"'\\n\", f);
  fputs(\"define sequence_start     =  \"+txt_sequence_start.text      +\" \\n\", f);
  fputs(\"define sequence_maxvalue  =  \"+txt_sequence_maxvalue.text   +\" \\n\", f);
  fputs(\"define path_dbfile        = '\"+txt_path_dbfile.text         +\"'\\n\", f);
  fputs(\"define path_tempdbfile    = '\"+txt_path_tempdbfile.text     +\"'\\n\", f);
  fputs(\"define path_oraclebin     = '\"+txt_path_oraclebin.text      +\"'\\n\", f);
  fputs(\"define instance_name      = '\"+txt_instance_name.text       +\"'\\n\", f);
  fputs(\"define host_name          = '\"+txt_host_name.text           +\"'\\n\", f);
  fputs(\"define path_backup        = '\"+txt_path_backup.text         +\"'\\n\", f);
  fputs(\"define path_alert         = '\"+txt_path_alert.text          +\"'\\n\", f);
  fputs(\"define path_event         = '\"+txt_path_event.text          +\"'\\n\", f);
  fputs(\"define mytimezone         = '\"+cbo_mytimezone.text          +\"'\\n\", f);
  fputs(\"define asm_instance       = '\"+txt_asm_instance.text        +\"'\\n\", f);
  fputs(\"define service_name       = '\"+txt_service_name.text        +\"'\\n\", f);
  fputs(\"define occi_upgrade       = '\"+str_occi_upgrade             +\"'\\n\", f);
  fputs(\"define use_xpisso         = '\"+str_use_xpisso               +\"'\\n\", f);
  fputs(\"define use_java           = '\"+str_use_java                 +\"'\\n\", f);

  fclose(f);
}

//--------------------------------------------------------------------------------------
  
int readRDBConfig()
{
  string fconf=getPath(DATA_REL_PATH, \"RDBSetup/ora/RDB_config.sql\");
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"Reading from RDB config: \"+fconf);
  file f=fopen(fconf, \"r\");
  
  if (!f)
  {
    DebugN(\"Could not open RDB_config.sql\");
    return -1;
  }  

  dyn_string params;
  string line, parName, parValue;
  int cnt, pos;
  
  while (feof(f)==0)
  {
    fgets(line, 400, f);
    //DebugN(line);
    cnt = strreplace (line, \"define \", \"\");
    if (cnt == 0) //if no \"define\"
      continue;
    pos = strpos(line, \"--\");
    if (pos >= 0)
    {
      line = substr(line, 0, pos);
    }
    //DebugN(line);
    params = strsplit(line, \"=\");
    //DebugN(params);
    if (dynlen(params)==2)
    {
      parName = strrtrim(strltrim(params[1]));
      parValue = strrtrim(strltrim(params[2]));
      strreplace(parValue,\"'\",\"\"); //Hochkomma nicht anzeigen
      //DebugN(\"Parameter \"+parName+\": \"+parValue);
    }
    if (strpos(parName, \"password\") == -1) //do not save pwds as value is not found in config anyway
      mSetupParams[parName]=parValue;
  }
  fclose(f); // Datei schließen
  if (isDbgFlag(DBGFLAG_PARAMS))
    DebugTN(\"List of Parameters\", mSetupParams);
}

//--------------------------------------------------------------------------------------
  
void fillRDBSetupPanel()
{
  bool state;
  
  state = (mSetupParams[\"yesno_newuser\"]==\"yes\");// ? 1 : 0;
  setValue(\"chk_yesno_newuser\", \"state\", 0, state);
  state = (mSetupParams[\"occi_upgrade\"]==\"yes\");// ? 1 : 0;
  setValue(\"chk_occi_upgrade\", \"state\", 0, state);
  state = (mSetupParams[\"zip_backup\"]==\"yes\");// ? 1 : 0;
  setValue(\"chk_zip_backup\", \"state\", 0, state);
  state = (mSetupParams[\"use_xpisso\"]==\"yes\");// ? 1 : 0;
  setValue(\"chk_use_xpisso\", \"state\", 0, state);
  state = (mSetupParams[\"use_java\"]==\"yes\");// ? 1 : 0;
  setValue(\"chk_use_java\", \"state\", 0, state);

  state = (mSetupParams[\"sysdba_user\"]==\"\");// ? 1 : 0;
  setValue(\"chk_osauthsetup\", \"state\", 0, state);
  txt_sysdba_user.enabled = !state;
  
	txt_connect_identifier.text = mSetupParams[\"connect_identifier\"];
	txt_sysdba_user.text        = mSetupParams[\"sysdba_user\"];
	txt_schema_user.text        = mSetupParams[\"schema_user\"];
	txt_app_user.text           = mSetupParams[\"app_user\"];
	cbo_use_rman.text           = strtoupper(mSetupParams[\"use_rman\"]);
	cbo_os_sys.text             = strtoupper(mSetupParams[\"os_sys\"]);
	txt_sequence_start.text     = mSetupParams[\"sequence_start\"];
	txt_sequence_maxvalue.text  = mSetupParams[\"sequence_maxvalue\"];
	txt_path_dbfile.text        = mSetupParams[\"path_dbfile\"];
	txt_path_tempdbfile.text    = mSetupParams[\"path_tempdbfile\"];
	txt_path_oraclebin.text     = mSetupParams[\"path_oraclebin\"];
	txt_instance_name.text      = mSetupParams[\"instance_name\"];
	txt_host_name.text          = mSetupParams[\"host_name\"];
	txt_path_backup.text        = mSetupParams[\"path_backup\"];
	txt_path_alert.text         = mSetupParams[\"path_alert\"];
	txt_path_event.text         = mSetupParams[\"path_event\"];
	cbo_mytimezone.text         = mSetupParams[\"mytimezone\"];
	txt_asm_instance.text       = mSetupParams[\"asm_instance\"];
	txt_service_name.text       = mSetupParams[\"service_name\"];
}

//--------------------------------------------------------------------------------------
  
string winPath(string path)
{
  bool db_is_win = (strtolower(cbo_os_sys.text) == \"win\");
  //DebugN(\"Windows Path vorher: \"+path);
  if (db_is_win)   
  {
    strreplace(path, \"/\", \"\\\\\"); //double backslash for interpreter
    if ( _UNIX) //double backslash to have Unix read it as one backslash
      strreplace(path, \"\\\\\", \"\\\\\\\\\");
  }
  //DebugN(\"Path nachher: \"+path);
  return path;
}

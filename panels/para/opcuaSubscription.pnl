"main()
{
  if ( !checkSubscriptionDataSaved() )
    return;

  opcuaDriver_checkUnsavedDps(cmbConnection.text, $SERVER_DP);
  setSubscriptionType();

  int i;
  dyn_float df;
  dyn_string ds;

  ChildPanelOnCentralModalReturn(\"para/opcuaNewDp.pnl\",
                                 getCatStr(\"da\", \"createdp\"),
                                 makeDynString(getCatStr(\"para\", \"apc_inputdpname\"), \"\"), df, ds);

  if ( dynlen(ds) != 1 || dynlen(df) != 1 )
    return;

  if ( dpExists(\"_\" + ds[1]) )
  {
    string msg;
    sprintf(msg, getCatStr(\"opcua\",\"subscrAllreadyExists\"), \"_\" + ds[1]);

    ChildPanelOnCentralModalReturn(\"vision/MessageWarning\",
          getCatStr(\"para\",\"warning\"),
          makeDynString(msg),df,ds);
    return;
  }

  opcuaDriver_panelToMapping(gOpcUaSubs_mData); //save Data of old Subscription

  opcuaDriver_createSubscription(ds[1], gOpcUaSubs_mData, isRedundant() && false, false);  //create a new Subscription
//  if (isRedundant())
//    opcuaDriver_createSubscription(ds[1]+\"_2\", gOpcUaSubs_mData, false, true);  //create a new Subscription

  opcuaDriver_displaySubscription(gOpcUaSubs_mData[\"Subscription\"][ds[1]], ds[1]); //display new Subscription
//  if (isRedundant())
//    opcuaDriver_displaySubscription(gOpcUaSubs_mData[\"Subscription\"][ds[1]+\"_2\"], ds[1]+\"_2\", true); //display new Subscription

  sLastConn = ds[1];

  // check name
  cmbConnection.text=ds[1];

  // clear status fields
  setMultiValue(\"tfSubscriptionId\",            \"text\", \"\",
                \"tfRevisedPublishingInterval\", \"text\", \"\",
                \"tfAssignedOPCUAServer\",       \"text\", \"\",
                \"tfRevisedLifetimeCount\",      \"text\", \"\",
                \"tfRevisedMaxKeepAliveCount\",  \"text\", \"\",

                \"tfSubscriptionId_redu\",            \"text\", \"\",
                \"tfRevisedPublishingInterval_redu\", \"text\", \"\",
                \"tfAssignedOPCUAServer_redu\",       \"text\", \"\",
                \"tfRevisedLifetimeCount_redu\",      \"text\", \"\",
                \"tfRevisedMaxKeepAliveCount_redu\",  \"text\", \"\",

                \"cmbSubscriptionType\", \"selectedPos\", df[1]);

  if (df[1] == 3)
  {
    // initialize
    txtNodeId.text = dsNodeIds[1];

// IM 103819    tblAlarmRanges.deleteAllLines();
// IM 103819    tblAlarmRanges.appendLines(dynlen(alarmStates), \"status\", alarmStates, \"alarmRange\", alarmRanges);

// IM 101449
    for (i=1; i<=dynlen(dsNodeIds); i++)
    {
      if (txtNodeId.text == dsNodeIds[i]) cbAlarmTypes.text = dsAlarmTypes[i];
    }
  }

  setSubscriptionType();
  enableConnection(cmbConnection.text != \"\");
}











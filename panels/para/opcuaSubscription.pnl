E "#uses \"opcuaDriver_plugin.ctl\"
//mapping mDriverData;
langString lsDescription;
string sLastConn;
dyn_string opcuaDriver_dsConnDpes;
dyn_string opcuaDriver_dsConnDpesOld;

// Both following dyns must have the same length ever!
const dyn_string dsAlarmTypes = makeDynString(\"OffNormalAlarm\", \"WinCCOAContinuousAlarm\", \"WinCCOADiscreteAlarm\", \"ExclusiveLimitAlarm\", \"ExclusiveDeviationAlarm\", \"Manual ...\" );
dyn_string dsNodeIds = makeDynString(\"ns=0;i=10637\", \"ns=2;i=1003\", \"ns=2;i=1002\", \"ns=0;i=9341\", \"ns=0;i=9764\", \"\");

////////////////////////////////////////////////////////////////////////////////////////////////////

void sendCommand(string sCommand)
{
  string sSubscription = cmbConnection.text;
  anytype aValue;

  if (sSubscription == \"\")
    return;

  switch (sCommand)
  {
    case \"GQ\":
      aValue = (int)0;
      break;
    default:
      DebugN(\"OPCUA Subscription Command: case not implemented!\",sCommand);
      return;
      break;
  }
  if ( dpExists(getSystemName() + OPCUADRIVER_sSubscriptionDpPrefix + sSubscription + \".Command.\"+sCommand) )
  {
    dpSet(getSystemName() + OPCUADRIVER_sSubscriptionDpPrefix + sSubscription + \".Command.\"+sCommand, aValue);
  }
  else
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\", \"\", makeDynString(\"$1:\" + getCatStr(\"opcua\", \"applyBeforEditSubscr2\")));
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void loadSubscription(string sSubscription, bool bOnInit = 1)
{
  // tfs 55028 - call opcuaDriver_panelToMapping() on initalize only
  if ( bOnInit )
    opcuaDriver_panelToMapping(gOpcUaSubs_mData);

  opcuaDriver_displaySubscription(gOpcUaSubs_mData[\"Subscription\"][sSubscription], sSubscription);

  if (gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"hasRedu\"])
    opcuaDriver_displaySubscription(gOpcUaSubs_mData[\"Subscription\"][sSubscription+\"_2\"], sSubscription, true);

  sLastConn = sSubscription;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void setSubscriptionType()
{
  opcuaDriver_setSubscriptionModified(gOpcUaSubs_mData, cmbConnection.text);

  setMultiValue(\"grpDataVisible\", \"visible\", cmbSubscriptionType.selectedPos == 1,
                \"grpDataInvisible\", \"visible\", cmbSubscriptionType.selectedPos == 3,
                \"cbDiscardOldest\", \"visible\", cmbSubscriptionType.selectedPos == 1,
                //\"ptInfo\", \"visible\", cmbSubscriptionType.selectedPos == 2);
                \"ptEventTypeNodeId\", \"visible\", cmbSubscriptionType.selectedPos == 2,
                \"tfEventTypeNodeId\", \"visible\", cmbSubscriptionType.selectedPos == 2);
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void setAlarmType()
{
  int i;
  string sSubscription = cmbConnection.text, alarmType;

  int index = dynContains(dsAlarmTypes, cbAlarmTypes.text);
  if (index < 1) return;
  // provide default values to mapping according to selected alarm type

  // IM 101449
  if (index > 0 && index <= dynlen(dsNodeIds))
  {
    gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmType\"] = dsNodeIds[index];
    setValue(\"txtNodeId\", \"text\", gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmType\"]);
  }

  if (cbAlarmTypes.text == dsAlarmTypes[dynlen(dsAlarmTypes)])
  {
      gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmType\"] = dsNodeIds[dynlen(dsNodeIds)];
  }

  opcuaDriver_setSubscriptionModified(gOpcUaSubs_mData, sSubscription);

  // workaround:
  // mappingToPanel() needs a parameter that specifies the initial subscription to show. This
  // parameter is currently missing and always the first subscriptions is shown - what
  // is probably the wrong one. Therefore UI adaptations are done here.

  // IM 101449
  for (i=1; i<=dynlen(dsNodeIds); i++)
  {
    if (txtNodeId.text == dsNodeIds[i]) cbAlarmTypes.text = dsAlarmTypes[i];
  }
// IM 103819  tblAlarmRanges.deleteAllLines();
// IM 103819  tblAlarmRanges.appendLines(dynlen(gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmStates\"]), \"status\", gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmStates\"], \"alarmRange\", gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmRanges\"]);
  setSubscriptionType();
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void enableConnection(bool enable)
{
  setMultiValue(\"cbPublishingEnabled\", \"enabled\", enable,
                \"spPubInterval\",       \"enabled\", enable,
                \"spPriority\",          \"enabled\", enable,
                \"spLifetime\",          \"enabled\", enable,
                \"spKeepAlive\",         \"enabled\", enable,
                \"spNotifications\",     \"enabled\", enable,
                \"cbDiscardOldest\",     \"enabled\", enable,
                \"grpDataVisible\",      \"enabled\", enable);

  showTableAlarmRanges();
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void setAlarmTypeUi()
{
  bool quit = false;
  int i;

  // IM 101449
  for ( i = 1; i <= dynlen(dsNodeIds); i++ )
  {
    if ( txtNodeId.text == dsNodeIds[i] )
    {
      cbAlarmTypes.text = dsAlarmTypes[i];
      quit = true;
      break;
    }
  }

  if ( !quit )
  {
    cbAlarmTypes.text = dsAlarmTypes[dynlen(dsAlarmTypes)];
  }

  // IM 101449
  enableNodeId();

  // IM 103819 tblAlarmRanges.deleteAllLines();
  // IM 103819 tblAlarmRanges.appendLines(dynlen(gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmStates\"]),\"status\", gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmStates\"], \"alarmRange\", gOpcUaSubs_mData[\"Subscription\"][sSubscription][\"AlarmRanges\"]);
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void enableNodeId()
{
  // IM 101449
  // set NodeId fields active for \"Manual ...\" alert type
  if (cbAlarmTypes.text == dsAlarmTypes[dynlen(dsAlarmTypes)] && cmbSubscriptionType.selectedPos == 3)
  {
    txtNodeId.enabled = true;
    primtextNodeId.enabled = true;
  }
  else
  {
    txtNodeId.enabled = false;
    primtextNodeId.enabled = false;
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void setNodeIdNotEmpty()
{
  // IM 101449
  // if the NodeId is set empty in \"Manual ...\" alert type, the \"OffNormalAlarm\" will be set on next start

  int index = dynContains(dsNodeIds, txtNodeId.text);

  if (txtNodeId.text == \"\" && cmbSubscriptionType.selectedPos == 3)
  {
    cbAlarmTypes.text = dsAlarmTypes[1];
    gOpcUaSubs_mData[\"Subscription\"][cmbConnection.text][\"AlarmType\"] = dsNodeIds[1];
    txtNodeId.text = dsNodeIds[1];
    setAlarmTypeUi();
  }

  else if (index < 1 || index >= dynlen(dsNodeIds) && cmbSubscriptionType.selectedPos == 3)
//   else if (txtNodeId.text != dsNodeIds[1] && txtNodeId.text != dsNodeIds[2] &&
//            txtNodeId.text != dsNodeIds[3] && txtNodeId.text != dsNodeIds[4] &&
//            txtNodeId.text != dsNodeIds[5] && cmbSubscriptionType.selectedPos == 3)
  {
    cbAlarmTypes.text = dsAlarmTypes[dynlen(dsAlarmTypes)];
    dsNodeIds[dynlen(dsNodeIds)] = gOpcUaSubs_mData[\"Subscription\"][cmbConnection.text][\"AlarmType\"];
    txtNodeId.text = dsNodeIds[dynlen(dsNodeIds)];
    setAlarmTypeUi();
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void manualNodeIdToMapping()
{
  // IM 101449
  if (cbAlarmTypes.selectedPos == dynlen(dsAlarmTypes))
  {
    gOpcUaSubs_mData[\"Subscription\"][cmbConnection.text][\"AlarmType\"] = dsNodeIds[dynlen(dsNodeIds)];
    dsNodeIds[dynlen(dsAlarmTypes)] = dsNodeIds[dynlen(dsNodeIds)];
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void showTableAlarmRanges()
{
  if (cmbSubscriptionType.selectedPos == 3 && cbAlarmTypes.selectedPos == 1)
  {
    setMultiValue(\"tblAlarmRanges\",  \"visible\", false,
                  \"cmdInsertAfter\",  \"visible\", false,
                  \"cmdInsertBefore\", \"visible\", false,
                  \"cmdRemove\",       \"visible\", false);
                  //\"cbAlarmFlags\",    \"visible\", false); // IM 105734 - always visible
  }
  else if (cmbSubscriptionType.selectedPos == 3 && cbAlarmTypes.selectedPos != 1)
  {
    setMultiValue(\"tblAlarmRanges\",  \"visible\", true,
                  \"cmdInsertAfter\",  \"visible\", true,
                  \"cmdInsertBefore\", \"visible\", true,
                  \"cmdRemove\",       \"visible\", true);
                  //\"cbAlarmFlags\",    \"visible\", true); // IM 105734 - always visible
  }
}

////////////////////////////////////////////////////////////////////////////////////////////////////

bool checkSubscriptionDataSaved()
{
  if ( cmbConnection.text != \"\" && !dpExists(\"_\" + cmbConnection.text) )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
                             \"\",
                             makeDynString(\"$1:\" + getCatStr(\"opcua\", \"applyBeforEditSubscr2\")));
    return 0;
  }
  else
    return 1;
}

////////////////////////////////////////////////////////////////////////////////////////////////////

initSubscriptions()
{
  gOpcUaSubs_mData = opcuaDriver_loadDataIntoMapping($SERVER_DP);
  opcuaDriver_mappingToPanel(gOpcUaSubs_mData);

  if ( isDollarDefined(\"$SUBSCRIPTION\") && getDollarValue(\"$SUBSCRIPTION\") )
  {
    setMultiValue(\"cmbConnection\", \"enabled\", false,
                  \"cmdNewDp\", \"enabled\", false,
                  \"cmdRemoveDp\", \"enabled\", false,
                  \"cmbSubscriptionType\", \"enabled\", false);

    loadSubscription(getDollarValue(\"$SUBSCRIPTION\"));
  }

  setValue(\"reduStatus\", \"enabled\", isRedundant());
  //setValue(\"tfAssignedOPCUAServer_redu\", \"enabled\", FALSE);
  //setValue(\"tfRevisedPublishingInterval_redu\", \"enabled\", FALSE);
  //setValue(\"tfSubscriptionId_redu\", \"enabled\", FALSE);

  enableConnection(cmbConnection.text != \"\");

  setSubscriptionType();

  cbAlarmTypes.deleteAllItems();
  cbAlarmTypes.items = dsAlarmTypes;

  // update combobox setting according to alarm type

  // IM 101449
  setNodeIdNotEmpty();

  // IM 101449
  for ( int i = 1; i <= dynlen(dsNodeIds); i++ )
  {
    if (txtNodeId.text == dsNodeIds[i]) cbAlarmTypes.text = dsAlarmTypes[i];
  }

  showTableAlarmRanges();
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void addOpcUaSubsGlobals()
{
  if ( !globalExists(\"gOpcUaSubs_mData\") )
    addGlobal(\"gOpcUaSubs_mData\", MAPPING_VAR);
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void removeOpcUaSubsGlobals()
{
  if ( globalExists(\"gOpcUaSubs_mData\") )
    removeGlobal(\"gOpcUaSubs_mData\");
}



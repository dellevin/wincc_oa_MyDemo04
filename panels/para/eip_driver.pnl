"main()
{
  dyn_float  df;
  dyn_string ds;

//!!!TagSets - similar to the way poll-groups work

  string currPlc = cmbConnection.text;
  //DebugTN(\"TagSets btn clicked: currPlc=\", currPlc);
  string currTagSet = \"\";
  string dollar1 = \"\";
  string sys = \"\";
  if ( isDollarDefined(\"$currPlc\") )
  {
    dollar1 = $1;
    sys = dpSubStr( dollar1, DPSUB_SYS );
  }

  ChildPanelOnCentralModalReturn(\"para/eip_tagsets.pnl\",
    \"Tag sets\",
    makeDynString( \"$fromPara:\"+ currTagSet,
                   \"$SYSTEM:\"  + sys,
                   \"$currPlc:\" + currPlc,
                   \"$currTagSet:\" + currTagSet ),
    df, ds );

  //DebugTN(\"TagSets btn - back from modal, dynlen=\", dynlen(df), dynlen(ds));
}


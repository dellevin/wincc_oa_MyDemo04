E "#uses \"eipDriver_Plugin\"
mapping mDriverData;
langString lsDescription;
string sLastConn;
int iWarningConn, iWarningDrv, iWarningDp;

////////////////////////////////////////////////////////////////////////////////////////////////////

void sendCommand(string sCommand)
{
  string sConnection = cmbConnection.text;
  anytype aValue;

  if (sConnection == \"\")
    return;

  switch (sCommand)
  {
    case \"GQ\":
      aValue = (int)-1;
      break;
    case \"IQ\":
      sCommand = \"GQ\";
      aValue = (int)2;
      break;
    default:
      DebugN(\"EIP Command: case not implemented!\",sCommand);
      return;
      break;
  }

  dpSet(getSystemName() + \"_\" + sConnection + \".Command.\"+sCommand, aValue);
}

////////////////////////////////////////////////////////////////////////////////////////////////////

void msgWarning()
{
  if      ( iWarningConn == 1 && iWarningDrv == 0 )
  {
    eipDriver_PopupWarningMessage(1);
    iWarningConn = 0;
  }
  else if ( iWarningConn == 0 && iWarningDrv == 1 )
  {
    eipDriver_PopupWarningMessage(2);
    iWarningDrv = 0;
  }
  else if ( iWarningConn == 1 && iWarningDrv == 1 )
  {
    eipDriver_PopupWarningMessage(3);
    iWarningConn = 0;
    iWarningDrv = 0;
  }

  if ( iWarningDp == 1 )
  {
    eipDriver_PopupWarningMessage(4);
    iWarningDp = 0;
  }
  else if ( iWarningDp == 2 )
  {
    eipDriver_PopupWarningMessage(5);
    iWarningDp = 0;
  }
}


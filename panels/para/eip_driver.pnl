"main()
{
  eipDriver_checkUnsavedConnections(cmbConnection.text);

  dyn_float df;
  dyn_string ds;

  ChildPanelOnCentralModalReturn(\"vision/stringInput.pnl\", // define dp name
                                 getCatStr(\"da\", \"createdp\"),
                                 makeDynString(getCatStr(\"para\", \"apc_inputdpname\"), cmbConnection.text),
                                 df, ds);

  if ( EipDRIVER_DEBUGMESSAGES )
    DebugTN(\"EIP Driver panel Create, dynlen(df)=\", dynlen(df));

  if ( dynlen(df) >= 1)
    if ( EipDRIVER_DEBUGMESSAGES )
      DebugTN(\" ...df[1]=\", df[1]);

  if ( dynlen(df) < 1 || df[1] == 0 )
    return;

  if ( dynlen(ds) )
  {
    if ( dpExists(\"_\" + ds[1]) )
    {
      iWarningDp = 1;
      msgWarning();
      return;
    }
    else if ( cmbConnection.text == ds[1] )
    {
      iWarningDp = 2;
      msgWarning();
      return;
    }
  }

  eipDriver_panelToMapping(mDriverData); //save Data of old Connection

  eipDriver_createConnection(ds[1], mDriverData, isRedundant(), false);  //create a new Connection
  if ( isRedundant() )
    eipDriver_createConnection(ds[1]+\"_2\", mDriverData, false, true);  //create a new Connection

  eipDriver_displayConnection(mDriverData[\"Connection\"][ds[1]], ds[1]); //display new Connection
  if ( isRedundant() )
    eipDriver_displayConnection(mDriverData[\"Connection\"][ds[1]+\"_2\"], ds[1]+\"_2\", true); //display new Connection

  sLastConn = ds[1];

  // check name
  cmbConnection.text=ds[1];

  string sConnection = cmbConnection.text;
  int plcType =  cmbPlcType.selectedPos();
  btnTagSets.enabled = (sConnection != \"\" && dpExists(\"_\"+sConnection) && ((plcType <= 3) || (plcType == 7)));
}


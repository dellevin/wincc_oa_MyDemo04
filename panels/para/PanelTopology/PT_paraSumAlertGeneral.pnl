"main()
{
  int pos=$pos,j;
  dyn_string ds1,ds2,ds3;

  ds1=gPtNa; ds2=gPtOn; ds3=gPtOf;
  for (j=1;j<=dynlen(ds1);j++)
  {
    if ( strrtrim(strltrim(ds1[j]))==\"\" ||
         strrtrim(strltrim(ds2[j]))==\"\" ||
         strrtrim(strltrim(ds3[j]))==\"\"   )
    {
      ChildPanelOnCentralModal(\"vision/MessageWarning\",
        getCatStr(\"para\",\"warning\"),makeDynString(getCatStr(\"pt\",\"emptynames\")));
      return;
    }
  }

  if (pos<1) pos=dynlen(gPtNameSumAlertGeneral)+1;

  gPtNameSumAlertGeneral[pos]=gPtNa;
  gPtOnSumAlertGeneral[pos]=gPtOn;
  gPtOffSumAlertGeneral[pos]=gPtOf;
  gPtMinSumAlertGeneral[pos]=gPtMi;
  gPtMaxSumAlertGeneral[pos]=gPtMa;

  removeGlobal(\"gPtNa\");
  removeGlobal(\"gPtMi\");
  removeGlobal(\"gPtMa\");
  removeGlobal(\"gPtOn\");
  removeGlobal(\"gPtOf\");

  PanelOffReturn(makeDynFloat(1),makeDynString());
}" 0

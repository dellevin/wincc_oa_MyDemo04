"main()
{
  int         pos=$pos;
  string      sYes=getCatStr(\"para\",\"yes\"),  
              sNo=getCatStr(\"para\",\"no\");  
  dyn_float   df;  
  dyn_string  ds,sPara,dsN,names;
  
  if (pos<1 ||
      gPtNa!=gPtNameSumAlertGeneral[pos] ||
      gPtMi!=gPtMinSumAlertGeneral[pos] ||
      gPtMa!=gPtMaxSumAlertGeneral[pos] ||
      gPtOn!=gPtOnSumAlertGeneral[pos] ||
      gPtOf!=gPtOffSumAlertGeneral[pos] )
  {
    sPara=makeDynString(getCatStr(\"pt\",\"rangenotsaved\"),sYes,sNo);
    ChildPanelOnCentralModalReturn(\"vision/MessageInfo\",  
                                   getCatStr(\"para\",\"warning\"),  
                                   sPara,df,ds);  
    if (df[1]!=1)
    {
      return;
    }
  }

  removeGlobal(\"gPtNa\");
  removeGlobal(\"gPtMi\");
  removeGlobal(\"gPtMa\");
  removeGlobal(\"gPtOn\");
  removeGlobal(\"gPtOf\");

  PanelOffReturn(makeDynFloat(0),ds);
}" 0

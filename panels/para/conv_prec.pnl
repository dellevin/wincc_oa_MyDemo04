updatePreview()
{
  int     inv,z,n;
  anytype round;
  
  getMultiValue(\"inv\",\"number\",inv,
                \"zehner\",\"text\",z,
                \"nachkomma\",\"text\",n);
  
  // no division by zero
  if ( n == 0 ) n  = 1;
  if ( z == 0 ) z  = 1;
  
  if ( !inv )
    round = z;
  else
    round = 1 / (float)n;
     
  setValue(\"rundung\", \"text\", round);
}







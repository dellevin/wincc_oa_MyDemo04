"main()
{
  int        pos;
  string     s;
  dyn_string list, ds;

  getMultiValue(\"txtHost\",  \"text\",        s,
                \"lstHosts\", \"items\",       list,
                \"lstHosts\", \"selectedPos\", pos);

  if ( strrtrim(strltrim(s)) == \"\" )
    return;

  if ( pos < 1 )
    pos = dynlen(list);

  ds = strsplit(s, \":\");

  if ( strpos(s, \":\") < 1 )
    s += \":502\";
  else if ( dynlen(ds) < 2)
    s += \"502\";

  if ( dynContains(list, s) > 0 )
  {
    setValue(\"lstHosts\", \"selectedPos\", dynContains(list, s));
    setValue(\"txtHost\",  \"text\",        s);
    return;
  }

  //dynInsertAt(list,s,pos+1);

  dynAppend(list, s);
  setValue(\"txtHost\",  \"text\",  s);
  setValue(\"lstHosts\", \"items\", list);

  //setValue(\"lstHosts\", \"selectedPos\", pos+1);
  setValue(\"lstHosts\", \"selectedPos\", dynlen(list));
}

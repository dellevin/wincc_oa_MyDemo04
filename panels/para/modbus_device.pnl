"main()
{
  int        pos;
  string     s;
  dyn_string list, ds;

  getMultiValue(\"txtHost\",  \"text\",        s,
                \"lstHosts\", \"items\",       list,
                \"lstHosts\", \"selectedPos\", pos);

  if ( pos < 1 || s == \"\" )
    return;

  if ( dynContains(list, s) > 0 )
    return;

  ds = strsplit(s, \":\");

  if ( strpos(s, \":\") < 1 )
    s += \":502\";
  else if ( dynlen(ds) < 2)
    s += \"502\";

  if ( dynContains(list, s) > 0 )
  {
    setValue(\"lstHosts\", \"selectedPos\", dynContains(list, s));
    setValue(\"txtHost\",  \"text\",        s);
    return;
  }

  list[pos] = s;
  setValue(\"lstHosts\", \"items\",       list);
  setValue(\"lstHosts\", \"selectedPos\", pos);
}

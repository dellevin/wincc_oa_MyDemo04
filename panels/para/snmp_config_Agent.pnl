E "#uses \"panel\"
#uses \"snmpCrypt\"

bool isNewAgent;
string actualAgent;
dyn_string availableAgents;

// id = cmb selected item, key = internal key
mapping internalPrivProtocolId;

void openNewManagerPanel()
{
  string manNum;
  dyn_string ds;
  dyn_float df;

  ChildPanelOnCentralModalReturn(\"para/snmp_new_Agent.pnl\", paGetCatStr(\"snmp\",\"agentCreation\"),
    makeDynString(), df, ds);

  if(dynlen(df)!=0 && dynlen(ds) != 0 && df[1]!=0)
  {
    initPanel(ds[1]);
    cmbFlags.selectedPos(4);
  }

  txtAgentChanged.text = \"changed_\";
}

//-----------------------------------------------------------------------------
void initScriptVars()
{
  // the snmpManager uses internal other constant IDs for the encryption
  // thats the reason for this mapping
  //USM_v3.h :
  //#define SNMP_PRIVPROTOCOL_NONE     1 ///< None
  //#define SNMP_PRIVPROTOCOL_DES      2 ///< DES
  //#define SNMP_PRIVPROTOCOL_IDEA     9 ///< IDEA (non standard)
  //#define SNMP_PRIVPROTOCOL_AES128   4 ///< AES128 (draft standard)
  //#define SNMP_PRIVPROTOCOL_AES192  20 ///< AES192 (draft standard)
  //#define SNMP_PRIVPROTOCOL_AES256  21 ///< AES256 (draft standard)
  internalPrivProtocolId[1] = 2;
  internalPrivProtocolId[2] = 4;
  internalPrivProtocolId[3] = 20;
  internalPrivProtocolId[4] = 21;
}

//-----------------------------------------------------------------------------
void initPanel(const string &actualDp, bool wasCalledFromDelete = false)
{
  dynClear(availableAgents);
  int selectedIndex = 1;

  dyn_string allAgents = dpNames(\"_*_*\", \"_SNMPAgent\");
  dynAppend(allAgents, dpNames(\"_*_*\", \"_SNMPV3Entity\"));

  for(int i = 1; i <= dynlen(allAgents); ++i)
    if(!isReduDp(allAgents[i]))
      dynAppend(availableAgents, allAgents[i]);

  dyn_string itemsForComboBox = availableAgents;
  for(int i = 1; i <= dynlen(itemsForComboBox); ++i)
  {
    if(itemsForComboBox[i] == actualDp || dpSubStr(itemsForComboBox[i], DPSUB_DP) == actualDp)
      selectedIndex = i;

    //langString ls = dpGetDescription(itemsForComboBox[i] + \".\");
    //string name = ls;
    //itemsForComboBox[i] = name;
    itemsForComboBox[i] = dpSubStr(itemsForComboBox[i], DPSUB_DP);
  }

  setMultiValue(
    \"cmbAgent\",   \"items\", itemsForComboBox
  );

  if(actualDp == \"\" && !wasCalledFromDelete)
    cmbAgent.text = \"\";
  else
    cmbAgent.selectedPos = selectedIndex;

  if (actualDp == \"\" && wasCalledFromDelete && dynlen(availableAgents) > 0)
    txtAgentChanged.text = \"changed_\" + availableAgents[selectedIndex];
  else
    txtAgentChanged.text = \"changed_\" + actualDp;
}

//-----------------------------------------------------------------------------
void selectedAgentChanged(const string &newAgent)
{
  string nAgent = substr(newAgent, strlen(\"changed_\"));
  if(actualAgent != \"\" && nAgent != actualAgent && dpExists(actualAgent))
  {

    dpDisconnect(
      \"workCB\",
      actualAgent + \".Status.ActiveConn\",
      actualAgent + \".Status.ConnState\",
      actualAgent + \".Redu.Status.ConnState\",
      actualAgent + \".Redu.Access.IPAddress\"
    );

    txtStatusSum.text = \"\";
    txtTarget1State.text = \"\";
    txtTarget2State.text = \"\";
  }

  if(newAgent != \"changed_\")
  {
    actualAgent = nAgent;
    isNewAgent = false;

    dpConnect(
      \"workCB\", true,
      actualAgent + \".Status.ActiveConn\",
      actualAgent + \".Status.ConnState\",
      actualAgent + \".Redu.Status.ConnState\",
      actualAgent + \".Redu.Access.IPAddress\"
    );

    setPanelValues();
  }
}


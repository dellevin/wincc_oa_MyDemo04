//-----------------------------------------------------------------------------
bool save()
{
  string sDriverNr = sbDrvNum.text;
  if (!dpExists(\"_\" + sDriverNr + \"_SNMPManager\"))
  {
    dpCreate(\"_\" + sDriverNr + \"_SNMPManager\", \"_SNMPManager\");
  }

  string dp = \"_\" + sbDrvNum.text + (cmbVersion.selectedText == \"V3\" ? \"_SNMPV3Entity_\" : \"_SNMPAgent_\") + sbAgentId.text;

  if(isNewAgent)
  {
    if (!dpExists(dp))
    {
      dpCreate(dp, (cmbVersion.selectedText == \"V3\" ? \"_SNMPV3Entity\" : \"_SNMPAgent\"));
    }
    else
    {
      ChildPanelOnCentralModal(\"vision/MessageWarning\",
                               getCatStr(\"para\",\"warning\"),
                               makeDynString(getCatStr(\"snmp\",\"alreadyExistingAgent\")));
    }
  }

  int timeout;
  int retries;
  langString description;
  string ip = \"\";
  string ipRedu = \"\";
  int port = \"\";
  int portRedu = \"\";

  // v1/2
  string readCommunity = \"\";
  string readCommunityRedu = \"\";
  string writeCommunity = \"\";
  string writeCommunityRedu = \"\";
  int proto = 2;

  // v3
  string authPasswd;
  int authProtocol;
  string contextEngineId;
  string contextName;
  string privPasswd;
  int privProtocol;
  int securityLevel;
  string securityName;
  string authPasswdRedu;
  int authProtocolRedu;
  string contextEngineIdRedu;
  string contextNameRedu;
  string privPasswdRedu;
  int privProtocolRedu;
  int securityLevelRedu;
  string securityNameRedu;

  bool isReduAgentSelected = false;
  bool isFlagKeepAlive = true;
  bool isFlagReadWrite = true;
  bit32 flags;

  // get actual flags from DP
  dpGet(dp + \".Access.Flags\", flags);

  // get common values
  getMultiValue(\"sbTimeout\",             \"text\",     timeout,
                \"sbRetries\",             \"text\",     retries,
                \"chbReduAgentEntity\",    \"state\", 0, isReduAgentSelected,
                \"txtIp\",                 \"text\",     ip,
                \"txtIpRedu\",             \"text\",     ipRedu,
                \"txtPort\",               \"text\",     port,
                \"txtPortRedu\",           \"text\",     portRedu);

//   flags = isFlagKeepAlive << 1 | isFlagReadWrite;

  int selectedFlag = cmbFlags.selectedPos();
  // set flags for redu switch
  // selectedPos 1 means no flag
  if (selectedFlag == 1)
  {
    setBit(flags, 0, 0);
    setBit(flags, 1, 0);
  }
  // selectedPos 2 means switchOnTimeout
  if (selectedFlag == 2)
  {
    setBit(flags, 0, 0);
    setBit(flags, 1, 1);
  }
  // selectedPos 3 means switchOnReadWriteError
  else if (selectedFlag == 3)
  {
    setBit(flags, 0, 1);
    setBit(flags, 1, 0);
  }
  // selectedPos 4 means 2+3
  else if (selectedFlag == 4)
  {
    setBit(flags, 0, 1);
    setBit(flags, 1, 1);
  }

  if(cmbVersion.selectedText == \"V3\")
  {
    getMultiValue(\"txtSecurityName\",       \"text\",          securityName,
                  \"txtSecurityNameRedu\",   \"text\",          securityNameRedu,
                  \"cmbSecurityLevel\",      \"selectedPos\",   securityLevel,
                  \"cmbSecurityLevelRedu\",  \"selectedPos\",   securityLevelRedu,
                  \"cmbAuthProtocol\",       \"selectedPos\",   authProtocol,
                  \"cmbAuthProtocolRedu\",   \"selectedPos\",   authProtocolRedu,
                  \"cmbPrivProtocol\",       \"selectedPos\",   privProtocol,
                  \"cmbPrivProtocolRedu\",   \"selectedPos\",   privProtocolRedu,
                  \"txtContextEngineID\",    \"text\",          contextEngineId,
                  \"txtContextEngineIDRedu\",\"text\",          contextEngineIdRedu,
                  \"txtContextName\",        \"text\",          contextName,
                  \"txtContextNameRedu\",    \"text\",          contextNameRedu,
                  \"txtAuthPasswd\",         \"text\",          authPasswd,
                  \"txtAuthPasswdRedu\",     \"text\",          authPasswdRedu,
                  \"txtPrivPasswd\",         \"text\",          privPasswd,
                  \"txtPrivPasswdRedu\",     \"text\",          privPasswdRedu);

    if(securityLevel == 1) //NONE
    {
      authProtocol = 1;
      privProtocol = 1;
    }
    else
    {
      authProtocol++;  // because 1 ... none is not displayed
      privProtocol = internalPrivProtocolId[privProtocol];
    }

    if(securityLevelRedu == 1) //NONE
    {
      authProtocolRedu = 1;
      privProtocolRedu = 1;
    }
    else
    {
      authProtocolRedu++;  // because 1 ... none is not displayed
      privProtocolRedu = internalPrivProtocolId[privProtocolRedu];
    }


    dpSet(dp + \".Access.Timeout\",              timeout,
          dp + \".Access.Retries\",              retries,
          dp + \".Access.Flags\",                flags,
          dp + \".Access.IPAddress\",            ip,
          dp + \".Access.Port\",                 port,
          dp + \".Access.AuthPasswd\",           snmpcrypt_encryptAccessParameter(sbDrvNum.text, authPasswd),
          dp + \".Access.AuthProtocol\",         authProtocol,
          dp + \".Access.ContextEngineID\",      snmpcrypt_encryptAccessParameter(sbDrvNum.text, contextEngineId),
          dp + \".Access.ContextName\",          snmpcrypt_encryptAccessParameter(sbDrvNum.text, contextName),
          dp + \".Access.PrivPasswd\",           snmpcrypt_encryptAccessParameter(sbDrvNum.text, privPasswd),
          dp + \".Access.PrivProtocol\",         privProtocol,
          dp + \".Access.SecurityLevel\",        securityLevel,
          dp + \".Access.SecurityName\",         securityName,
          dp + \".Redu.Access.IPAddress\",       isReduAgentSelected ? ipRedu : \"\",
          dp + \".Redu.Access.Port\",            isReduAgentSelected ? portRedu : 0,
          dp + \".Redu.Access.AuthPasswd\",      isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, authPasswdRedu) : \"\",
          dp + \".Redu.Access.AuthProtocol\",    isReduAgentSelected ? authProtocolRedu : 1,
          dp + \".Redu.Access.ContextEngineID\", isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, contextEngineIdRedu) : \"\",
          dp + \".Redu.Access.ContextName\",     isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, contextNameRedu) : \"\",
          dp + \".Redu.Access.PrivPasswd\",      isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, privPasswdRedu) : \"\",
          dp + \".Redu.Access.PrivProtocol\",    isReduAgentSelected ? privProtocolRedu : 1,
          dp + \".Redu.Access.SecurityLevel\",   isReduAgentSelected ? securityLevelRedu : 1,
          dp + \".Redu.Access.SecurityName\",    isReduAgentSelected ? securityNameRedu : \"\");
  }
  else
  {
    getMultiValue(
      \"txtReadCommunity\",      \"text\",     readCommunity,
      \"txtReadCommunityRedu\",  \"text\",     readCommunityRedu,
      \"txtWriteCommunity\",     \"text\",     writeCommunity,
      \"txtWriteCommunityRedu\", \"text\",     writeCommunityRedu,
      \"cmbVersion\",            \"selectedPos\", proto
    );

    dpSet(
      dp + \".Access.Timeout\",             timeout,
      dp + \".Access.Retries\",             retries,
      dp + \".Access.Flags\",               flags,
      dp + \".Access.IPAddress\",           ip,
      dp + \".Access.Port\",                port,
      dp + \".Access.ReadCommunity\",       snmpcrypt_encryptAccessParameter(sbDrvNum.text, readCommunity),
      dp + \".Access.WriteCommunity\",      snmpcrypt_encryptAccessParameter(sbDrvNum.text, writeCommunity),
      dp + \".Redu.Access.IPAddress\",      isReduAgentSelected ? ipRedu : \"\",
      dp + \".Redu.Access.Port\",           isReduAgentSelected ? portRedu : 0,
      dp + \".Redu.Access.ReadCommunity\",  isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, readCommunityRedu) : \"\",
      dp + \".Redu.Access.WriteCommunity\", isReduAgentSelected ? snmpcrypt_encryptAccessParameter(sbDrvNum.text, writeCommunityRedu) : \"\",
      dp + \".Access.Protocol\",            proto
    );
  }

  langString ls = langliste1.items;
  dpSetDescription(dp+\".\", ls);

  initPanel(dp);

  return true;
}

//-----------------------------------------------------------------------------
void workCB(string dp1, anytype activeConn,
            string dp2, anytype connState,
            string dp3, anytype reduConnState,
            string dp4, anytype reduIp)
{
  string connText;

  txtTarget1State.text = (connState ? getCatStr(\"snmp\",\"connected\") : getCatStr(\"snmp\",\"notConnected\"));

  // set target 2 text if redu is configured
  if (reduIp != \"\")
  {
    txtTarget2State.text = (reduConnState ? getCatStr(\"snmp\",\"connected\") : getCatStr(\"snmp\",\"notConnected\"));
  }
  else
  {
    txtTarget2State.text = \"\";
  }

  if(activeConn == 0)
  {
    if(connState)
      connText = \"firstConnected\";
    else
      connText = \"firstDisconnected\";
  }
  else
  {
    if(reduConnState)
      connText = \"secondConnected\";
    else
      connText = \"secondDisconnected\";
  }
  if(!connState && !reduConnState)
    connText = \"noConnection\";

  txtStatusSum.text = getCatStr(\"snmp\",connText);
}

//-----------------------------------------------------------------------------
int getKeyFromValue(int value)
{
  dyn_int dsKeys = mappingKeys(internalPrivProtocolId);

  for(int i = 1; i <= mappinglen(internalPrivProtocolId); i++)
  {
    if (internalPrivProtocolId[i] == value)
      return i;
  }

  return 1;
}





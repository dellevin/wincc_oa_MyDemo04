  dyn_float  df;
  dyn_string ds;
  dyn_string dsError;
  
  for(int i = 1; i <= getNoOfLangs(); i++)
    dsError[i] = getCatStr(\"ChannelManager\", \"chOutOfOrder\", i - 1);
  
  ChildPanelOnCentralReturn(\"para/ChannelManager/Popup/Channel/CM_New_Channel.pnl\",

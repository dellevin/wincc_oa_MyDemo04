    if(!dpExists(sChannel))
    {
      dpCreate(sChannel, \"_ChannelManager_Channel\");
    }
    
    dyn_errClass error;
    
    error = getLastError();
    
    if(dynlen(error))
    {
      errorDialog(error);
      return;
    }
  }
}

  setMultiValue(\"servername\", \"items\", servers,
                \"oldname\",    \"text\",  \"\",
                \"vendorinfo\", \"text\",  \"\");

  sText = getCatStr(\"para\", \"removeopcserver\");
  strreplace(sText, \"%server\", server);

  ChildPanelOnCentralModal(\"para/mp_messageInfo.pnl\",
                           getCatStr(\"para\", \"information\"),
                           makeDynString(sText));
}


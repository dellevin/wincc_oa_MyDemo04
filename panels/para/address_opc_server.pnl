                \"currenttimebiasRedu\", \"text\", currenttimebias,
                \"serverstateRedu\",     \"text\", serverstate,
                \"serverstatetxtRedu\",  \"text\", serverstatetxt,
                \"vendorinfoRedu\",      \"text\", vendorinfo,
                \"majorversionRedu\",    \"text\", majorversion,
                \"minorversionRedu\",    \"text\", minorversion,
                \"buildnumberRedu\",     \"text\", buildnumber,

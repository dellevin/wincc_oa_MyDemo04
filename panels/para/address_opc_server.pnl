  if (reconnecttimer<0)
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
                             paGetCatStr(\"para\",\"warning\"),
                             makeDynString(paGetCatStr(\"para\",\"wrongnegativ\")+\"\\nReconnectTimer\"));
      return;
  }

  if (qualinfo==1)
  {
    if (quality==1)
    {
      von=1;bis=3;
    }
    if (quality==2)
    {
      von=1;bis=5;
    }
    if (quality==3)
    {
      von=1;bis=7;
    }
    if ( (quality==1 && (userbit<1 || userbit>3)) ||
         (quality==2 && (userbit<1 || userbit>5)) ||

dyn_string getFileNamesRec(string dir = \".\", string pattern = \"*\")
{
  dyn_string files;
  dyn_string directories;

  if(_WIN32)
  {
    dynAppend(files, getFileNames(dir, pattern));

    dyn_string ds;
    getRecursiveDirectoryNames(ds, dir);
    for(int i = 1; i <= dynlen(ds); i++)
    {
      strreplace(ds[i], \"*\", \"\");
      strreplace(ds[i], \"\\\\\", \"/\");
      if(ds[i] == \"\")
        continue;

      dyn_string ds2 = getFileNames(dir + \"/\" + ds[i], pattern);
      for(int j = 1; j <= dynlen(ds2); j++)
        dynAppend(files, ds[i] + ds2[j]);
    }
  }
  else if(_UNIX)
  {
    system(\"find \" + dir + \" >  PVRcheckings.txt\");
    file f=fopen(\"PVRcheckings.txt\",\"r\");
    string content=\"\";
    string s=\"\";
    while (feof(f)==0)
    {
      fgets(s,100,f);
      strrtrim(s, \"\\n\");
      strreplace(s, \"\\n\", \"\");
      if(s == \"\" || isDirectory(s))
      {
        continue;
      }
      else if(patternMatch(pattern, s))
      {
        strreplace(s, dir, \"\");
        dynAppend(files, s);
      }
    }
    fclose(f);
  }

	return files;
}


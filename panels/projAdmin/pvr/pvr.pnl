int getDiskInformation(string& ProjectName, string& ProjectDiskName, string& ProjectFreeDiskSpace, string& PVSSDiskName, string& PVSSFreeDiskSpace)
{
  string st;
  string output1;
  string output2;
  int error;
  ProjectName = g_sProj;
  ProjectDiskName = g_sProjPath + \"/\";
  PVSSDiskName = PVSS_PATH;

  if(_WIN32)
  {
    string ProjectDiskCaption = substr(ProjectDiskName, 0, 2);
    string PVSSDiskCaption = substr(PVSSDiskName, 0, 2);

    system(\"%windir%/System32/wbem/wmic logicaldisk get caption > \" + pvrAuxPath);
    system(\"TYPE \" + pvrAuxPath + \" > \" + pvrDiskCaptionsPath);

    while(getFileSize(pvrDiskCaptionsPath) <= 0) //IM 79243 dspitzer: wait until the file that contains the drive letters exists for sure
      delay(1);

    file f1=fopen(pvrDiskCaptionsPath,\"r\");
    dyn_string captions;
    int positionPVSSDisk;
    int positionProjectDisk;
    fscanf (f1,\"%s\\n%s\\n%s\\n%s\\n%s\",output1, captions[1], captions[2], captions[3], captions[4]);
    fclose(f1);

    //The following loop finds the file line where for the project and PVSS disk captions
    for(int i=1;i<5;i++)
    {
      if (dynlen(captions) < i) continue;
      if (ProjectDiskCaption != \"\")
      {
        if(strtoupper(captions[i])==strtoupper(ProjectDiskCaption)) //IM 79243 dspitzer: be sure that the drive letters are both upper case
          positionProjectDisk = i;
      }
      if (PVSSDiskCaption != \"\")
      {
        if(strtoupper(captions[i])==strtoupper(PVSSDiskCaption)) //IM 79243 dspitzer: be sure that the drive letters are both upper case
          positionPVSSDisk = i;
      }
    }
    system(\"%windir%/System32/wbem/wmic logicaldisk get freespace > \" + pvrAuxPath);
    system(\"TYPE \" + pvrAuxPath + \" > \" + pvrDiskSpacesPath);
    file f2=fopen(pvrDiskSpacesPath,\"r\");
    dyn_string spaces;
    fscanf (f2,\"%s\\n%s\\n%s\\n%s\\n%s\",output1, spaces[1],spaces[2],spaces[3],spaces[4]);
    fclose(f2);

    PVSSFreeDiskSpace = spaces[positionPVSSDisk];
    ProjectFreeDiskSpace = spaces[positionProjectDisk];
  }
  else if(_UNIX)
  {
    system(\"df -k \" + PVSS_PATH + \" \" + g_sProjPath +\" > PVRaux.txt\");
    //system(\"TYPE PVRaux.txt >  PVRdiskCaptions.txt\");
    file f1=fopen(\"PVRaux.txt\",\"r\");
    string line;
    int positionPVSSDisk;
    int positionProjectDisk;

    // IM 109741
    dyn_string dsPVSSfree, dsPROJfree;

    fgets(line, 1024, f1);    //skip first line with captions

    // PVSS free disk
    fgets(line, 1024, f1);
    dsPVSSfree = strsplit(line, \" \");

    for ( int i = dynlen(dsPVSSfree); i > 0; i-- )
      if ( dsPVSSfree[i] == \"\" )
        dynRemove(dsPVSSfree, i);

    if ( dynlen(dsPVSSfree) )
      PVSSFreeDiskSpace = dsPVSSfree[4];
    else
      PVSSFreeDiskSpace = substr(line, 48, 8);

    // PROJ free disk
    fgets(line, 1024, f1);
    dsPROJfree = strsplit(line, \" \");

    for ( int i = dynlen(dsPROJfree); i > 0; i-- )
      if ( dsPROJfree[i] == \"\" )
        dynRemove(dsPROJfree, i);

    if ( dynlen(dsPROJfree) )
      ProjectFreeDiskSpace = dsPROJfree[4];
    else
      ProjectFreeDiskSpace = substr(line, 48, 8);

    fclose(f1);
  }
  return 0;
}

int getPVSSVersion(string& PVSSVersion)
{
  // IM 102802 -->
  string projVersion;
  if (TFProjectName.text == \"\" ||
      TFProjectDisk.text == \"\" ||
      paCfgReadValue(TFProjectDisk.text + \"config/config\", \"general\", \"proj_version\", projVersion) == -1)
  {
    // This case should never happen ...
    // ... but if so provide the running installation version
    // as there is no project to reference
    PVSSVersion = VERSION_DISP;
    return 0;
  }
  if (projVersion == VERSION)
  {
    // Return full installation version info
    PVSSVersion = VERSION_DISP;
  }
  else
  {
    // Return project version including \"not available\" marker
    PVSSVersion = projVersion + \" (SP \" + getCatStr(\"pvr\", \"notAvailable\") + \")\";
  }
  // --> IM 102802

  return 0;
}


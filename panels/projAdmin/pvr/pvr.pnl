                               \"$ProjPath:\" + g_sProjPath + \"/\"); 
    params[10] = FALSE;    

    dyn_anytype da, daa;
    child = childPanel(params, da);

    // dynlen(da) == 0: cancel
    // dynlen(da) == 1: send successfull
    // dynlen(da) == 2: send error; da[2] == error text
    if(dynlen(da) == 2)
    {
      setLogMessage(\"sendingError\", \"ERROR\");
    }
    else if (dynlen(da) == 1)
    {
      setLogMessage(\"reportSent\", \"INFO\");
    }
    else
    {
      setLogMessage(\"readyForEmail\", \"INFO\");
    }
  }
}














//  for(int k = 1; k <= dynlen(etmFiles); k++)
//  {
//    if(n == 100)
//    {
//      setLogMessage(\"Looking for overparameterized files. Process: \" + floor(k/dynlen(etmFiles))* 100 + \"%\", \"INFO\");
//      n = 0;
//    }
//    ++n;

    for(int i = 1; i <= dynlen(paths); i++)
    {
      dynClear(projectFiles);
      if(!patternMatch(\"*/\", paths[i]))
        paths[i] += \"/\";

      projectFiles = getFileNamesRec(paths[i] + \"scripts/\");
      for(int j = 1; j <= dynlen(projectFiles); j++)
      {
//        if(etmFiles[k] == projectFiles[j])
        if(dynContains(etmFiles, projectFiles[j]) > 0)
        {
          dynAppend(OverparameterizedFiles, paths[i] + \"scripts/\" + projectFiles[j]);
        }
      }
    }
//  }

  return 0;
}

int createReportFile()
{
  file f=fopen(reportPath,\"w\");
  string description = TEDescription.text;

  if(g_bRemote)
    fprintf(f, \"# WARNING - THIS PVR WAS CREATED ON A REMOTE PROJECT AND NOT ON A SERVER!\\n\");

  fprintf(f, \"# Report version\\n%s\\n\", ver);
  fprintf(f, \"# Date\\n%s\\n\", formatTime(\"%d.%m.%Y %H:%M:%S\", getCurrentTime()));
  fprintf(f, \"# Description\\n%s\\n\", description);
  fprintf(f, \"# Operating System\\n%s\\n\", OSType);
  fprintf(f, \"# Operating System Version\\n%s\\n\", OSVersion);
  fprintf(f, \"# Operating System Service Pack Level\\n%s\\n\", OSServicePack);
  fprintf(f, \"# WinCC OA Version\\n%s\\n\", PVSSVersion);
  fprintf(f, \"# WinCC OA Installation Path\\n%s\\n\", PVSSDiskName);
  fprintf(f, \"# Available WinCC OA Disk (MBytes)\\n%f\\n\", fPVSSFreeDiskSpace);
  fprintf(f, \"# Patch files\\n\");
  for (int i=1;i<=dynlen(PVSSPatches);i++)
  {
     fprintf(f, \"%s\\n\", PVSSPatches[i]);
  }
//   fprintf(f, \"# List of subprojects\\n%s\\n\", PVSSProjects);
  fprintf(f, \"# Project Disk\\n%s\\n\", ProjectDisk);
  fprintf(f, \"# Available Project Disk (MBytes)\\n%f\\n\", fProjectDiskSpace);
  fprintf(f, \"# Additional files\\n\");
  for (int i=1;i<=dynlen(FilesToBeZipped);i++)
  {
     fprintf(f, \"%s\\n\", FilesToBeZipped[i]);
  }
  string comments = TEComments.text;

  fprintf(f, \"# Comments\\n%s\\n\", comments);
  fprintf(f, \"# Subprojects Path\\n\");
  for (int i=1;i<=dynlen(SubProjectsPath);i++)
  {
     fprintf(f, \"%s\\n\", SubProjectsPath[i]);
  }
  fprintf(f, \"# Overparameterized Files\\n\");
  for (int i=1;i<=dynlen(OverparameterizedFiles);i++)
  {
     fprintf(f, \"%s\\n\", OverparameterizedFiles[i]);
  }

  if(isRDBUsed())
  {
    fprintf(f, \"# Oracle Information (SQLPlus)\\n%s\\n\", sOrclSQLP);
    fprintf(f, \"# Oracle Information (tnsping)\\n%s\\n\", sOrclTNSP);
  }

  fprintf(f, \"# Listing of the bin dir of the WinCC OA Version\\n%s\\n\", sBinDir);

  fclose(f);

  return 0;
}

setLogMessage(string catKey, string type = \"ERROR\")
{
  string colorName;
  type = strtoupper(type);

  switch (type)
  {
    case \"PROGRESS\":
      colorName = \"red\";
      break;

    case \"INFO\":
      colorName = \"STD_led_On\";
      break;

    case \"WARNING\":
      colorName = \"STD_value_default\";
      break;

    case \"ERROR\":
      colorName = \"STD_value_not_ok\";
      break;

    default:
      DebugN(\"Undefined type of error\" + type);
      return;
  }

  string message;

  if (type != \"PROGRESS\")
    message += type + \": \";

  message += getCatStr(\"pvr\", catKey);

  TFErrorMessage.visible = true;
  TFErrorMessage.text = message;
  TFErrorMessage.foreCol = colorName;
}

bool isRDBUsed()
{
  bool bRet = FALSE;
  string s;

  paCfgReadValue(PVSSDiskName + \"config/config\", \"general\", \"useRDBArchive\", s);

  if(s == \"1\")
    bRet = TRUE;

  //only if useRDBArchive wasn't true in previous config
  if(!bRet)
  {
    paCfgReadValue(ProjectDisk + \"config/config\", \"general\", \"useRDBArchive\", s);

    if(s == \"1\")
      bRet = TRUE;
  }

  //for each subproject but
  //only if useRDBArchive wasn't true in previous config
  for(int i = 1; i <= dynlen(SubProjectsPath) && !bRet; i++)
  {
    paCfgReadValue(SubProjectsPath[i] + \"/config/config\", \"general\", \"useRDBArchive\", s);

    if(s == \"1\")
      bRet = TRUE;
  }

  return bRet;
}

void extendPanel(bool bDown = TRUE)
{
  int i, x, y;
  dyn_string dsShapesSI = makeDynString(\"frmSetupInfo\",    \"lblOS\",               \"TFOSType\",
                                        \"frmPVSSSetup\",    \"lblVersion\",          \"TFPVSSVersion\",
                                        \"lblPatches\",      \"btPatches\",           \"lblPath\",
                                        \"TFPVSSDisk\",      \"lblRegProjs\",         \"btRegisteredProjects\",
                                        \"lblFreeDiskPVSS\", \"TFPVSSDiskSpace\",     \"frmProject\",
                                        \"lblName\",         \"TFProjectName\",       \"lblSubProjects\",
                                        \"btSubprojects\",   \"lblProjPath\",         \"TFProjectDisk\",
                                        \"lblOverParam\",    \"btOverparameterized\", \"lblFreeDiskProj\",
                                        \"TFProjectDiskSpace\");
  dyn_string dsShapes = makeDynString(\"lblZipName\", \"TFZipFileName\", \"btEmail\", \"cmdClose\", \"cmdExtend\", \"barBack\", \"BCreateZip\");

  for(i = 1; i <= dynlen(dsShapesSI); i++)
  {
    getValue(dsShapesSI[i], \"position\", x, y);
    setValue(dsShapesSI[i], \"position\", x, (bDown ? y + 100 : y - 100));
  }

  for(i = 1; i <= dynlen(dsShapes); i++)
  {
    getValue(dsShapes[i], \"position\", x, y);
    setValue(dsShapes[i], \"position\", x, (bDown ? y - 300 : y + 300));
    setValue(\"cmdExtend\", \"text\", (bDown ? \">>\":\"<<\"));
  }

  panelSize(\"\", x, y);
  setPanelSize(myModuleName(), myPanelName(), FALSE, x, (bDown ? 480 : 780));
}


























































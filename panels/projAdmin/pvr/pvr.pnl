int getProjects(string& PVSSProjects)
{
  string content;
  string s;
  file f;

  if(_WIN32)
  {
    f=fopen(getenv(\"PROGRAMDATA\")+\"\\\\Siemens\\\\WinCC_OA\\\\pvssinst.conf\",\"r\");

    while (feof(f)==0) // as long as it is not at the end of the file
    {
       fgets(s,100,f); // Import string again
       content = content + s;
    }
    fclose(f);
    PVSSProjects = content;
  }
  else if(_UNIX)
  {
    f = fopen(\"/etc/opt/pvss/pvssInst.conf\", \"r\");
    while (feof(f)==0) // as long as it is not at the end of the file
    {
       fgets(s,100,f); // Import string again
       content = content + s;
    }
    fclose(f);
    PVSSProjects = content;
  }
  else
  {
    setLogMessage(\"undedfinedOS\");
    return -1;
  }

  return 0;
}

int getSubProjectsPath(dyn_string& SubProjectsPath)
{
  string filename = g_sProjPath + \"/\" + CONFIG_REL_PATH + \"config\";
  paCfgReadValueList(filename, \"general\", \"proj_path\", SubProjectsPath);

  for(int i =1; i <= dynlen(SubProjectsPath); i++)
    strreplace(SubProjectsPath[i], \"\\\\\", \"/\");

  //rglock replaced begin
  //dynRemove(SubProjectsPath, dynContains(SubProjectsPath, PROJ_PATH));
  dynRemove(SubProjectsPath, dynlen(SubProjectsPath));
  //rglock replaced end

  string projPath = g_sProjPath;
  projPath = strrtrim(projPath, \"/\");
  projPath = strrtrim(projPath, \"\\\\\");
  dynRemove(SubProjectsPath, dynContains(SubProjectsPath, projPath));

  return 0;
}


"int cmGenerateReport(string filename)
{
  CmContainerDataProcessor dataProcessor;
  return dataProcessor.generateReport(filename);
}

main()
{
  // get file name TFZipFileName and check it is != \"\"
  // create zip that contains:  config files, prog files, shield files (btb just current project)
  int error = createReportFile();
  ZipFileName = TFZipFileName.text;
  if(ZipFileName==\"\")
  {
    setLogMessage(\"zipNotDefined\");
    return;
  }
  if(strpos(ZipFileName, \".zip\")<0)
  {
    ZipFileName = ZipFileName + \".zip\";
  }

  // log directory
  string logDir = getPath(LOG_REL_PATH, \"\", getActiveLang(), SEARCH_PATH_LEN);

  // start creating report
  setLogMessage(\"reportInProgress\", \"PROGRESS\");
  string files;

  // shield
  if(access(PVSSDiskName + \"shield.txt\",F_OK) == 0)
    files = PVSSDiskName + \"shield.txt \";
  else if(access(PVSSDiskName + \"shield\", F_OK) == 0)
    files = PVSSDiskName + \"shield \";

  // CodeMeter
  setLogMessage(\"cmReportInProgress\", \"PROGRESS\");
  string codeMeterReportFile = logDir + \"CodeMeterReport.txt\";
  cmGenerateReport(codeMeterReportFile);
  files = files + codeMeterReportFile + \" \";

  // diverse configuration files from installation
  setLogMessage(\"reportInProgress\", \"PROGRESS\");
  files = files + PVSSDiskName + \"config/config\" + \" \";
  files = files + PVSSDiskName + \"config/progs\" + \" \";

  if(access(PVSSDiskName + \"config/config.level\", \"F_OK\") == 0)
    files = files + PVSSDiskName + \"config/config.level\" + \" \";

  if(access(PVSSDiskName + \"config/config.redu\", \"F_OK\") == 0)
    files = files + PVSSDiskName + \"config/config.redu\" + \" \";

  // create hw.txt
  if(_WIN32)
    system(PVSS_PATH + \"bin/\" + getComponentName(GETHW_COMPONENT) + \".exe > \" + logDir + \"hw.txt\");
  else if(_UNIX)
    system(PVSS_PATH + \"bin/\" + getComponentName(GETHW_COMPONENT) + \" > \" + logDir + \"hw.txt\");

  files = files + logDir + \"hw.txt\" + \" \";

  // patches
  dyn_string dss = getFileNames(PVSS_PATH + \"install/\", \"*.lst\");

  for(int j = 1; j <= dynlen(dss); j++)
    files = files + PVSS_PATH + \"install/\" + dss[j] + \" \";

  // progs and configs
  files = files + ProjectDisk + CONFIG_REL_PATH + \"config\" + \" \";
  files = files + ProjectDisk + CONFIG_REL_PATH + \"progs\" + \" \";

  if(access(ProjectDisk + \"config/config.level\", \"F_OK\") == 0)
    files = files + ProjectDisk + \"config/config.level\" + \" \";

  if(access(ProjectDisk + \"config/config.redu\", \"F_OK\") == 0)
    files = files + ProjectDisk + \"config/config.redu\" + \" \";

  for(int i = 1; i <= dynlen(SubProjectsPath); i++)
  {
    if(!patternMatch(\"*/\", SubProjectsPath[i]))
      SubProjectsPath[i] += \"/\";

    //for each proj_path retrieve the list of config and progs files:
    dyn_string ds = getFileNames(SubProjectsPath[i] + CONFIG_REL_PATH, \"config.*\");

    for(int j = 1; j <= dynlen(ds); j++)
      files = files + SubProjectsPath[i] + CONFIG_REL_PATH + ds[j] + \" \";
    if(access(SubProjectsPath[i] + CONFIG_REL_PATH + \"config\", \"F_OK\") == 0)
      files = files + SubProjectsPath[i] + CONFIG_REL_PATH + \"config\" + \" \";

    if(access(SubProjectsPath[i] + CONFIG_REL_PATH + \"progs\", \"F_OK\") == 0)
      files = files + SubProjectsPath[i] + CONFIG_REL_PATH + \"progs\" + \" \";

    if(access(SubProjectsPath[i] + CONFIG_REL_PATH + \"config.level\", \"F_OK\") == 0)
      files = files + SubProjectsPath[i] + CONFIG_REL_PATH + \"config.level\" + \" \";

    if(access(SubProjectsPath[i] + CONFIG_REL_PATH + \"config.redu\", \"F_OK\") == 0)
      files = files + SubProjectsPath[i] + CONFIG_REL_PATH + \"config.redu\" + \" \";
  }

  files = files + reportPath + \" \";

  for (int i=1;i<=dynlen(FilesToBeZipped);i++)
  {
     files = files + \"\\\"\" + FilesToBeZipped[i] + \"\\\"\" + \" \";
  }

  for (int i=1;i<=dynlen(OverparameterizedFiles);i++)
  {
     files = files + \"\\\"\" + OverparameterizedFiles[i] + \"\\\"\" + \" \";
  }

  //IM 104621 start - update zip file, string length (files): max. 8000 characters
  bool bFirstZip = true;
  string sFileParts;
  dyn_string dsSingleFile;
  int iSize;
  dsSingleFile = strsplit(files, \" \");

  for (int i=1;i<=dynlen(dsSingleFile);i++)
  {
    iSize += strlen(dsSingleFile[i]);
    if (iSize >= commandSize - 512) // corrected with TFS 73810
    {
      if (bFirstZip)
      {
        i--;
        bFirstZip = false;
        runZip(sFileParts, false);
        sFileParts = \"\";
        iSize = 0;
      }
      else
      {
        i--;
        runZip(sFileParts, true);
        sFileParts = \"\";
        iSize = 0;
      }
    }
    else
    {
      sFileParts += dsSingleFile[i] + \" \";
    }
  }

  if (bFirstZip)
  {
    bFirstZip = false;
    runZip(sFileParts, false);
  }
  else
  {
    runZip(sFileParts, true);
  }
  // IM 104621 end

  string content=\"\";
  string s=\"\";
  file f=fopen(tmpOutPath,\"r\");
  while (feof(f)==0) // as long as it is not at the end of the file
  {
    fgets(s,100,f); // Import string again
    content = content + s;
  }
  fclose(f);

  int error = strpos(content, \"sent successfully\");
  if(error>=0)
  {
    setLogMessage(\"reportSuccess\", \"INFO\");
  }
  error = strpos(content, \"warning\");
  if(error>=0)
  {
    setLogMessage(\"problemRep\", \"WARNING\");
    DebugN(\"PVR WARNINGS: \" + content);
  }

  //enable email button if zip file was created...even if there were errors:
  if(access(ZipFileName, R_OK) == 0)
  {
    setLogMessage(\"readyForEmail\", \"INFO\");
    btEmail.enabled = true;
    TECommon.text = \"\";
    TECommon.insertParagraph(getCatStr(\"pvr\",\"weblink\"),1);
    TECommon.insertParagraph(\" \",2);
    TECommon.insertParagraph(getCatStr(\"pvr\",\"weblink_1\"),3);
    TECommon.insertParagraph(\"<span style='color:red'><b>\"+getCatStr(\"pvr\",\"weblink_1_1\")+\"</b></span>\",4);
    TECommon.insertParagraph(\" \",5);
    TECommon.insertParagraph(getCatStr(\"pvr\",\"weblink_2\"),6);
    TECommon.insertParagraph(\" \",7);  }
  else
  {
    setLogMessage(\"repFailed\", \"ERROR\");
    btEmail.enabled = false;
  }
}

langString gLsColTooltip;

string getUniqueName(shape table)
{
  int max = 0, num, i;
  for (i = 0; i < table.columnCount; i++)
  {
    string name = table.columnName(i);
    if ( name[0] != '#' )
      continue;
    num = (int)(substr(name, 1));
    if ( num > max ) max = num;
  }
  return \"#\" + (string)(max+1);
}" 0

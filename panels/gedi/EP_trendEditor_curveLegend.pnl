    trend.curveLegendShowMilli(ds[ngSelectedTrendCurve]) = (TimeFormatMsec.state(0)>0);

    TimeFormatDate.enabled=true;
    TimeFormatMsec.enabled=true;
    ValueFormatDecimal.enabled=true;
  }
}

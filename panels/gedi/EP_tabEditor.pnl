string getUniqueName(shape tab)
{
  int max = 0, num, i;
  for (i = 0; i < tab.registerCount; i++)
  {
    string name = tab.registerName(i);
    if ( name[0] != '#' )
      continue;
    num = (int)(substr(name, 1));
    if ( num > max ) max = num;
  }
  return \"#\" + (string)(max+1);

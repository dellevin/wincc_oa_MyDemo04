"main()
{
dyn_string dsData;                // gesammelte Paneldaten
int        iTyp;                  // Typ der Panelfunktion
string     sFilename;             // Paneldatei
string     sFilenameOrig;
string     sPanelname;            // Panelname
string     sPara;                 // $-Parameter
dyn_string dsPara;                // $-Parameter


  //Buttons setzen
  setMultiValue(\"Help\",\"visible\",true,
                \"Back\",\"visible\",true,
                \"Fore\",\"visible\", true,
                \"OK\",  \"visible\", false,
                \"Cancel\",\"visible\",true);

  // Daten aus Dynfeld laden
  ep_searchData(\"gedi/ep_panelOn_w.pnl\",dsData);
  // ausgelesene Daten verarbeiten
  if(dynlen(dsData)>1)
  {
    sFilename=  dsData[2];
    sPanelname= dsData[3];
    sPara=      dsData[4];
  }
  // keine Daten geladen > default Werte
  else
  {
    sFilename=  \"\";
    sPanelname= \"\";
    sPara=      \"\";
  }
  strreplace(sPara, \" | \", \"|\");
  dsPara= ep_strToDyn(sPara, '|');
  if(sPara==\"\") dsPara= makeDynString();

  // Daten in das Panel eintragen
  setMultiValue(\"filename\", \"text\",  sFilename,
                \"panelname\",\"text\",  sPanelname,
                \"dyn_list\", \"items\", dsPara);

  // DollarParameter aus File holen bzw. aktuallisieren
  while(1)
  {
    getValue(\"filename\", \"text\", sFilename);
    if(sFilename != sFilenameOrig)
    {
      sFilenameOrig= sFilename;
      dollarInit(sFilename);
    }
    delay(0, 500);
  }
}

// DollarParameter aus File holen bzw. aktuallisieren
void dollarInit(string sFile)
{
  int i, j, iAnz;
  dyn_string asDollar;  // nötige $Parameter
  dyn_string asPara;    // Liste der $Parameter
  string     fullPath;
  bool       bVorh;
  string filename;
  string filePath;

  filename = getFileNameFromPath(sFile);
  int pos = strpos(sFile, filename);
  filePath = substr(sFile, 0, pos);

  // Abbrechen wenn es das Panel nicht gibt
  fullPath = PROJ_PATH + \"panels/\" + filePath;
  if( dynlen(getFileNames(fullPath, filename))<1 )
  {
    fullPath = PVSS_PATH + \"panels/\" + filePath;
    if( dynlen(getFileNames(fullPath, filename))<1 )
    {
      return;
    }
  }

  // Dollarparameter einlesen
  getDollarList(sFile, asDollar);
  iAnz= dynlen(asDollar);

  // Liste einlesen
  getValue(\"dyn_list\", \"items\", asPara);

  // Evtl. Einträge hinzufügen
  for(i=1; i<=iAnz; i++)
  {
    j= 1;
    bVorh= false;
    // Prüfen ob Eintrag schon vorhanden
    while(j<=dynlen(asPara) && !bVorh)
    {
      bVorh= (strpos(asPara[j], asDollar[i])==0);
      j++;
    }
    if(!bVorh)
      dynAppend(asPara, asDollar[i]+\":\"+asDollar[i]);
  }

  // Evtl. zuviele Einträge löschen
  iAnz= dynlen(asPara);
  for(i=iAnz; i>=1; i--)
  {
    j= 1;
    bVorh= false;
    // Prüfen ob Eintrag schon vorhanden
    while(j<=dynlen(asDollar) && !bVorh)
    {
      bVorh= (strpos(asPara[i], asDollar[j])==0);
      j++;
    }
    if(!bVorh)
      dynRemove(asPara, i);
  }

  // Liste neu setzen
  dynSortAsc(asPara);
  setValue(\"dyn_list\", \"items\", asPara);
}

"main()
{
  if(g_epUlcCompatibleScripts)   //Wenn der ULC Kompatibilitaetsmodus aktiviert wurde nur xml Panels zur Auswahl anbieten da der ULC keine pnl Panels unterstuetzt
  {
    string sAbsPanelsPath, sFileName;
    fileSelector(sFileName, getPath(PANELS_REL_PATH), TRUE, \"*.xml\", TRUE);

    if(sFileName == \"\")  //Der FileSelector wurde gecancelt daher das aktuell eingetragene Panel im Textfeld stehen lassen
      return;

    for(int i=1; i<=SEARCH_PATH_LEN; i++)  //Alle moeglichen Projektpfade fuer Panels durchgehen um den relativen Pfad des ausgewaehlten Panels ermitteln zu koennen
    {
      sAbsPanelsPath = getPath(PANELS_REL_PATH, \"\", 0, i);

      if(strpos(sFileName, sAbsPanelsPath) == 0)
      {
        strreplace(sFileName, sAbsPanelsPath, \"\");  //Den absoluten Pfad zum Panels Ordner aus dem Filenamen entfernen da im Script der Panelname relativ gespeichert werden muss
        break;
      }
    }

    setValue(\"filename\",\"text\",sFileName);
  }
  else  //Kein ULC Kompatibilitaetsmodus daher die alte Implementierung fuer die Panelauswahl verwenden
  {
    string sFileName;
    getValue(\"filename\",\"text\",sFileName);
    panelSelector(sFileName);

    if(sFileName == \"\")  //Der PanelSelector wurde gecancelt daher das aktuell eingetragene Panel im Textfeld stehen lassen
      return;

    setValue(\"filename\",\"text\",sFileName);
  }
}

  dyn_string ds;
  
  if (!dpExists($dpe)) return;

  strreplace(sTemp,\"SPS-\",\"\");
  strreplace(sTemp,\"plc \",\"\");
  ChildPanelOnCentralModalReturn(\"vision/MessageWarning2\",
    this.text,
    makeDynString(sTemp,
                  getCatStr(\"para\",\"yes\"),
                  getCatStr(\"para\",\"no\")),

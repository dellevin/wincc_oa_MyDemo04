  if (!df[1])
    return;  

  // resetting datapoint value
 	dpSet($dpe+\".value.dpValue:_original.._value\", 0.0);
 	txtDpValue.text = 0.0;
  
  // resetting pvss counter
  dpGet($dpe+\".value.pvssCounter:_dp_fct.._param\",param,  
        $dpe+\".value.pvssCounter:_dp_fct.._global\",globals,  
        $dpe+\".value.pvssCounter:_dp_fct.._fct\",fct,
        $dpe+\".counter.value:_online.._value\",hourValue);  

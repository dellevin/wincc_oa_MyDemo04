        $dpe+\".value.pvssCounter:_dp_fct.._param\",param,  
        $dpe+\".value.pvssCounter:_dp_fct.._global\",globals,  
        $dpe+\".value.pvssCounter:_dp_fct.._fct\",fct);  
  txtpvssCounter.text = 0.0;

  // resetting countervalue

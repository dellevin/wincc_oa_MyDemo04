         fct2;
  dyn_int      st_t;
  dyn_float    df_ret;
  dyn_string   ds=lstLt.items,ds_ret;
  langString   ls=ds;
  dyn_errClass eClass;

  if ( overflow<=0 || tolerance<=0 || max<=0 )
  {
    ChildPanelOnCentralModalReturn(\"vision/MessageWarning\",
      getCatStr(\"para\",\"warning\"),makeDynString(getCatStr(\"STD_Symbols\",\"badparacounter\")),
      df_ret,ds_ret);

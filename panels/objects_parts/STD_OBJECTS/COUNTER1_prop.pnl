 "main()
{
  bool       b;
  int        type;
  string     source;
  dyn_string ds;

  type=this.number+1;
  
  if (type==2)
  {
    dpGet($dpe+\".value.dpValue:_dp_fct.._param\",ds); 
    if (dynlen(ds)>0)
      txtDp.text=dpSubStr(ds[1],DPSUB_SYS_DP_EL);
    else
      txtDp.text=\"\";
    source=txtDp.text;
  }
  
//  getCAddresses();
//  getResetAddresses();
  b=(type!=2 || source==$dpe+\".value.plcCounter\");
  if (b)
  {
    getCAddresses();
    getResetAddresses();
  }
  else
  {
    txtCounterAddress.text=\"\";
    txtResetAddress.text=\"\";

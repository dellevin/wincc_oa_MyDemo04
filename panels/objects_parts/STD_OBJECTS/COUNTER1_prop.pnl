  string dp=$dpe+\".value.\",plcAddress;
  dyn_float df;
 dyn_string ds, leaf, attr;
 string s, rootdp, sSearchDp;
  if (number==2) return;

  dp+=(number==1)?\"plcCounter\":\"plcImpulse\";

  rootdp = dpSubStr(dp, DPSUB_DP);
  
  sSearchDp = dp;
  
  strreplace( sSearchDp, rootdp, \"\");
  
  sSearchDp = \"_mp_\"+dpTypeName(dp)+sSearchDp+\":_address\";

  dpGet(\"_dt_\"+ dpTypeName(dp) +\".Leaf:_online.._value\",leaf,
        \"_dt_\"+ dpTypeName(dp) +\".DynamicAttribute:_online.._value\",attr);
  iPos = dynContains(leaf, sSearchDp);
  if ( iPos < 1 ) return;

  s = attr[iPos]; strreplace(s,\"_da\",\"\");



  ChildPanelOnCentralModalReturn(\"objects_parts/STD_OBJECTS/COMMON_prop_single.pnl\",
    getCatStr(\"STD_Symbols\",\"PARA:\"),
    makeDynString(\"$da_type:\"+s,\"$dpe:\"+dp),df,ds);

  dpGet(dp+\":_address.._active\",plcAddressActive,
        dp+\":_address.._reference\", plcAddress);
  if (strrtrim(strltrim(plcAddress))!=\"\" && !plcAddressActive)
    plcAddress=getCatStr(\"STD_Symbols\",\"inactive\")+plcAddress;
  txtCounterAddress.text=plcAddress;
}

"main()
{
  bool       b,moreLang=(getNoOfLangs()>1);
  int        type;
  string     source;
  dyn_string ds,dls;
  langString ls;

  txtLt.editable=!moreLang;
  langedit3.visible=moreLang;
  dpGet($dpe+\".para.type:_online.._value\",type);
  ls=dpGetComment($dpe+\".\");
  dls=ls;
  lstLt.items=dls;
  txtLt.text=ls;
  txtLt.editable=!moreLang;
  langedit3.visible=moreLang;
  
  if (type==2)
  {
    dpGet($dpe+\".value.dpValue:_dp_fct.._param\",ds); 
    if (dynlen(ds)>0)
      txtDp.text=dpSubStr(ds[1],DPSUB_SYS_DP_EL);
    else
      txtDp.text=\"\";
    source=txtDp.text;
  }
  
  b=(type!=2 || source==$dpe+\".value.plcCounter\");
  if (b)
  {
    getCAddresses();
    getResetAddresses();
  }
  else
  {
    txtCounterAddress.text=\"\";
    txtResetAddress.text=\"\";
  }
  cmdCounterAddress.visible=b;
  txtCounterAddress.visible=b;
  Text2.visible=b;
  cmdResetAddress.visible=b;
  txtResetAddress.visible=b;
  Text3.visible=b;
}

getCAddresses()
{
  int    type;
  bool   plcAddressActive;
  string plcAddress,
         dp=$dpe+\".value.\";
  dyn_errClass eClass;

  dpGet($dpe+\".para.type:_online.._value\",type);
  if (type==2) return;

  dp+=(type==1)?\"plcCounter\":\"plcImpulse\";
  
  dpGet(dp+\":_address.._active\",plcAddressActive);
  eClass=getLastError();
  if (dynlen(eClass)>0)
    plcAddress=\"\";
  else
  {
    dpGet(dp+\":_address.._reference\", plcAddress);
    eClass=getLastError();
    if (dynlen(eClass)>0) plcAddress=\"\";
  }
  if (strrtrim(strltrim(plcAddress))!=\"\" && !plcAddressActive)
    plcAddress=getCatStr(\"STD_Symbols\",\"inactive\")+plcAddress;
  txtCounterAddress.text=plcAddress;
}

getResetAddresses()
{
  int    type;
  bool   plcAddressActive;
  string plcAddress,
         dp=$dpe+\".cmd.resetPLC\";
  dyn_errClass eClass;

  dpGet($dpe+\".para.type:_online.._value\",type);
  if (type==2) return;

  dpGet(dp+\":_address.._active\",plcAddressActive);
  eClass=getLastError();
  if (dynlen(eClass)>0)
    plcAddress=\"\";
  else
  {
    dpGet(dp+\":_address.._reference\", plcAddress);
    eClass=getLastError();
    if (dynlen(eClass)>0) plcAddress=\"\";
  }
  if (strrtrim(strltrim(plcAddress))!=\"\" && !plcAddressActive)
    plcAddress=getCatStr(\"STD_Symbols\",\"inactive\")+plcAddress;
  txtResetAddress.text=plcAddress;
}


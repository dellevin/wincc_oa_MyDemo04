    dpSet($dpe+\".value.plcImpulse:_original.._value\",0);
  if ((iTime=tDp)==0)
    dpSet($dpe+\".value.dpValue:_original.._value\",0.0);
  if ((iTime=tPlc)==0)
    dpSet($dpe+\".value.plcCounter:_original.._value\",0);

  dpGet($dpe+\".counter.value:_pv_range.._type\",pvType);

  if (pvType!=DPCONFIG_MINMAX_PVSS_RANGECHECK)
    dpSetWait($dpe+\".counter.value:_pv_range.._type\",DPCONFIG_MINMAX_PVSS_RANGECHECK);
  dpSet($dpe+\".counter.value:_pv_range.._min\",min,
        $dpe+\".counter.value:_pv_range.._max\",max);

  if(type==2)

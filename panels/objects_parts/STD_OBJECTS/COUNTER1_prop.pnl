//      dpSetWait( $dpe+\".value.dpValue:_dp_fct.._param\",makeDynString(dp+\":_online.._value\")); 
      eClass=getLastError();
      if ( dynlen(eClass) ) errorDialog(eClass);
    }
  }
  else

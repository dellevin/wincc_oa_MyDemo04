getCAddresses()
{
  int    type;
  bool   plcAddressActive;
  string plcAddress,
         dp=$dpe+\".value.\";
  dyn_errClass eClass;

  type=this.number+1;
  if (type==2) return;

  dp+=(type==1)?\"plcCounter\":\"plcImpulse\";
  
  dpGet(dp+\":_address.._active\",plcAddressActive);
  eClass=getLastError();
  if (dynlen(eClass)>0)
    plcAddress=\"\";
  else
  {
    dpGet(dp+\":_address.._reference\", plcAddress);
    eClass=getLastError();
    if (dynlen(eClass)>0) plcAddress=\"\";
  }
  if (strrtrim(strltrim(plcAddress))!=\"\" && !plcAddressActive)
    plcAddress=getCatStr(\"STD_Symbols\",\"inactive\")+plcAddress;
  txtCounterAddress.text=plcAddress;
}

getResetAddresses()
{

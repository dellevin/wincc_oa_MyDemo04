 else if(dpExists($dpe))
 {
 if(glsFarbe==\"\")
  glsFarbe=\"STD_state\";

    // Pr√ºfen ob meldebehandlung-Konfig vorhanden    
    dpGet( DPE + \":_alert_hdl.._type\", iAType);
    STD_checkAddress(DPE,bCheckAddress);
 

    if(bCheckAddress) 
    { 
    
    setValue(\"\",\"visible\",1); 
        if( iAType!=0 ) {  
            if( dpConnect(\"MT_S_1\",1,DPE + \":_alert_hdl.._act_state_color\",  

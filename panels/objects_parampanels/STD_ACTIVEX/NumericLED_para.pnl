{
  int        Col=this.number;
  string     cOn,cOff,cBack;
  dyn_float  df;
  dyn_string ColOn    = makeDynString(\"[100,0,0]\",\"[0,100,0]\",\"[100,100,100]\"),
             ColOff   = makeDynString(\"[20,0,0]\",\"[0,20,0]\",\"[20,20,20]\"),

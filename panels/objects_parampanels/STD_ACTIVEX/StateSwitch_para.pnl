"DragStart" "main(int keys)
{
  out=1;
  float x,y,xo,yo,xk,yk,alpha_start,alpha,alpha_rot, alpha_soll;
  getCursorPosition(xo,yo);
//   alpha_rot=Dragger.rotation;
  getValue(\"4Dragger\",\"rotation\",alpha_rot);
  while(out)
    {
      delay(0,50);
      getValue(\"4Dragger\",\"position\",xk,yk);
      getCursorPosition(x,y); 
      
      alpha_start=90+rad2deg(asin((yk-yo)/(sqrt(pow(yk-yo,2)+pow(xo-xk,2))))); 
      if ((xo-xk)>0) alpha_start=360-alpha_start;
      alpha=90+rad2deg(asin((yk-y)/(sqrt(pow(yk-y,2)+pow(x-xk,2))))); 
      if ((x-xk)>0) alpha=360-alpha;
      alpha_soll=((alpha_start-alpha)+alpha_rot); 
      if (alpha_soll>=360)alpha_soll=alpha_soll-360;
      if (alpha_soll<0)alpha_soll=alpha_soll+360;

      if ((alpha_soll>=295)||(alpha_soll<=65))
      { 
        setValue(\"4Dragger\",\"rotation\",alpha_soll);
//         Dragger.rotation=alpha_soll;  
        SchalterBalken.rotation=alpha_soll;  
        Pfeil.rotation=alpha_soll;
      }
    }
    if ((alpha_soll>65)  &&(alpha_soll<=180)) alpha_soll=60;
    if ((alpha_soll<65)  &&(alpha_soll>=45))  alpha_soll=60;
    if ((alpha_soll<45)  &&(alpha_soll>=0))   alpha_soll=30;    
    if ((alpha_soll<360) &&(alpha_soll>=315)) alpha_soll=330;  
    if ((alpha_soll<315) &&(alpha_soll>=295)) alpha_soll=300;
    if ((alpha_soll>180) &&(alpha_soll<=295)) alpha_soll=300;
    if (alpha_soll<0)                         alpha_soll=0;

  if ((alpha_soll>=295)||(alpha_soll<=65))
   { 
    setValue(\"4Dragger\",\"rotation\",alpha_soll);
//     Dragger.rotation=alpha_soll;  
    SchalterBalken.rotation=alpha_soll;  
    Pfeil.rotation=alpha_soll;
   }



}"

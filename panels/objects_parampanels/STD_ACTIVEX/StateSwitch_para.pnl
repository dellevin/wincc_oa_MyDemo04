"$refFilename"
"main() 
{ 
  int        type,nr,pos;
  //arguments of \"getDollarParams\"                          
  bool       IBit;
  string     refName,Type,s=$refFilename;
  dyn_string dollars, values;        
  dyn_string color;
  
  // $-Parameter
  string SwitchText;
  string Dp1,Dp2,Dp3,Dp4,Txt1,Txt2,Txt3,Txt4;

  pos=strpos(s,\"StateSwitch.pnl\");
  if (pos>0)
  {
    s=substr(s,pos-1,1);
    nr=s;
    txtNr.text=nr;
//     axExample.Selections=nr;
  }

  //gets the values of the reference
  getDollarParams(refName,dollars,values);  
  Type        = values[dynContains(dollars,\"$s_Type\")];
  SwitchText  = values[dynContains(dollars,\"$s_SwitchText\")];
  Txt1        = values[dynContains(dollars,\"$s_Txt1\")];
  Txt2        = values[dynContains(dollars,\"$s_Txt2\")];
  Txt3        = values[dynContains(dollars,\"$s_Txt3\")];  
  Dp1         = values[dynContains(dollars,\"$s_Dp1\")];
  Dp2         = values[dynContains(dollars,\"$s_Dp2\")];
  Dp3         = values[dynContains(dollars,\"$s_Dp3\")];
  IBit        = values[dynContains(dollars,\"$b_IBit\")];
  if (nr==4)
  {  
    Txt4        = values[dynContains(dollars,\"$s_Txt4\")];  
    Dp4         = values[dynContains(dollars,\"$s_Dp4\")];
  }

  
  //shows the name of the refrence
  setValue(\"refNameBox\",\"text\",refName); 

  //shows the $-parameters and the corresponding values
  if (SwitchText==\"$s_SwitchText\") SwitchText=\"\";
  if (Type==\"$s_Type\")
  {
    Type=0;
    IBit=false;
  }
  type=Type;

  if (Dp1==\"$s_Dp1\") Dp1=\"\";
  if (Dp2==\"$s_Dp2\") Dp2=\"\";
  if (Dp3==\"$s_Dp3\") Dp3=\"\";
  if (Dp4==\"$s_Dp4\") Dp4=\"\";
  if (Txt1==\"$s_Txt1\") Txt1=\"1\";
  if (Txt2==\"$s_Txt2\") Txt2=\"2\";
  if (Txt3==\"$s_Txt3\") Txt3=\"3\";
  if (Txt4==\"$s_Txt4\") Txt4=\"4\";
  
  // IM 118062  
  
  rboDp.text(0, \"1 \" + getCatStr(\"STD_Symbols\", \"analog\"));
  rboDp.text(1, \"3 \" + getCatStr(\"STD_Symbols\", \"digital\"));
  
  if ( nr == 4 )
  {
    rboDp.text(0, \"1 \" + getCatStr(\"STD_Symbols\", \"analog\"));
    rboDp.text(1, \"4 \" + getCatStr(\"STD_Symbols\", \"digital\"));
  }  
  
  setMultiValue(\"rboDp\",\"number\",type,
                \"IBit\",\"state\",0,IBit,
                \"txt2\",\"visible\",type,
                \"txt3\",\"visible\",type,
                \"txt4\",\"visible\",(nr==4 && type==1),
                \"txtDp2\",\"visible\",type,
                \"txtDp3\",\"visible\",type,
                \"txtDp4\",\"visible\",(nr==4 && type==1),
                \"txtDp1\",\"text\",Dp1,
                \"txtDp2\",\"text\",Dp2,
                \"txtDp3\",\"text\",Dp3,
                \"txtDp4\",\"text\",Dp4,
                \"txtSwitchText\",\"text\",SwitchText,
                \"txtTitle1\",\"text\",Txt1,
                \"txtTitle2\",\"text\",Txt2,
                \"txtTitle3\",\"text\",Txt3,
                \"txtTitle4\",\"text\",Txt4,
                \"txtTitle4\",\"visible\",(nr==4),
                \"t4\",\"visible\",(nr==4),
                \"cmdDpSel2\",\"visible\",type,
                \"cmdDpSel3\",\"visible\",type,
                \"cmdDpSel4\",\"visible\",(nr==4 && type==1));  

  if (nr==4)
  {
   setMultiValue(\"Text_Name\",\"visible\",true,
                 \"Text_Name\",\"text\",SwitchText,
                 \"4Text_1\",  \"visible\",true,
                 \"4Text_2\",  \"visible\",true,
                 \"4Text_3\",  \"visible\",true,
                 \"4Text_4\",  \"visible\",true,
                 \"4L1\",      \"visible\",true,
                 \"4L1q\",     \"visible\",true,
                 \"4L2\",      \"visible\",true,
                 \"4L2q\",     \"visible\",true,
                 \"4L3\",      \"visible\",true,
                 \"4L3q\",     \"visible\",true,
                 \"4L4\",      \"visible\",true,
                 \"4L4q\",     \"visible\",true,
                 \"4Dragger\", \"visible\",true,
                 \"4Dragger\", \"enabled\",true,
                 \"4Clk1\",    \"enabled\",true,
                 \"4Clk2\",    \"enabled\",true,
                 \"4Clk3\",    \"enabled\",true,
                 \"4Clk4\",    \"enabled\",true,
                 \"4Text_1\",  \"text\",Txt1,
                 \"4Text_2\",  \"text\",Txt2,
                 \"4Text_3\",  \"text\",Txt3,
                 \"4Text_4\",  \"text\",Txt4);
  }
  else
  {
   setMultiValue(\"Text_Name\",\"visible\",true,
                 \"Text_Name\",\"text\",SwitchText,
                 \"3Text_1\",  \"visible\",true,
                 \"3Text_2\",  \"visible\",true,
                 \"3Text_3\",  \"visible\",true,
                 \"3L1\",      \"visible\",true,
                 \"3L1q\",     \"visible\",true,
                 \"3L2\",      \"visible\",true,
                 \"3L3\",      \"visible\",true,
                 \"3L3q\",     \"visible\",true,
                 \"3Dragger\", \"visible\",true,
                 \"3Dragger\", \"enabled\",true,
                 \"3Clk1\",    \"enabled\",true,
                 \"3Clk2\",    \"enabled\",true,
                 \"3Clk3\",    \"enabled\",true,
                 \"3Text_1\",  \"text\",Txt1,
                 \"3Text_2\",  \"text\",Txt2,
                 \"3Text_3\",  \"text\",Txt3);

  }  
  
  
  dollars[1]=\"$s_Type\";
  dollars[2]=\"$s_SwitchText\";
  dollars[3]=\"$s_Txt1\";
  dollars[4]=\"$s_Txt2\";
  dollars[5]=\"$s_Txt3\";
  dollars[6]=\"$s_Txt4\";
  dollars[7]=\"$s_Dp1\";
  dollars[8]=\"$s_Dp2\";
  dollars[9]=\"$s_Dp3\";
  dollars[10]=\"$s_Dp4\";
  dollars[11]=\"$b_IBit\";
   
  values[1]=type;
  values[2]=strrtrim(strltrim(SwitchText));
  values[3]=strrtrim(strltrim(Txt1));
  values[4]=strrtrim(strltrim(Txt2));
  values[5]=strrtrim(strltrim(Txt3));
  values[6]=strrtrim(strltrim(Txt4));
  values[7]=strrtrim(strltrim(Dp1));
  values[8]=(type==1)?strrtrim(strltrim(Dp2)):\"\";
  values[9]=(type==1)?strrtrim(strltrim(Dp3)):\"\";
  values[10]=(type==1)?strrtrim(strltrim(Dp4)):\"\";
  values[11]=IBit;
  setDollarParams(refName, dollars, values);
} 
 
 














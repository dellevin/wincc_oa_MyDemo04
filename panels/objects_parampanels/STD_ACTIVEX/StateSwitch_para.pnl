"main() 
{ 
  int        type, nr=txtNr.text;
  //arguments of \"getDollarParams\"                          
  string     refName,Type,s;  
  bool       IBit;
  dyn_string dollars, values;        
  dyn_string color;
  
  // $-Parameter
  string SwitchText;
  string Dp1,Dp2,Dp3,Dp4,Txt1,Txt2,Txt3,Txt4;

  getMultiValue(\"rboDp\",\"number\",type,
                \"IBit\",\"state\",0,IBit,
                \"txtSwitchText\",\"text\",SwitchText,
                \"txtDp1\",\"text\",Dp1,
                \"txtDp2\",\"text\",Dp2,
                \"txtDp3\",\"text\",Dp3,
                \"txtDp4\",\"text\",Dp4,
                \"txtTitle1\",\"text\",Txt1,
                \"txtTitle2\",\"text\",Txt2,
                \"txtTitle3\",\"text\",Txt3,
                \"txtTitle4\",\"text\",Txt4);  

  dollars[1]=\"$s_Type\";
  dollars[2]=\"$s_SwitchText\";
  dollars[3]=\"$s_Txt1\";
  dollars[4]=\"$s_Txt2\";
  dollars[5]=\"$s_Txt3\";
  dollars[6]=\"$s_Txt4\";
  dollars[7]=\"$s_Dp1\";
  dollars[8]=\"$s_Dp2\";
  dollars[9]=\"$s_Dp3\";
  dollars[10]=\"$s_Dp4\";
  dollars[11]=\"$b_IBit\";
   
  values[1]=type;
  values[2]=strrtrim(strltrim(SwitchText));
  values[3]=strrtrim(strltrim(Txt1));
  values[4]=strrtrim(strltrim(Txt2));
  values[5]=strrtrim(strltrim(Txt3));
  values[6]=strrtrim(strltrim(Txt4));
  values[7]=strrtrim(strltrim(Dp1));
  values[8]=(type==1)?strrtrim(strltrim(Dp2)):\"\";
  values[9]=(type==1)?strrtrim(strltrim(Dp3)):\"\";
  values[10]=(type==1)?strrtrim(strltrim(Dp4)):\"\";
  values[11]=IBit;
           
  if ( (type==0 && Dp1==\"\") ||
       (type==1 && nr==3 && (Dp1==\"\" || Dp2==\"\" || Dp3==\"\")) ||
       (type==1 && nr==4 && (Dp1==\"\" || Dp2==\"\" || Dp3==\"\" || Dp4==\"\")) )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
      getCatStr(\"para\",\"warning\"),
      makeDynString(getCatStr(\"ax\", \"missingdp\")));
    return;
  }

  if ( (type==1 && nr==3 && (Dp1==Dp2 || Dp1==Dp3 || Dp2==Dp3)) ||
       (type==1 && nr==4 && (Dp1==Dp2 || Dp1==Dp3 || Dp1==Dp4 ||
                             Dp2==Dp3 || Dp2==Dp4 ||
                             Dp3==Dp4 )) )

  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
      getCatStr(\"para\",\"warning\"),
      makeDynString(getCatStr(\"ax\", \"doubleddp\")));
    return;
  }

  if (  type==0 &&
        dpElementType(Dp1)!=DPEL_UINT &&
        dpElementType(Dp1)!=DPEL_INT &&
        dpElementType(Dp1)!=DPEL_ULONG &&
        dpElementType(Dp1)!=DPEL_LONG &&
        dpElementType(Dp1)!=DPEL_FLOAT
     )
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
      getCatStr(\"para\",\"warning\"),
      makeDynString(getCatStr(\"ax\",\"notanalog\")));
    return;
  }

  if (type==1)
  {
    if ( ( nr==3 &&
          (dpElementType(Dp1)!=DPEL_BOOL ||
           dpElementType(Dp2)!=DPEL_BOOL ||
           dpElementType(Dp3)!=DPEL_BOOL   ) ) ||
         ( nr==4 &&
          (dpElementType(Dp1)!=DPEL_BOOL ||
           dpElementType(Dp2)!=DPEL_BOOL ||
           dpElementType(Dp3)!=DPEL_BOOL ||
           dpElementType(Dp4)!=DPEL_BOOL   ) ) )
    {
      ChildPanelOnCentralModal(\"vision/MessageWarning\",
        getCatStr(\"para\",\"warning\"),
        makeDynString(getCatStr(\"ax\",\"notdigital\")));
      return;
    }
  }

  //gets the assigned name for the reference
  getValue(\"refNameBox\", \"text\", refName);
  
  setDollarParams(refName, dollars, values);

  closeDialog();
} 

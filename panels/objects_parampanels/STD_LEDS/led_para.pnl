                \"Alert\",\"state\",0,Alert,
                \"IBit\",\"state\",0,IBit);
           
  dollars[1]=\"$s_ColOn\";
  dollars[2]=\"$s_ColOff\";
  dollars[3]=\"$dpe_value\";
  dollars[4]=\"$b_ShowAlert\";
  dollars[5]=\"$b_IBit\";
   
  values[1]=ColOn;
  values[2]=ColOff;
  values[3]=DPE;
  values[4]=Alert;
  values[5]=IBit;
           
  //gets the assigned name for the reference
   getValue(\"refNameBox\", \"text\", refName);
  
  setDollarParams(refName, dollars, values);

  closeDialog();
}


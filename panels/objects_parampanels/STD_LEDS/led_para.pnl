{
  //counter
   // $-Parameter
  dyn_string dollars,values;
  string refName;
  string ColOn;
  string ColOff;
  string DPE;
  string Alert;
  string IBit;
  
  getMultiValue(\"ColOn\",\"text\",ColOn,

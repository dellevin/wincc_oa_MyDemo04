"// Handelt es sich um einen absoluten Filenamen ?
bool  isAbsoluteFileName(string fname)
{
  if (_UNIX)
  {
    if (strpos(fname, 0, 1) == \"/\")
      return true;
    else
      return false;
  }
  else if (_WIN32)
  {
    if (strpos(fname, 0, 1) == \"/\")
      return true;
    else if (strpos(fname, 0, 1) == \"\\\\\")
      return true;
    else if (strpos(fname, 1, 1) == \":\")
      return true;
    else
      return false;
  }
  else
    return false;
}


main()
{
  string mmlib;
  string edit;
  string PAGER = \"_Pager_1\";  
  // if (strlen($PAGER) > 0) PAGER = $PAGER;


  dpGet(PAGER + \".mmlibConfigName\"+\":_online.._value\", mmlib);

  if (!isAbsoluteFileName(mmlib))
    mmlib = DATA_PATH+mmlib;
  
  edit = \"notepad \" + mmlib;
  system(edit);
}" 0

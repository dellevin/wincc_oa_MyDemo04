"main()
{
  int i;
  int rtc;
  int nrOfStati = 8;
  dyn_string stateTxt;
  string PAGER = \"_Pager_1\";   
  // if (strlen($PAGER) > 0) PAGER = $PAGER; 

  setValue(\"\", \"deleteAllLines\");
  
  rtc = setValue(\"stateTab\", \"selectByClick\", TABLE_SELECT_COLUMN);
  dpGet(PAGER + \".stateTxt:_original.._value\", stateTxt);
  
  for (i = 1; i <= nrOfStati; i++)
  {
    if (i <= dynlen(stateTxt))
    {
      setValue(\"stateTab\", \"appendLine\",
               \"#1\", i-1,    				     
               \"#2\", stateTxt[i]);
    }
    else
    {
      string text;
      switch (i-1)
      {
        case 0 :
          text = getCatStr(\"pager\",\"msgAtDrv\"); break;
        case 1 : 
          text = getCatStr(\"pager\",\"msgTransmit\"); break;
        case 2 : 
          text = getCatStr(\"pager\",\"msgOK\"); break;
        case 3 :
          text = getCatStr(\"pager\",\"noRecToReach\"); break;
        case 4 :
          text = getCatStr(\"pager\",\"noTransmit\"); break;
        case 5 : 
          text = getCatStr(\"pager\",\"break\"); break;
        case 6 :
          text = getCatStr(\"pager\",\"unknownRec\"); break;
        case 7 :
          text = getCatStr(\"pager\",\"drvDisabled\"); break;
        default :
          text = \"\"; break;
      }

      setValue(\"stateTab\", \"appendLine\",\"#1\", i-1, \"#2\", text);
    }

  }
  

"// SimpleCtrlScriptStart {invalid}
// SimpleCtrlScript {PanelOff}
// SimpleCtrlScriptEnd
main()
{
  int n;
  int cnt;
  string txt;
  
  int  retries;
  string  name;
  bool active;
  dyn_string stateTxt;

  string PAGER = \"_Pager_1\";  
  // if (strlen($PAGER) > 0) PAGER = $PAGER;

  getValue(\"stateTab\", \"lineCount\", cnt);
  for(n = 0; n < cnt; n++)
  {
    txt = \"\";
    getValue(\"stateTab\", \"cellValueRC\", n, \"#2\", txt);
 
    dynAppend(stateTxt, txt);
  }

  getValue(\"sendRetries\", \"text\", txt);
  sscanf(txt, \"%d\", retries);
   
  getValue(\"mmlibConfigFilename\", \"text\", name);
  getValue(\"active\", \"state\", 0, active);

  dpSet(PAGER + \".sendRetries:_original.._value\", retries,
        PAGER + \".mmlibConfigName:_original.._value\", name,
        PAGER + \".stateTxt:_original.._value\", stateTxt,
        PAGER + \".active:_original.._value\", active);

  setValue(\"stateTab\", \"deleteAllLines\");

  PanelOff();
}

"DragDrop" "main(string information, int dragType, int row, string column)
{
  dyn_anytype da;
  getValue(\"tabGroups\", \"getColumnN\", 0, da);

  dyn_string ds = stringToDynString(information);
  dynUnique(ds);

  unsigned uId = getUserId();
  dyn_mapping dmG = getGroupsOfUserPVSS(uId);

// check if there is root
  if ( !dynContains(da, dmG[1][\"Name\"]) && !dynContains(ds, dmG[1][\"Name\"]))
  {
    notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_notAddedOwnGroup\", NOTIFICATION_BAR_PRIO_WARNING, \"\",
                           makeDynAnytype(dmG[1][\"Name\"]), false, notificationBar_getUserDpName(), true);
    return;
  }

  for ( int i = 1; i <= dynlen(ds); i++ )
  {
    if ( !dynContains(da, ds[i]) )
      cns_addGroup(ds[i]);
  }
}

// TFS 30878: renamed to avoid conflict with system's addGroup() function
private cns_addGroup(string information)
{
  dyn_anytype daNames;
  getValue(\"tabGroups\", \"getColumnN\", 0, daNames);

  string sId = treeGroups.getText(information, 1);
  int iId = (int)sId;

  bit32 b32Perm;
  setBit(b32Perm, 0, 1);

  if ( dynlen(daNames) == 1 && daNames[1] == getCatStr(\"general\", \"all\") )
    dynClear(daNames);

  if ( !dynlen(daNames) )
    setBit(b32Perm, 1, 1);
  else
    setBit(b32Perm, 1, 0);

  if ( !dynContains(daNames, information) )
  {
    if ( cns_setViewPermission(sView, iId, b32Perm) )
      notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_permCreated\", NOTIFICATION_BAR_PRIO_INFO, \"\",
                             makeDynAnytype(information, sView), false, notificationBar_getUserDpName(), true);
  }
  else
    notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_groupAlreadyUsed\", NOTIFICATION_BAR_PRIO_WARNING, \"\",
                           makeDynAnytype(information, sView), false, notificationBar_getUserDpName(), true);
}


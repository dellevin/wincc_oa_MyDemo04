 "main(int row, string column)
{
  dyn_anytype daNames, daPerms;
  getMultiValue(\"tabGroups\", \"getColumnN\", 0, daNames,
                \"tabGroups\", \"getColumnN\", 1, daPerms);
  
  if ( row == -1 )
    return;
  
  string sName = daNames[row+1];
  string sId;
  dyn_mapping dmGroups = getAllGroupsPVSS();
  for ( int i = 1; i <= dynlen(dmGroups); i++ )
  {
    if (dmGroups[i][\"Name\"] == sName)
      sId = dmGroups[i][\"PVSSID\"];
  }
  int iId = (int)sId;
  int iAnswer;
  
  string sViewName = sView;
  if ( sViewName[strlen(sViewName)-1] == ':' )
    sViewName = substr(sViewName, 0, strlen(sViewName)-1);
  
  bit32 b32Perm = daPerms[row+1];
  
  if ( bLockedByMe )
  {
    if ( dynlen(daNames) >= 1 )
    {
      dyn_string dsEntries = makeDynString(\"PUSH_BUTTON, \" + getCatStr(\"cnsEditor\", \"cnsEditor_changePermission\") + \", 1, 1\",
                                           \"PUSH_BUTTON, \" + getCatStr(\"cnsEditor\", \"cnsEditor_deletePermission\") + \", 2, 1\");
      popupMenu(dsEntries, iAnswer);

      if ( iAnswer == 1 )
      {
        if ( cnsEditor_changeViewPermission(sViewName, iId) )
        {
          notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_permChanged\", NOTIFICATION_BAR_PRIO_INFO, \"\",
                                 makeDynAnytype(sName, sView), false, notificationBar_getUserDpName(), true);
        
          if ( getBit(b32Perm, 1) == 1 )
          {
            this.cellFillRC(row, \"icon\", \"[pattern,[fit,any,cns/read_20.png]]\");
            setBit(b32Perm, 1, 0);
          }
          else
          {
            this.cellFillRC(row, \"icon\", \"[pattern,[fit,any,cns/write_20.png]]\");
            setBit(b32Perm, 1, 1);
          }
          
          this.cellFillRC(row, \"del\", \"[pattern,[fit,any,cns/delete_20.png]]\");
          this.updateLine(1, \"id\", iId, \"perm\", b32Perm);
        }
      }
      else if ( iAnswer == 2 )
      {
        if ( cnsEditor_deleteViewPermission(sViewName, iId) )
        {
          this.deleteLineN(row);
          notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_permDeleted\", NOTIFICATION_BAR_PRIO_INFO, \"\",
                                 makeDynAnytype(sName, sView), false, notificationBar_getUserDpName(), true);
        }
      }
    }
  }
}





















//sets the View ID
void setID (string id)
{
  bool bReturn = cnsEditor_viewNameChanged(sView, id);
  
  if ( !bReturn && id != cnsSubStr(tfViewCnsPath.text, CNSSUB_VIEW) )
  {
    notificationBar_newMsg(\"cnsEditor\", \"cnsEditor_viewExist\", NOTIFICATION_BAR_PRIO_ERROR, \"\",
                           makeDynAnytype(id), false, notificationBar_getUserDpName(), true);
    tfViewId.text = cnsSubStr(tfViewCnsPath.text, CNSSUB_VIEW);
  }
  else
  {
    sView = sSystem + \".\" + id + \":\";
    tfViewCnsPath.text = sView;
    tfViewId.text = cnsSubStr(sView, CNSSUB_VIEW);
    sOldViewText = id;
  }
}

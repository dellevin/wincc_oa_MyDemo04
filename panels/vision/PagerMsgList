"// Initialisierung Tabelle

main()
{
  time start, start_2;
  time now;
  string PAGER = \"_Pager_1\";  
  string PAGER_2 = PAGER+\"_2\";

  // if (strlen($PAGER) > 0) PAGER = $PAGER;

  setValue(\"\", \"deleteAllLines\");

  now = getCurrentTime();
  dpGet(PAGER  +\".notSendTime:_original.._value\", start,
        PAGER_2+\".notSendTime:_original.._value\", start_2);
  
  fillTable((start < start_2 ? start : start_2), now); 
  
  setValue(\"msgTab\", \"selectByClick\", TABLE_SELECT_LINE);
  setValue(\"msgTab\", \"tableMode\", TABLE_SELECT_BROWSE);

  dpConnect(\"addNewMsgCB\", false, 
  	PAGER+\".statusMessage.index:_original.._value\"   ,
  	PAGER+\".statusMessage.receiver:_original.._value\",
  	PAGER+\".statusMessage.provider:_original.._value\",
  	PAGER+\".statusMessage.message:_original.._value\" ,
  	PAGER+\".statusMessage.sendTime:_original.._value\",
  	PAGER+\".statusMessage.state:_offline.._value\"   );

  dpConnect(\"updMsgStateCB\", false, PAGER+\".sendState:_original.._value\");

}


updMsgStateCB(string dp, dyn_int state)
{
  string txt, key;

  sprintf(key, \"pager%d\", state[2]);

  txt = getCatStr(\"general\", key);
  
  setValue(\"msgTab\", \"lineParam\", 1,  
           \"key\"   , state[1], 
           \"value\" , makeDynString(\"#1\"), makeDynString(txt));
              
  setLineColor(state[1], state[2]);
}


addNewMsgCB(
  string dp1, int idx, 
  string dp2, string recv,
  string dp3, string prov,
  string dp4, string msg,
  string dp5, time   sndT,
  string dp6, int    state)
{
  int i, k;
  string key;
  
  sprintf(key, \"pager%d\", state);  // key in general.cat

  getValue(\"\", \"lineCount\", i);
  for (; i--; )
  {
    getValue(\"\", \"cellValueRC\", i, \"key\", k);
    if (k == idx)
      return;
  }
    
  setValue(\"msgTab\", \"appendLine\", 
  	     \"key\", idx, 
             \"#1\", getCatStr(\"general\", key), 
             \"#2\", formatTime(\"%c\", sndT), 
             \"#3\", \" \", 
             \"#4\", recv, 
             \"#5\", prov, 
             \"#6\", msg);      

  setLineColor(idx, state);
}

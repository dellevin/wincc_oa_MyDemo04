"main() 
{ 
  int auftragsNr; 
  int tabLen;   
  dyn_int sel;
  int cur;
  string PAGER = \"_Pager_1\";  
  // if (strlen($PAGER) > 0) PAGER = $PAGER;
  
  getMultiValue(\"msgTab\"  ,\"getSelectedLines\", sel,
                \"msgTab\"  ,\"lineCount\", tabLen);
  cur = -1;
  if (dynlen(sel) > 0) cur = sel[1];
    
  if ((tabLen == 0) || (cur == -1)) return;

  getValue(\"msgTab\", \"cellValueRC\" , cur,\"key\", auftragsNr);

  dpSet(PAGER+\".abortMessage:_original.._value\", auftragsNr);

"main()
{
  string serviceName, name;
  string PAGER = \"_Pager_1\";  
  // if (strlen($PAGER) > 0) PAGER = $PAGER;

  getValue(\"mmlibConfigFilename\", \"text\", serviceName);

  if ( serviceName == \"\" ) return;
  
  if (substr(serviceName, 0, 1) != \"/\"   &&
      substr(serviceName, 0, 1) != \"\\\\\"  &&
      substr(serviceName, 1, 1) != \":\")
    name = getPath(DATA_REL_PATH, serviceName);

  // if the file was not found, create an empty file
  if ( name == \"\" )
  {
    file fd;
    
    name = getPath(DATA_REL_PATH) + serviceName;
    fd = fopen(name, \"w\");
    fclose(fd);
  }

  system(\"notepad \" + name);
}" 0

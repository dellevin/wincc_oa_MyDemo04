// ============================================================================
// Function:  valuesOK()
// ============================================================================
bool valuesOK(dyn_string dsDpeString, dyn_string dsValuesOn, dyn_string dsValuesOff, dyn_string dsValues)
{
  int          n, x;
  bool         b = TRUE;

  if (dynlen(dsDpeString) > 0)
  {
    if (p_bSingleOrOnOff)      // TRUE if on/off
    {
      for (n = 1; n <= dynlen(dsValuesOn); n++)
      {
        if (dsValuesOn[n] == \"\") x++;
      }

      for (n = 1; n <= dynlen(dsValuesOff); n++)
      {
        if (dsValuesOff[n] == \"\") x++;
      }
//DebugTN(\"dsValuesOff\",dsValuesOff);
//DebugTN(\"dsValuesOn\",dsValuesOn);
//DebugTN(\"x range\",x);
      //if (x > 0 && chkDPE.state(0) == TRUE) b = FALSE;
      if (x > 0) b = FALSE;
    }
    else
    {
      for (n = 1; n <= dynlen(dsValues); n++)
      {
        if (dsValues[n] == \"\") x++;
      }
//DebugTN(\"dsValues\",dsValues);
//DebugTN(\"x single\",x);
      //if (x > 0 && chkDPE.state(0) == TRUE) b = FALSE;
      if (x > 0) b = FALSE;
    }
  }
  return b;
}

// ============================================================================
// Function:  edit()
// ============================================================================
edit()
{
  string    dpe_string;
  dyn_int   line;

  getValue(\"tabDPE\",\"getSelectedLines\",line);

  if(dynlen(line)!= 0)
  {
    //Get the line for editing
    tabDPE.currentCell(p_iRow, 3);
    dpe_string = tabDPE.cellValue();

    sc_dpeAndValues(dpe_string, \"txtDPEsetList\", \"txtDPEonValue\", FALSE, \"txtDPEoffValue\");
    sc_dpeAndValues(dpe_string, \"txtDPEsetList\", \"txtDPEValue\", FALSE);

    tabDPE.currentCell(p_iRow, 1);
    setValue(\"txtDPEonValue\", \"text\", tabDPE.cellValue());
    tabDPE.currentCell(p_iRow, 2);
    setValue(\"txtDPEoffValue\", \"text\", tabDPE.cellValue());
    tabDPE.currentCell(p_iRow, 4);
    setValue(\"txtDPEValue\", \"text\", tabDPE.cellValue());

    p_sSEdit = dpe_string;

    last_dpe.text = p_sSEdit;

   //cmdInsLINE.visible(FALSE);
    cmdApply.visible(TRUE);
  }
  else
  {
    if (dynlen(line)>0)
    {
      if (line[1] < tabDPE.lineCount()- 1){}
    }
    else
    {
      string sMessageText=getCatStr(\"para\",\"nolineselected\");
      ChildPanelOnCentralModal(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText));
      return;
    }
  }
}
// ============================================================================
// Function:  sc_insApply() -  Bool check
// ============================================================================
sc_insApply(int mode)
{
  int        i,type,o,u,x;
  string     dp1,dpe,dpe2,dpe3,dpe4,dpe5,
             sDPEonValue,
             sDPEoffValue,
             sDPEValue;
  dyn_string dpes;

  getMultiValue  (\"txtDPEsetList\",  \"text\",  dpe,
                  \"txtDPEonValue\",  \"text\",  sDPEonValue,
                  \"txtDPEoffValue\", \"text\",  sDPEoffValue,
                  \"txtDPEValue\",    \"text\",  sDPEValue);

  if (dpe == \"\") // If no DPE ist selected
  {
    string sMessageText=getCatStr(\"dpeMonitor\",\"dpeMon_add_item\");
    ChildPanelOnCentralModal(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText));
    return;
  }

  //This part checks and adds a datapoint element.
  if (dpExists(dpe)) // Check if DPE exists
  {
    if (dpTypeName(dpe) == \"_Sc\")// _Sc type is only \"mode\" valid!
    {
      dpe  = dpSubStr(dpe,DPSUB_SYS_DP_EL);
      dpe2 = dpSubStr(dpe,DPSUB_SYS_DP);
      dpe3 = dpe;
      strreplace(dpe3, dpe2, \"\");

      if(dpe3 != \".mode\" || dpSubStr(dpe2,DPSUB_SYS_DP) == dpSubStr(p_sName,DPSUB_SYS_DP))  //if(dpe3 != \".mode\")
      {
        string sMessageText = getCatStr(\"scheduler\",\"invalidEntry\");
        ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
        txtDPEsetList.text  = \"\";
        txtDPEValue.text    = \"\";
        txtDPEonValue.text  = \"\";
        txtDPEoffValue.text = \"\";
        return;
      }
    }

    if (dpTypeName(dpe) == \"_ScCom\")// _ScCom type is not allowed!
    {
      string sMessageText=getCatStr(\"ac\",\"invalidname\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      txtDPEsetList.text  = \"\";
      txtDPEValue.text    = \"\";
      txtDPEonValue.text  = \"\";
      txtDPEoffValue.text = \"\";
      return;
    }
  }
  else
    return;

  // Question if both values should be ident
  if(sDPEonValue == sDPEoffValue)
  {
    if(p_bSingleOrOnOff == TRUE)
    {
      bool b = sc_2x(\"txtDPEonValue\",\"txtDPEoffValue\");
//DebugTN(\"b\",b);
      if (b == FALSE)
        return;
    }
  }

  // Check the datatype and returns if not correct
  sc_getDpeType(dpe,type);

  if(type<0)
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),
                             makeDynString(getCatStr(\"scheduler\",\"novalidDPE\")));
    return;
  }
  if (type == 0)
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),
                             makeDynString(getCatStr(\"scheduler\",\"invalidDataType\")));
    return;
  }

  //Duplicates
  for(i=0; i<=tabDPE.lineCount()-1; i++) //Get current DPEs(dpes)
  {
    tabDPE.currentCell(i,3); //\"dpeString\");
    dynAppend(dpes,dpSubStr(tabDPE.cellValue(), DPSUB_SYS_DP_EL));
  }
  //dp1 = dpSubStr(dpe, DPSUB_SYS_DP_EL);

  getValue(\"tabDPE\",\"lineRangeVisible\", o, u);
  x = tabDPE.lineCount()-1;

  //Edit - apply
  if(mode == 0)
  {
    //If someone checks \"Exclusive system name\", add mySysName for complete dpe string
    if (dpSubStr(dpe, DPSUB_SYS) == \"\")
      dpe4 = getSystemName()+dpe;
    else
      dpe4 = dpe;

    sc_checkSys(dpe, dpe);
    tabDPE.updateLine(1, \"dpeString\",  p_sSEdit,
                         \"datapoints\", dpe,
                         \"on_value\",   sDPEonValue,
                         \"off_value\",  sDPEoffValue,
                         \"value\",      sDPEValue);
    tabDPE.updateLine(4, \"datapoints\", dpe,
                         \"on_value\",   sDPEonValue,
                         \"off_value\",  sDPEoffValue,
                         \"value\",      sDPEValue,
                         \"dpeString\",  dpe4);


    //Place cursor
    if(o<=x && x<=u)
      tabDPE.selectLineN(p_diLine[1]); //tabDPE.lineCount()-1);
    else
    {
      if ( p_diLine[1] >= o )
      {
        tabDPE.lineVisible(o);
        tabDPE.selectLineN(p_diLine[1]);
      }
      else
      {
        tabDPE.lineVisible(p_diLine[1]);
        tabDPE.selectLineN(p_diLine[1]);
      }
    }
  }

  //New - insert
  if(mode == 1)
  {
    if(!dynContains(dpes,dpSubStr(dpe, DPSUB_SYS_DP_EL))) //&& !dynContains(dpes,dpe)) // Check if duplicate
    {
      sc_checkSys(p_sS, p_sS);
      tabDPE.appendLine(\"datapoints\", p_sS, \"on_value\", sDPEonValue, \"off_value\", sDPEoffValue, \"dpeString\", dpe ,\"value\", sDPEValue);
    }
    else
    {
      string sMessageText=getCatStr(\"dpeMonitor\",\"dpeMon_dpe_dublicate\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",
                           getCatStr(\"general\",\"warning\"),
                           makeDynString(sMessageText));
      return;
    }
    //Place cursor
    if(o<=x && x<=u)
      tabDPE.selectLineN(tabDPE.lineCount()-1);
    else
    {
      tabDPE.lineVisible(tabDPE.lineCount()-1);
      tabDPE.selectLineN(tabDPE.lineCount()-1);
    }
  }

  //Reset fields
  txtDPEoffValue.format(\"%s\");  txtDPEoffValue.text = \"\";
  txtDPEonValue.format(\"%s\");   txtDPEonValue.text = \"\";
  txtDPEValue.format(\"%s\");     txtDPEValue.text = \"\";
  txtDPEsetList.text = \"\";
  txtDataType.text = \"\";

  if (mode == 0)
    this.visible(FALSE);
}

// ============================================================================
// Function:  sc_rcp() -  This fills the recipe combo box with data
// ============================================================================
sc_rcp(dyn_string &rcps)
{
  //This shows the values of the selection of a recipe type.
  string     rct;
  dyn_string ds;

  getValue(\"\",\"text\",rct);

  if (rct!=\"\")
    dpGet(rct+\".recipes:_online.._value\",  rcps);
  else
    dynClear(rcps);
}
// ============================================================================
// Function:  sc_rct() -  This fills the recipe combo box with data
// ============================================================================
sc_rct(dyn_string &rcts)
{
  int      i, e;

  //TEST: Get all user types
  //dynAppend(p_dsSysNames, \"System1\");
  //dynAppend(p_dsSysNames, \"System1\");

  //Get all recipe datapoints - also from the dist systems
  for (i=1; i<=dynlen(p_dsSysNames); i++)
  {
    dynAppend(rcts, dpNames(p_dsSysNames[i]+\":*\",\"_Rct\"));
  }

  //Entsprechend formatieren
  for(e=1;e<=dynlen(rcts);e++)
  {
    if(dynlen(p_dsSysNames) >= 2)
      rcts[e]=dpSubStr(rcts[e],DPSUB_SYS_DP_EL);
    else
      rcts[e]=dpSubStr(rcts[e],DPSUB_DP_EL);
  }
}

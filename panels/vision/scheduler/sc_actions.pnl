"main()
{
  string       sDpeList, sValueOn, sValueOff, sDpeString, sValues, sPopupText, sAlarmText;
  dyn_string   ds, dsDpeList, dsValuesOn, dsValuesOff, dsDpeString, dsValues;
  dyn_float    df;
  dyn_anytype  daTab;
  int          n, x, y;
  bool         b;
  
  for(n = 0; n < tabDPE.lineCount; n++)
  {
    daTab = tabDPE.getLineN(n);
    dynAppend(dsDpeList, daTab[1]);
    dynAppend(dsValuesOn, daTab[2]);
    dynAppend(dsValuesOff, daTab[3]);
    dynAppend(dsDpeString, daTab[4]);
    dynAppend(dsValues, daTab[5]);
  }

  if (valuesOK(dsDpeString, dsValuesOn, dsValuesOff, dsValues) == FALSE) 
  {
    string sMessageText=getCatStr(\"scheduler\",\"m56\");
    ChildPanelOnCentralModal(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText));
    return;
  }

  // Convert dyn_strings into strings
  sDpeList = dsDpeList;
  sValueOn = dsValuesOn;
  sValueOff = dsValuesOff;
  sDpeString = dsDpeString;
  sValues = dsValues;
  
  // Convert language strings into strings 
  ls2s(p_lsPopUpText, sPopupText);
  ls2s(p_lsAlarmText, sAlarmText);
  
  ds[1] = txtStartScript.text;
  ds[2] = sAlarmText;
  ds[3] = sPopupText;
  ds[4] = sDpeList;
  if (p_bSingleOrOnOff)      // TRUE if on/off
  {
    ds[5] = sValueOn;
    ds[6] = sValueOff;
    ds[7] = \"\"; // Popup name - obsolet
    ds[8] = cbRecipeTypeOn.text;
    ds[9] = cbRecipeNameOn.text;
    ds[10] = cbRecipeTypeOff.text;
    ds[11] = cbRecipeNameOff.text;
    
    for (n = 23; n < 26; n++)
      ds[n] = \"\";
  }
  else
  {
    for (n = 5; n < 12; n++)
      ds[n] = \"\";
    ds[23] = sValues;
    ds[24] = cbRecipeType.text;
    ds[25] = cbRecipeName.text;
  }
  ds[12] = chkDPE.state(0);
  ds[13] = chkRecipe.state(0);
  ds[14] = chkPopUp.state(0);
  ds[15] = chkReminderAlarm.state(0);
  ds[16] = chkScript.state(0);
  ds[17] = sDpeString;
  ds[18] = p_sPopUpMode;
  ds[19] = p_sPopUpDate;
  ds[20] = \"\"; // Group - obsolete
  ds[21] = p_sUsers;
  ds[22] = p_sUis;
  ds[26] = txtStartScriptON.text;
  ds[27] = txtStartScriptOFF.text;
  
  df[1] = 1; // Save changes
  
  PanelOffReturn(df,ds);  
}" 0

  // mt: write the times of displayed EWO-Bars into database
  sc_saveBars(getShape(\"Scheduler_ewo1\"), p_sName);
  
  // Write the values into the database
  sc_writeProgram(p_sName);
  
  p_bCancel = FALSE; //APPLY was done
  on.enabled(TRUE);
  off.enabled(TRUE);

  dynClear(p_dsPDC);
  dynClear(p_ddaPDC);
  sc_getObjectContent(p_dsPDC, p_ddaPDC);
    

"main()
{
  if ( Scheduler_ewo1.selectedRange == -1 )
  {
    cmdDeleteBar.enabled( false );
    return;
  }
  sc_deleteSelectedBar();
  
  spToH.text = \"0\";  spToM.text = \"0\";  spToS.text = \"0\";  
  spFromH.text = \"0\";  spFromM.text = \"0\";  spFromS.text = \"0\";
  setSpinbuttonsAttrib( \"enabled\", false );
  sc_delButtons( getShape ( Scheduler_ewo1.name ) );
}

sc_deleteSelectedBar()
{
  int selectedDay = Scheduler_ewo1.getRangeDay ( Scheduler_ewo1.selectedRange );

  if (Scheduler_ewo1.numRanges == 1)
    cmdDeleteAllBars.enabled(false);
  
  if ( p_sType == \"D\" )  // if in Day-Mode, refresh all days
  {
    Scheduler_ewo1.removeRange( Scheduler_ewo1.selectedRange );
    sc_otherDaysAreSelectedChangeAndDisplayThem ( getShape( Scheduler_ewo1.name) , selectedDay );
  }
  else  // in Week-Mode, uncheck the daycheckbox, if the day is empty
  {
    bool existingOtherBarsOnDay = false;
    int selectedRange = Scheduler_ewo1.selectedRange;
    for( int i = Scheduler_ewo1.numRanges-1 ; i >= 0; i-- )  // look if there is any other bar on that day
    {
      if( Scheduler_ewo1.getRangeDay(i) == selectedDay && selectedRange != i )
      {
        existingOtherBarsOnDay = true;
        break;
      }
    }
    
    Scheduler_ewo1.removeRange( selectedRange );        
    
    if ( !existingOtherBarsOnDay )  // uncheck the day-checkbox, if there are no more bars for that day
      setValue ( \"chk\"+selectedDay, \"state\", 0, false );    
  }

"main()
{
  int             i, iBars = 0;
  bool            ret;
  dyn_float       df;
  dyn_string      ds, ods;
  dyn_dyn_anytype odda;
  string          sName;
  
  dpSet(p_sName + \".name\", p_lsName);
  
  ChildPanelOnCentralModalReturn(\"vision/scheduler/sc_overview.pnl\", 
                                 getCatStr(KAT, \"m14\"), 
                                 makeDynString(\"$Type:\" + p_sType), 
                                 df, ds);

  if (dynlen(ds) > 0)
  {
    sName = ds[1];
    
    //If loading name is current schedule - do nothing!   
    sc_checkSys(p_sName, p_sName);
    sc_checkSys(sName, sName);
    
    if (p_sName == sName)
      return;
    
    //Get current objec data
    sc_getObjectContent(ods, odda);

    if (p_dsPDC != ods || p_ddaPDC != odda)
    {   
      // Ask for saving
      ChildPanelOnCentralModalReturn(\"vision/MessageInfo\", getCatStr(KAT, \"m15\"), 
                                     makeDynString(\"$1:\" + getCatStr(KAT, \"m5\"), 
                                     \"$2:\" + getCatStr(\"general\", \"yes\"),
                                     \"$3:\" + getCatStr(\"general\", \"no\")),
                                     df, ds);
      // Save the current plan
      if (df[1] == YES)
      {
        // Save into the global variables
        sc_saveDayPlan(getShape(\"Scheduler_ewo1\"), ret);
      
        if(ret != FALSE)
        {
          //Bar count
          for (i = 1; i <= 7; i++)
          {
            if ( Scheduler_ewo1.numRanges ( i ) > 0)
              iBars ++;
          }
          
          //Empty or not
          if (dynlen(p_diTime) == 0 || iBars == 0)
          {
            sc_noTimes2(ds,df);
            return;
          }
          else
          {
            sc_writeProgram(p_sName); 
            sc_saveBars ( getShape ( \"Scheduler_ewo1\" ), p_sName );
            p_bCancel = FALSE; //APPLY was done 
          }
        }
      }
      else
      {
        if (dynlen(p_diTime) == 0)
        {
          sc_noTimes2(ds,df);
          return;
        }
      }
    }
    
    // Load the new one
    //dpDisconnect(\"CBresult\", p_sName + \".result\"); //MH: Not needed any more because 
    //                                                     txtFieldTimeCondiFulfilled not used - 280103
    p_sName = sName;
    sc_deleteAllBars ( getShape ( \"Scheduler_ewo1\" ), -1 );
    sc_displayBars ( getShape ( \"Scheduler_ewo1\" ) , p_sName);
    
    // Common visibility
    buttonV.visible(TRUE);
  
    sc_eventInitDayWeekInclMotif(FALSE, p_sType);
  
    if (p_bCancel == TRUE)
    {
      on.enabled(FALSE);
      off.enabled(FALSE);
    }
    
    dynClear(p_dsPDC);
    dynClear(p_ddaPDC);
    sc_getObjectContent(p_dsPDC, p_ddaPDC);
    
    // Common visibility
    buttonV.visible(FALSE);
  }
}" 0

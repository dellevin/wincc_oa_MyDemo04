    dynClear(ds);
    dynAppend(ds, TRUE); //p_bCancel);  // TRUE means NO saving
    dynAppend(ds, p_sNewDP);
    PanelOffReturn(df, ds);
  }
  else
  {
    // Write the values into the database
    sc_writeProgram(p_sName);  
  
    PanelOff();  
  }     

}


  bool ret;
  dyn_float  df;
  dyn_string ds;
  string dp=\"1\";
  
  //write the times of displayed EWO-Bars into database
  sc_saveBars(getShape(\"Scheduler_ewo1\"), p_sName);
  
  bool b = sc_checkValidFromUntilEntryDW();
  if (b == FALSE)
    return;
 
  // Save into the global variables
  sc_saveDayPlan(getShape(Scheduler_ewo1.name), ret);


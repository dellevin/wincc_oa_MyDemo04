// ============================================================================
// Function: sc_saveDayPlan - Translate the Ewo and other 
//                            elements into global variables
// ============================================================================
sc_saveDayPlan(shape SchedulerEwo, bool &ret)
{
  time    barTimeON, barTimeOFF;
  int     i, e, nIndex, iSpecDay;

  bool    b;
  
  //--- Active days and times
  // delete old variables
  dynClear(p_diTime);
  dynClear(p_diWeekDay);


  // Save occupied days
  bool checked;
  for (i = 1; i < 8; i++)
  {
    getValue(\"chk\"+i, \"state\", 0, checked);
    if(checked)
      dynAppend(p_diWeekDay, i);
  }

  if (dynlen(p_diWeekDay) > 0)
  {
    nIndex = p_diWeekDay[1];
    bool SaveOnlyOneDay = FALSE;
    int firstDay;
    if ( p_sType == \"D\" )
      SaveOnlyOneDay = TRUE;
    
    for (i = 0; i < SchedulerEwo.numRanges; i++)
    {
      if ( i == 0 )
        firstDay = SchedulerEwo.getRangeDay ( i );
      else if ( SaveOnlyOneDay && firstDay != SchedulerEwo.getRangeDay ( i ) )  // save only one day if running in Day-Mode
        break;
        
      dynAppend(p_diTime, period(SchedulerEwo.getRangeStart(i)));
      dynAppend(p_diTime, period(SchedulerEwo.getRangeEnd(i)));
    }
  }


  //-- Additional value condition
  p_sVcDpe    = dpSubStr(cbDPE1.text, DPSUB_SYS_DP_EL);   // Additional value condtition - DPE
  
  if (txtDataType.text() == \"STRING\")                       // Additional value condition - value
    p_sVcValue = \"\\\"\"+txtAddValue.text+\"\\\"\";                        
  else
    p_sVcValue = txtAddValue.text;                       
    
  p_iVcRelOP = cbRelOp.selectedPos;                       // Additional value condition - operator
  
  p_bMode = (radCurrentState.number == 0);                // Activated/Deactivated
    
  //-- Special days list
  p_iSpecDayMode = radSpecialDayMode.number;      
  iSpecDay = cboSelectSpecDayList.selectedPos; 
   
  if (iSpecDay == 1)
  {
    for (i = 0; i < getNoOfLangs(); i++)
      setLangString(p_lsSpecDay, i, \"\");
  }
  else
    p_lsSpecDay = p_dlsSpecDayList[iSpecDay - 1];

  //-- Valid from - valid until
  p_tValidFrom = validFrom.text;
  p_tOldVF = p_tValidFrom;
  p_tValidUntil = validUntil.text;
  p_tOldVU = p_tValidUntil;
  sc_p_tValidFrom_p_tValidUntil(i);
  if (i != 0)
  {
    ret = FALSE;
    return;
  }
    
  p_iSyncWeekDay = -1;
  p_iSyncDay = -1;
  p_iSyncMonth = -1;
    
  ret = TRUE;
}


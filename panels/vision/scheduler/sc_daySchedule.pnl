    tmpTimeTo = period( this.getRangeEnd ( idx ) );  
    bool state;
    string checkbox=\"chk\"+this.getRangeDay ( idx );

    getValue( checkbox, \"state\", 0, state);
    sc_delButtons( getShape(this.name) );

    if ( !state && this.numRanges > 0 )  //if the checkbox of the day isn't selected and the week allready contais data
    {
      if ( this.numRanges == 1 || p_sType == \"W\" )  // the first created bar selects the Checkbox of the day
      {
        int theDay = this.getRangeDay ( idx );
        setValue ( \"chk\"+theDay, \"state\", 0, true );
      }
      else
      {
        this.removeRange (idx);  //remove the drawn Range and exit
        return;
      }
    }
    
    setSpinbuttonsAttrib( \"visible\", true );
    setSpinbuttonsAttrib( \"enabled\", true );    

    spFromH.text = tmpTimeFrom / 3600;
    spFromM.text = (tmpTimeFrom / 60) % 60;
    spFromS.text = tmpTimeFrom % 60;
 
    spToH.text = tmpTimeTo / 3600;
    spToM.text = (tmpTimeTo / 60) % 60;
    spToS.text = tmpTimeTo % 60;      
    
    setValue( checkbox, \"state\", 0, true );

    if ( p_sType == \"D\" )  //if the scheduler is started in day-mode, every day has to be changed
      sc_otherDaysAreSelectedChangeAndDisplayThem( getShape(this.name), this.getRangeDay(idx) );
  }
  else
  {
    sc_delButtons( getShape(this.name) );
    setSpinbuttonsAttrib( \"enabled\", false );
    return;
  }"

  bool b;
  b = sc_checkValidFromUntilEntryDW();

  if (b == FALSE)
    return;
  
  // Save into the global variables

  sc_saveDayPlan(getShape(\"Scheduler_ewo1\"), ret);
  if(ret == FALSE)
  {

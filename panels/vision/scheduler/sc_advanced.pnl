"main()
{
  int        i,
             o, u, x;
  string     s,
             sOld,
             sNew;
  dyn_string dsTab;
  
  if (txtFieldTime.text == \"\")
   return;
  
  //Get new entry
  s = p_tTime;
  sNew = s+\" | \"+txtFieldTime.text;
  
  //Get current entries
  for(i=0; i<=tbTimes.lineCount()-1; i++) 
  {
    sOld = tbTimes.getLineN(i);
    dynAppend(dsTab, sOld);
  }

  //Check if duplicate entries
  if (!dynContains(dsTab,sNew))
  {
    //Append table with entrys
    tbTimes.appendLine(\"timesOrig\", p_tTime,\"times\", txtFieldTime.text);
  }                        
  else
  {
    string sMessageText=getCatStr(\"scheduler\",\"sc_duplicateEntry\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",
                         getCatStr(\"general\",\"warning\"),
                         makeDynString(sMessageText));
    return;
  }      
  
  //Place cursor
  getValue(\"tbTimes\",\"lineRangeVisible\", o, u);
  x = tbTimes.lineCount()-1;
  
  if(o<=x && x<=u)
    tbTimes.selectLineN(tbTimes.lineCount()-1);
  else
  {
    tbTimes.lineVisible() = tbTimes.lineCount()-1;
    tbTimes.selectLineN(tbTimes.lineCount()-1);
  }
  
  //Reset entry fields
  p_tTime = 0;
  txtFieldTime.text = \"\";
}

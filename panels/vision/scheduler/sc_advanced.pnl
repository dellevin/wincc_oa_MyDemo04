  ds[9]  =  cboSyncDay.selectedPos-1;
  ds[10] =  cboSyncMonth.selectedPos-1;
  ds[11] =  txtFieldIntervall.text;
  ds[12] =  txtFieldDelay.text;
  ds[13] =  CheckButton1.state(0);

  df[1] = 1;

  PanelOffReturn(df,ds);  
}" 0

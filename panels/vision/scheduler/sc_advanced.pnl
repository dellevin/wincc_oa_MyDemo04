"main()
{
  int         i;
  dyn_string  dsWd,
              dsM,
              dsTimes;
  dyn_int     diWd,
              diMd,
              diM;              
  dyn_time    dtTimes;
  
  // Common visibility
  buttonV.visible(TRUE);
            
  //Validity
  p_tValidFromReal = (isDollarDefined(\"$validFrom\")? getDollarValue(\"$validFrom\") : 0);
  txtFieldValidFrom.text(formatTime(getCatStr(KAT, \"m7\"), p_tValidFromReal)); 
  
  p_tValidUntilReal = (isDollarDefined(\"$validUntil\")? getDollarValue(\"$validUntil\") : 0);
  txtFieldValidUntil.text(formatTime(getCatStr(KAT, \"m7\"), p_tValidUntilReal));
  
  //Times
  isDollarDefined(\"$Times\")? dsTimes = strsplit(getDollarValue(\"$Times\"), \"|\") : \"\";
  if (dynlen(dsTimes) >= 1)
  {
    for (i = 1; i <= dynlen(dsTimes); i++)
    {
      dsTimes[i] = strltrim(dsTimes[i]);
      dsTimes[i] = strrtrim(dsTimes[i]);
    }
    dtTimes = dsTimes;
    for (i=1; i<=dynlen(dtTimes); i++)
      tbTimes.appendLine(\"timesOrig\", dtTimes[i],\"times\",formatTime(\"%H:%M:%S\",dtTimes[i]));
  }
  //Weekday 
  diWd = (isDollarDefined(\"$WeekDay\")? strsplit(getDollarValue(\"$WeekDay\"), \"|\") : \"\");
  for (i=1; i<=dynlen(diWd); i++)
  {
    getValue (\"cboWeekDay\", \"items\", dsWd);
    tbWeekday.appendLine(\"weekdayOrig\", diWd[i], \"weekday\", dsWd[diWd[i]]);
  }
  //Monthday 
  diMd = (isDollarDefined(\"$MonthDay\")? strsplit(getDollarValue(\"$MonthDay\"), \"|\") : \"\");
  for (i=1; i<=dynlen(diMd); i++)
    tbDay.appendLine(\"monthday\", diMd[i]);
  //Month  
  diM = (isDollarDefined(\"$Month\")? strsplit(getDollarValue(\"$Month\"), \"|\") : \"\");
  for (i=1; i<=dynlen(diM); i++)
  {
    getValue (\"cboMonth\", \"items\", dsM);
    tbMonth.appendLine(\"monthOrig\", diM[i], \"month\", dsM[diM[i]]);
  }

  //Interval Parametrization
  //SyncTime
  p_tSyncTime = (isDollarDefined(\"$SyncTime\")? getDollarValue(\"$SyncTime\") : 0);
  if (period(p_tSyncTime) == 0)
    txtFieldSyncTime.text = \"\";
  else
    txtFieldSyncTime.text = formatTime(\"%H:%M:%S\",p_tSyncTime);
    
  //SyncTimeActive
  CheckButton1.state(0) = (isDollarDefined(\"$SyncTimeActive\")? getDollarValue(\"$SyncTimeActive\") : FALSE);
  ignoreSyncTime(CheckButton1.state(0));
  
  //SyncWeekday
  i = (isDollarDefined(\"$SyncWeekDay\")? getDollarValue(\"$SyncWeekDay\") : 0);

  if(i==0 || i == -1)
    cboSyncWeekday.selectedPos(1); 
  else
    cboSyncWeekday.selectedPos(i+1);
  //SyncDay  
  i = (isDollarDefined(\"$SyncDay\")? getDollarValue(\"$SyncDay\") : 0);

  if(i==0 || i == -1)
    cboSyncDay.selectedPos(1); 
  else
    cboSyncDay.selectedPos(i+1);
  //SyncMonth  
  i = (isDollarDefined(\"$SyncMonth\")? getDollarValue(\"$SyncMonth\") : 0);

  if(i==0 || i == -1)
    cboSyncMonth.selectedPos(1); 
  else
    cboSyncMonth.selectedPos(i+1);
    
  //Interval
  txtFieldIntervall.text = (isDollarDefined(\"$Interval\")? getDollarValue(\"$Interval\") : \"\");
  //Delay
  txtFieldDelay.text = (isDollarDefined(\"$Delay\")? getDollarValue(\"$Delay\") : \"\");
  
  // Common visibility
  buttonV.visible(FALSE);
}

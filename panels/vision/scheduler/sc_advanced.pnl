  for(i=0; i<=tbDay.lineCount-1; i++)
  {
    tbDay.currentCell(i,0);
    dynAppend(dsDay, tbDay.cellValue());
  }  
  md = dsDay;
  
  //Get all month entrys
  for(i=0; i<=tbMonth.lineCount-1; i++)
  {
    tbMonth.currentCell(i,0);
    dynAppend(dsMonth, tbMonth.cellValue());
  }
  m = dsMonth;

  ds[1]  =  p_tValidFromReal;
  ds[2]  =  p_tValidUntilReal;

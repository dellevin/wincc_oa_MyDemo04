"main()
{
  int         i;
  string      times,
              wd,
              md,
              m;
  dyn_string  dsTimes, 
              dsWeekday,
              dsDay,
              dsMonth,
              ds;
  dyn_float   df;
  
  // Check valid from and valid until
  if (period (p_tValidFromReal) > period (p_tValidUntilReal))
  {
    sc_illegalValidityPeriod();
    txtFieldValidFrom.text(formatTime(getCatStr(KAT, \"m7\"), p_tOldVF));  
    p_tValidFromReal = p_tOldVF;
    
    if (period (p_tValidFromReal) > period (p_tValidUntilReal))
    {
      txtFieldValidUntil.text(formatTime(getCatStr(KAT, \"m7\"), p_tOldVU));
      p_tValidUntilReal = p_tOldVU;
    }
    return;
  }
  
  //Get all times entrys
  for(i=0; i<=tbTimes.lineCount-1; i++)
  {
    tbTimes.currentCell(i,0);
    dynAppend(dsTimes, tbTimes.cellValue());
  }
  times = dsTimes;
  

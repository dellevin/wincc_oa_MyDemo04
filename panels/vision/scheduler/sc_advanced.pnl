ignoreSyncTime(bool state)
{
  if (state == TRUE)
  {
    buSyncTime.enabled(FALSE);
    cboSyncWeekday.enabled(FALSE);
    cboSyncDay.enabled(FALSE);
    cboSyncMonth.enabled(FALSE);
    txtFieldSyncTime.enabled(FALSE);
  }
  else
  {
    buSyncTime.enabled(TRUE);
    cboSyncWeekday.enabled(TRUE);
    cboSyncDay.enabled(TRUE);
    cboSyncMonth.enabled(TRUE);
    txtFieldSyncTime.enabled(TRUE);
  }  
}
            

"main()
{
  char       c = \"|\";
  string     shapeName, title;
  dyn_string header, footer;
  
  switch (tabMain.activeRegister())
  {
    case SCHED_LIST:          // schedulers
      shapeName = \"tabSchedules\";
      title     = getCatStr(\"scheduler\", \"p1\");
      break;
    case LOG:         // action log
      shapeName = \"tab1\";
      title     = getCatStr(\"scheduler\", \"p3\");
      break;
    case DEPENDENCIES:        // dependencies
      shapeName = \"tab2\";
      title     = getCatStr(\"scheduler\", \"p2\");
      break;
    default:
      return;
  } 

  //Printing
  header[1] = \"\\\\left{\"+getCatStr(\"scheduler\", \"p4\")+\"\"+title+\"} \\\\right{\"+getCatStr(\"scheduler\", \"p5\")+\"\\\\user}\";
  header[2] = \"\";
  footer[1] = \"\";
  footer[2] = \"\\\\left{\\\\date / \\\\time} \\\\center{WinCC OA Scheduler} \\\\right{\"+getCatStr(\"scheduler\", \"p6\")+\"\\\\page\"+getCatStr(\"scheduler\", \"p7\")+\"\\\\numpages}\" ;
  
  if (!isMotif())
  {    printTable(shapeName,              //shapeName
               TRUE,                      //showDialog
               header,                    //dyn_string header
               footer,                    //dyn_string footer
               PT_VISIBLECOLUMNS,         //int columnsType
               \"\",                        //dyn_string | dyn_int columns
               TRUE,                      //fitToPage
               TRUE,                      //landscape
               FALSE,                     //gridLines
               TRUE);                     //gridBackground
  }
  else
  {
    setValue (shapeName,                     //shape
              \"printTable\",                  //feature
              header,                        //header
              makeDynString(),               // no special column attributes
              footer,                        //footer
              \"AS_UNIX.cfg\",                 //printer config
              PAPER_FORMAT_A4,               //paper
              TRUE,                          //landscape
              FALSE,                         //show all
              c,                           //separator
              1,                             //left space                  
              1);                            //line space
  }             
}

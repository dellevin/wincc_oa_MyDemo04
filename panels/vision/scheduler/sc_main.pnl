"main()
{
  string     type, dpe, dpe1;
  
  switch (tabMain.activeRegister())
  {
    case SCHED_LIST:
      if (Row1.text() == \"\")
      {
        sc_nolineselected();  return;
      }
      tabSchedules.currentCell(Row1.text(),1);
      dpe = tabSchedules.cellValue();
      tabSchedules.currentCell(Row1.text(),3);
      type = tabSchedules.cellValue();
      break;
      
    case LOG:
      if (Row2.text() == \"\")
      {
        sc_nolineselected();  return;
      }      
      tab1.currentCell(Row2.text(),1);
      dpe  = tab1.cellValue();
      dpe1 = dpSubStr(dpe, DPSUB_SYS_DP);
      dpGet(dpe1+\".type:_online.._value\", type);
      break;
      
    case DEPENDENCIES:
      if (Row3.text() == \"\")
      {
        sc_nolineselected();  return;
      }  
      tab2.currentCell(Row3.text(),1);
      dpe = tab2.cellValue();
      tab2.currentCell(Row3.text(),2);
      type = tab2.cellValue();
      break;
      
    default:
      return;
  }  
  
  type = strtoupper(type);  
  sc_editCas(type, dpe);
}" 0

"#uses \"sched.ctl\"
main()
{
  int        i;
  string     sys, activeSchedules;
  dyn_string ds;
  dyn_float  df;

  //Check user permission
  if(!getUserPermission(1))  //At least visualisation level
  {
    ChildPanelOnCentralModalReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),
                              makeDynString(getCatStr(\"general\",\"permission\")+\"!\"),df,ds);

    PanelOff();
  }
  else
  {
    if(!getUserPermission(2) || !getUserPermission(3)) //\"Normal User Level\" or 
      pf_userPermission2();
    
    if(!getUserPermission(3)) // \"Extended User Level\"
      pf_userPermission3();
      
    
    if ( !isFunctionDefined(\"sc_loadReady\") )
    {
      string sMessageText = getCatStr(\"scheduler\",\"loadLib\");
      ChildPanelOnCentralModalReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
      PanelOff();
    } 

    p_iActiveRegister = 0;
    pf_emptyTable();
    
    dynClear(p_dsSysNames);
    dynClear(p_duiSysIds);
    getSystemNames(p_dsSysNames,p_duiSysIds);
  
    txtLoState.text(getCatStr(\"scheduler\", \"load\"));
    sys = getSystemName();

    if ( dynlen(p_dsSysNames) > 0 )
    {
      //Get the right programs
      for (i=1; i<=dynlen(p_dsSysNames); i++)
      {
        dpConnect(\"workCB1\", TRUE, p_dsSysNames[i]+\":\"+p_sScCom+\".transfer:_original.._value\");
        dpConnect(\"workCB2\", FALSE, p_dsSysNames[i]+\":\"+p_sScCom+\".mainState:_original.._value\");
        dpQueryConnectSingle(\"workCB1\", FALSE,
                                        p_dsSysNames[i] + \":\",
                                        \"SELECT '_original.._value' FROM '*.result' WHERE _DPT = \\\"_Sc\\\"\");
      }                               
    }
    buVis.visible(FALSE);
  }
}  

//*******************************
workCB1(string dp1, dyn_dyn_anytype value)
{
  int         i,
              running      = 0,
              active      = 0,
              inactive      = 0,
              sumOfAllCond = 0;
  bool        result,
              mode; 
  string      type;                
  dyn_string  dsAllProgs;

  dynClear(dsAllProgs);

  //Get ALL programs
  for (i=1; i<=dynlen(p_dsSysNames); i++)
  {
    //Get ALL program
    dsAllProgs = dpNames(p_dsSysNames[i]+\":*\", \"_Sc\");  //Get all UI datapoints - also from the dist systems
  }
    
  //Get all overrides and states
  for (i=1; i<=dynlen(dsAllProgs); i++)
  {
    if (dpExists(dsAllProgs[i]+\".result:_original.._value\"))
    {
      dpGet(dsAllProgs[i]+\".result:_original.._value\", result,
            dsAllProgs[i]+\".mode:_original.._value\",   mode);
            
      //Number of result of conditions == TRUE
      if(result == TRUE)
        sumOfAllCond++;
      //Number of activated
      if(mode == TRUE)
        active++;
      //Number of deactivated
      if(mode == FALSE)
        inactive++;      
      //Number of running
      if((result && mode) == TRUE)
        running++;
    }
  }
  setMultiValue(\"txtNoOfActive\", \"text\",    active,
                \"txtNoOfInactive\", \"text\",  inactive, 
                \"txtNoOfRunning\", \"text\",   running,
                \"txtNoOfAllConFullfilled\", \"text\",  sumOfAllCond,
                \"txtNoOfSchedules\", \"text\", dynlen(dsAllProgs));
  
}  

//*******************************
workCB2(string dp1, string value)
{
  int     i;
  bool    b;
  string  sl1, sl2, sl3, mainState;

  mainState = getSystemName()+myUiNumber();
  if (value != mainState)
    return;
  
  getValue(\"load1\", \"text\", sl1);
  getValue(\"load2\", \"text\", sl2);
  getValue(\"load3\", \"text\", sl3);
  sl1 = strltrim(sl1, \"\\\"\");  sl2 = strltrim(sl2, \"\\\"\");  sl3 = strltrim(sl3, \"\\\"\");
  sl1 = strrtrim(sl1, \"\\\"\");  sl2 = strrtrim(sl2, \"\\\"\");  sl3 = strrtrim(sl3, \"\\\"\");
  
  i = tabMain.activeRegister();
  
  if (i == 0) b = sl1;
  if (i == 1) b = sl2;
  if (i == 2) b = sl3;
  
  if (!b)
  {
    txtLoState.text(getCatStr(\"scheduler\", \"ready\"));
    pf_emptyTable();
  }
  else
    txtLoState.text(getCatStr(\"scheduler\", \"load\"));
}    



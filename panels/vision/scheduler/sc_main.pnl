E "// ============================================================================
// Panel Global Variables
// ============================================================================
  int        p_iActiveRegister;
  string     p_sScCom = \"_ScCom\";
  dyn_string p_para_register,
             p_dsSysNames;
  dyn_uint   p_duiSysIds;
  const int  SCHED_LIST = 0;
  const int  LOG = 1;
  const int  DEPENDENCIES = 2;

//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
//Function: pf_emptyTable()
//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
pf_emptyTable()
{
  int i;

  //Schedules tab
  switch (p_iActiveRegister)
  {
    case SCHED_LIST:
      i = tabSchedules.lineCount();
      break;
    case LOG:
      i = tab1.lineCount();
      break;
    case DEPENDENCIES:
      i = tab2.lineCount();
      break;
  }

  if (i <= 0)
  {
    cmdActivate.enabled(FALSE);
    cmdCopy.enabled(FALSE);
    cmdDeactivate.enabled(FALSE);
    cmdEdit.enabled(FALSE);
    cmdPrint.enabled(FALSE);
    cmdDelete.enabled(FALSE);
  }
  else
  {
    if (p_iActiveRegister != LOG)
    {
      cmdActivate.enabled(TRUE);
      cmdCopy.enabled(TRUE);
      cmdDeactivate.enabled(TRUE);
      cmdEdit.enabled(TRUE);
      cmdPrint.enabled(TRUE);
      cmdDelete.enabled(TRUE);
    }
    else
    {
      cmdActivate.enabled(FALSE);
      cmdCopy.enabled(FALSE);
      cmdDeactivate.enabled(FALSE);
      cmdEdit.enabled(FALSE);
      cmdPrint.enabled(TRUE);
      cmdDelete.enabled(TRUE);
    }
  }
}
//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
//Function: pf_userPermission2()
//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
pf_userPermission2()
{
  cmdActivate.enabled(FALSE);
  cmdCopy.enabled(FALSE);
  cmdDaySchedule.enabled(FALSE);
  cmdDeactivate.enabled(FALSE);
  cmdEdit.enabled(FALSE);
  cmdFreeSchedule.enabled(FALSE);
  cmdInfoAction.enabled(FALSE);
  cmdPrint.enabled(FALSE);
  cmdReactionProg.enabled(FALSE);
  cmdRefresh.enabled(FALSE);
  cmdSpecDays.enabled(FALSE);
  cmdWeekSchedule.enabled(FALSE);
}
//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
//Function: pf_userPermission3()
//** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** ** **
pf_userPermission3()
{
  cmdDelete.enabled(FALSE);
}


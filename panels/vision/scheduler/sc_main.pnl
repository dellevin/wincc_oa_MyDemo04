"main()
{
  int        i;
  string     transfer;
  dyn_string aLog;
    
  switch (tabMain.activeRegister())
  {
    case SCHED_LIST:
      if (dynlen(p_dsSysNames) > 0 )
      {
        for (i=1; i<=dynlen(p_dsSysNames); i++)
        {
          dpGet(p_dsSysNames[i]+\":\"+p_sScCom+\".transfer:_online.._value\", transfer);
          dpSet(p_dsSysNames[i]+\":\"+p_sScCom+\".transfer:_original.._value\", transfer); 
        }                       
      }  
      break;
    case DEPENDENCIES:
      if (dynlen(p_dsSysNames) > 0 )
      {
        for (i=1; i<=dynlen(p_dsSysNames); i++)
        {
          dpGet(p_dsSysNames[i]+\":\"+p_sScCom+\".transfer:_online.._value\", transfer);
          dpSet(p_dsSysNames[i]+\":\"+p_sScCom+\".transfer:_original.._value\", transfer); 
        }                       
      }  
      break;
      
    case LOG:
      if (dynlen(p_dsSysNames) > 0 )
      {
        for (i=1; i<=dynlen(p_dsSysNames); i++)
        {
          dpGet(p_dsSysNames[i]+\":\"+p_sScCom+\".actionLog:_online.._value\", aLog);
          dpSet(p_dsSysNames[i]+\":\"+p_sScCom+\".actionLog:_original.._value\", aLog); 
        }                       
      }
      break;
  }  

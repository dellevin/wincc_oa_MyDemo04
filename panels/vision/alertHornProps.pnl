PANEL,-1 -1 620 560 N "STD_panel_back" 0
"main()
{
  bool 		bBeeperStat, bWavStat, bDpeStat, bTwoPrios;
  string	sFrequ, freq2, beepDuration, beepDuration2, sPause, pause2, 
  			sLowerSnd, sHighSnd, sDpeValue, sDpeValue2, sPanelName;
  int		iPriority, iAlertDelay, iWavSec, iWavSec2;
  
  addGlobal(\"MultiPriority\", BOOL_VAR);
  
  if (!_WIN32) setValue(\"cmdVolume\",\"visible\",FALSE);
  
  // If this panel was opened out of the \"Properties\" panel, the close bitton will be hidden, else it will be shown.
  
  dpGet(myUiDpName()+\".ChildPanelOn.PanelName:_online.._value\", sPanelName);
  
  if ( (sPanelName == \"Properties\") || (sPanelName == \"Eigenschaften\")) setValue(\"closeHornProp\",\"visible\",0);
  else setValue(\"closeHornProp\",\"visible\",1);
  
    
  // Function for initializing the saved Selections  
  
  dpGet(\"AlertHorn.BeeperProps.Enabled:_online.._value\", 	bBeeperStat,
  		\"AlertHorn.WavProps.Enabled:_online.._value\",    	bWavStat,
  		\"AlertHorn.ExternalProps.Enabled:_online.._value\",	bDpeStat,
  		\"AlertHorn.HighPrioLimit:_original.._value\", 		iPriority,
  		\"AlertHorn.HornSilenceTime:_online.._value\",		iAlertDelay,
  		\"AlertHorn.DistinguishTwoPrios:_original.._value\",  bTwoPrios);
  		
  chkPriority.state(0) = bTwoPrios;
  setValue(\"ChooseDelay\",\"text\",iAlertDelay);
  
  ///// case Beeper Sound /////
  if (bBeeperStat == 1)
  {
    chkMode.state(0) = 1;
    
    dpGet(\"AlertHorn.BeeperProps.LowPrioFrequency:_online.._value\", 	  sFrequ,
    	  \"AlertHorn.BeeperProps.LowPrioIntervalTime:_online.._value\",    sPause,
    	  \"AlertHorn.BeeperProps.LowPrioBeepTime:_online.._value\", 		  beepDuration,
    	  \"AlertHorn.BeeperProps.HighPrioIntervalTime:_original.._value\", pause2,
		  \"AlertHorn.BeeperProps.HighPrioFrequency:_original.._value\",	  freq2,
		  \"AlertHorn.BeeperProps.HighPrioBeepTime:_original.._value\",	  beepDuration2);
  
    if (bTwoPrios == 1) setMultiValue(\"txtBeeperCaption\",\"enabled\", TRUE,
  				    				  \"txtBeepBreak\",\"enabled\",		TRUE,
  				    				  \"txtBeepDuration\",\"enabled\", 	TRUE,
  				       				  \"txtBeepFrequ\",\"enabled\", 	TRUE,
  				   					  \"txt_frequency\",\"enabled\", 	TRUE,
  				   					  \"txt_duration\",\"enabled\",  	TRUE,
  				   					  \"txt_pause\",\"enabled\",	   	TRUE,
  				  					  \"txt_frequency2\",\"enabled\",	TRUE,
  				  					  \"txt_duration2\",\"enabled\", 	TRUE,
  				   					  \"txt_pause2\",\"enabled\",	   	TRUE,
  				    				  \"txtMsec1\",\"enabled\",	   		TRUE,
  				   					  \"txtMsec2\",\"enabled\",	   		TRUE,
  				   					  \"txtHz\",\"enabled\",		   	TRUE,
  				    				  \"cmdBeeperTest\",\"enabled\", 	TRUE,
  				    				  \"txt_frequency\",\"text\",    	sFrequ,
  				    				  \"txt_duration\",\"text\",     	beepDuration,
  				    				  \"txt_pause\",\"text\",	       	sPause,
  				    				  \"txt_frequency2\",\"text\",   	freq2,
  				    				  \"txt_duration2\",\"text\",	   	beepDuration2,
  				    				  \"txt_pause2\",\"text\",	   		pause2,
  				    				  \"txtBeepHighPrio\",\"visible\",	TRUE,
  				    				  \"txtBeepLowPrio\",\"visible\", 	TRUE,
  				    				  \"txtHighPrio\",\"visible\",	TRUE,
  				    				  \"txtLowPrio\",\"visible\", 	TRUE,
  				    				  \"txt19\",\"visible\",	TRUE,
  				    				  \"txt20\",\"visible\", 	TRUE,
  				    				  \"txtMsec3\",\"enabled\",	   		TRUE,
  				    				  \"txtMsec4\",\"enabled\",	   		TRUE,
  				    				  \"txtHz2\",\"enabled\",	   		TRUE,
  				    				  \"cmdBeeperTest2\",\"enabled\",	TRUE,
  									  \"txt15\",\"enabled\", 	   	   	TRUE,
  				  					  \"txt16\",\"enabled\", 	   	   	TRUE,
  				  					  \"ChoosePrio\",\"enabled\",	   	TRUE,
  				  					  \"ChoosePrio\",\"text\",   	   	iPriority);

    if (bTwoPrios == 0) setMultiValue(\"txtBeeperCaption\",\"enabled\", TRUE,
  				    				  \"txtBeepBreak\",\"enabled\",		TRUE,
  				    				  \"txtBeepDuration\",\"enabled\", 	TRUE,
			  				   		  \"txtBeepFrequ\",\"enabled\", 	TRUE,
  				    				  \"txt_frequency\",\"enabled\", 	TRUE,
  				    				  \"txt_duration\",\"enabled\",  	TRUE,
  				    				  \"txt_pause\",\"enabled\",	   	TRUE,
  				    				  \"txtMsec1\",\"enabled\",	   		TRUE,
  				    				  \"txtMsec2\",\"enabled\",	   		TRUE,
  				    				  \"txtHz\",\"enabled\",		   	TRUE,
  				   					  \"cmdBeeperTest\",\"enabled\", 	FALSE,
  				   					  \"cmdBeeperTest2\",\"enabled\", 	FALSE,
  				    				  \"txt_frequency\",\"text\",    	sFrequ,
  				   					  \"txt_duration\",\"text\",     	beepDuration,
  				    				  \"txt_pause\",\"text\",	       	sPause,
  				    				  \"txtBeepLowPrio\",\"enabled\", 	TRUE);
  }
  
  ///// case .wav Sound ///////
  if (bWavStat == 1)
  {
    chkMode.state(1) = 1;
    
    dpGet(\"AlertHorn.WavProps.LowPrioWavFile:_online.._value\",  	  sLowerSnd,
    	  \"AlertHorn.WavProps.HighPrioWavFile:_online.._value\", 	  sHighSnd,
    	  \"AlertHorn.WavProps.LowPrioCycleInterval:_online.._value\",  iWavSec,
    	  \"AlertHorn.WavProps.HighPrioCycleInterval:_online.._value\", iWavSec2);
    	  
    if (bTwoPrios == 1)
    {
      setMultiValue(\"cmdWavTest\",\"enabled\", 	TRUE,
    			    \"cmdWavTest2\",\"enabled\",	TRUE,
    			    \"txtWavCaption\",\"enabled\", 	TRUE,
  				    \"cmdFileSelect\",\"enabled\", 	TRUE,
  				    \"cmdFileSelect2\",\"enabled\",	TRUE,
  				    \"FileName\",\"text\", 	      	sLowerSnd,
  				    \"FileName2\",\"text\",		 	sHighSnd,
  				    \"txtLowPrio\",\"enabled\", 	TRUE,
  				    \"txtHighPrio\",\"enabled\",	TRUE,
					\"txt15\",\"enabled\", 	  	  	TRUE,
  				    \"txt16\",\"enabled\", 	  	  	TRUE,
  				    \"ChoosePrio\",\"enabled\",   	TRUE,
  				    \"ChoosePrio\",\"text\",   	  	iPriority);
    
      if (iWavSec > 0) setMultiValue(\"WavContCyc\",\"enabled\", TRUE,
      							    \"WavContCyc\",\"number\",  1,
      							    \"wavCyclMode\",\"enabled\",TRUE,
      							    \"wavCyclMode\",\"text\",   iWavSec);
      							    	  
	  if (iWavSec <= 0) setMultiValue(\"WavContCyc\",\"enabled\", TRUE,
      							     \"WavContCyc\",\"number\",  0);
      							   
	  if (iWavSec2 > 0)setMultiValue(\"WavContCyc2\",\"enabled\", TRUE,
      							    \"WavContCyc2\",\"number\",  1,
      							    \"wavCyclMode2\",\"enabled\",TRUE,
      							    \"wavCyclMode2\",\"text\",   iWavSec2);
      							  
	  if (iWavSec2 <= 0) setMultiValue(\"WavContCyc2\",\"enabled\",TRUE,
      							      \"WavContCyc2\",\"number\", 0);
     }
     if (bTwoPrios == 0)
     {
       setMultiValue(\"cmdWavTest\",\"enabled\", 	 TRUE,
    			     \"txtWavCaption\",\"enabled\",  TRUE,
  				     \"cmdFileSelect\",\"enabled\",  TRUE,
  				     \"FileName\",\"text\", 	  	 sLowerSnd,
  				     \"txtLowPrio\",\"enabled\",	 TRUE);
    
       if (iWavSec > 0) setMultiValue(\"WavContCyc\",\"enabled\", TRUE,
      							     \"WavContCyc\",\"number\",  1,
      							     \"wavCyclMode\",\"enabled\",TRUE,
      							     \"wavCyclMode\",\"text\",   iWavSec);
      							    	  
	   if (iWavSec <= 0) setMultiValue(\"WavContCyc\",\"enabled\", TRUE,
      							      \"WavContCyc\",\"number\",  0);
     }  	
  }
  
  ///// case DPE //////
  if (bDpeStat == 1)
  {
    chkMode.state(2) = 1;
    
    dpGet(\"AlertHorn.ExternalProps.LowPrioExternalHornDPE:_original.._value\",  sDpeValue,
    	  \"AlertHorn.ExternalProps.HighPrioExternalHornDPE:_original.._value\", sDpeValue2);
    
    if (bTwoPrios == 1) setMultiValue(\"txtDpeCaption\",\"enabled\", TRUE,
  				    				  \"DpSel\",\"enabled\",	     TRUE,
  				   					  \"DpSel2\",\"enabled\",	     TRUE,
  				    				  \"DpName\",\"enabled\",    	 TRUE,
  				    				  \"DpName\",\"text\", 	   		 sDpeValue,
  				    				  \"DpName2\",\"enabled\",   	 TRUE,
  				    				  \"DpName2\",\"text\", 	     sDpeValue2,
  				    				  \"txt19\",\"enabled\",	     TRUE,
  				    				  \"txt20\",\"enabled\",	     TRUE,
  	  								  \"txt15\",\"enabled\",         TRUE,
  				    				  \"txt16\",\"enabled\",         TRUE,
  				    				  \"ChoosePrio\",\"enabled\",    TRUE,
  				    				  \"ChoosePrio\",\"text\",       iPriority);
  				    
  	if (bTwoPrios == 0) setMultiValue(\"txtDpeCaption\",\"enabled\", TRUE,
  				    				  \"DpSel\",\"enabled\",	     TRUE,
  				    				  \"DpName\",\"enabled\",        TRUE,
  				    				  \"DpName\",\"text\", 	         sDpeValue,
  				    				  \"txt19\",\"enabled\",	     TRUE);
  }
  if ( (!chkMode.state(0)) && (!chkMode.state(1)) && (!chkMode.state(2)) ) setMultiValue (\"chkPriority\",\"enabled\", FALSE,
  																						  \"chkPriority\",\"state\",0, FALSE);
}" 0

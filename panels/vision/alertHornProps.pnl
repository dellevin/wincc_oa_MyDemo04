  getMultiValue(\"txt_duration\",\"text\",  beepDuration,
		      	\"txt_frequency\",\"text\", frequency,
		      	\"txt_pause\",\"text\",	    pause_time);

  if (beepDuration != 0 || frequency != 0 || pause_time != 0)
  {
    dpSet(\"AlertHorn.Enabled:_original.._value\",0);     // Stops alert sound for sound check
    for(i=1; i<=3; i++)
	{
	  beep(frequency,beepDuration);
	  delay(0,pause_time);
	}
	delay(1);
    dpSet(\"AlertHorn.Enabled:_original.._value\",1);     // After sound check has finished, the horn will be started again.
  }
	
  else
  { 
    string sMessageText=getCatStr(\"misc\",\"AlarmHornTEST\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
    return;
  }

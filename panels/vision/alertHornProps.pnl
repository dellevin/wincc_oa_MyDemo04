"main()
{
  string     sWIN;
  dyn_string dsWIN;

  // This function searches for the file 'sndvol32exe'
  
  system (\"cmd /c set windir > C:/temp.txt\");
  delay(0,300);
 
  if (access(\"C:/temp.txt\",F_OK) == 0)
  {
    fileToString(\"C:/temp.txt\", sWIN);
  
    dsWIN = strsplit(sWIN,\"=\");
  
    if (access (strrtrim(dsWIN[2],\"\\n\") + \"/System32/sndvol32.exe\",F_OK) == 0)
      system(strrtrim(dsWIN[2],\"\\n\") + \"/System32/sndvol32.exe\");
    else
    {
      string sMessageText=getCatStr(\"general\",\"noFile\")+ \" \\'SNDVOL32.EXE\\'\";
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
    }
  
    system (\"rm C:/temp.txt\");
  }
  else
  {
    string sMessageText=getCatStr(\"general\",\"noFile\")+ \" \\'sndvol32.exe\\'\";
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
    return; 
  }

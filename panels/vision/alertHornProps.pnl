"main()
{
  int 		iBeepDuration, iBeepDuration2, iPause, iPause2, iFreq, 
            iFreq2, iPriority, iWavSec, iWavSec2, iAlertDelay;
  string 	sFileName1, sFileName2, sDpeValue, sDpeValue2;

  // Function for saving the choosen selections-
  // Checks general values as priority and alert delay
  
  getMultiValue(\"ChooseDelay\",\"text\", iAlertDelay,
  				\"ChoosePrio\",\"text\",  iPriority);
  				
  
  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  // Checks all done settings
  // Checks case: priority missing
  if ( (chkPriority.state(0)) && (iPriority == 0) )
  {
    string sMessageText=getCatStr(\"misc\",\"NoPriority\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
    return; 
  }
  
  // Checks case: alert delay == 0
  if (iAlertDelay == 0)
  {
    string sMessageText=getCatStr(\"misc\",\"NoAlertDelay\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
    return; 
  }
  
  // Checks case BEEPER
  if (chkMode.state(0))
  {
	getMultiValue(\"txt_duration\",\"text\",	iBeepDuration,
			      \"txt_frequency\",\"text\",	iFreq,
			      \"txt_pause\",\"text\",	 	iPause,
			      \"txt_duration2\",\"text\",	iBeepDuration2,
			      \"txt_frequency2\",\"text\",	iFreq2,
			      \"txt_pause2\",\"text\", 	 	iPause2);
			      
    if (chkPriority.state(0))
    {
      if (iBeepDuration == 0 || iFreq == 0 || iPause == 0 || iBeepDuration2 == 0 || iFreq2 == 0 || iPause2 == 0)
      {
        string sMessageText=getCatStr(\"misc\",\"AlertHornBEEPER\");
        ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
        return; 
      }
    }
    if (iBeepDuration == 0 || iFreq == 0 || iPause == 0 && !chkPriority.state(0))
    {
      string sMessageText=getCatStr(\"misc\",\"AlertHornBEEPER\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
    }
  }
  
  // Checks case WAV
  if (chkMode.state(1))
  {
    getMultiValue(\"FileName\",\"text\", sFileName1,
		      	  \"FileName2\",\"text\",sFileName2);			      	  

    if (sFileName1 == \"\" || sFileName2 == \"\" && chkPriority.state(0))
    {
      string sMessageText=getCatStr(\"misc\",\"AlertHornWAV\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
  	}
  }
  
  // Checks case DPE:
  if (chkMode.state(2))
  {
    getMultiValue(\"DpName\",\"text\", sDpeValue,
    			  \"DpName2\",\"text\",sDpeValue2);
    
    if(!dpExists(sDpeValue))  // Does dp exist?
    {
      string sMessageText=getCatStr(\"misc\",\"SelectDp\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
    }
    
    if (chkPriority.state(0) && !dpExists(sDpeValue2)) // Does dp2 exist?
    {
      string sMessageText=getCatStr(\"misc\",\"SelectDp\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
    }
    
     
    if (sDpeValue == \"\" || sDpeValue2 == \"\" && chkPriority.state(0))
    {
      string sMessageText=getCatStr(\"misc\",\"AlertHornDPE\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),makeDynString(sMessageText));
      return; 
    }
  }
  
  // If everything is alright, set values to the datapoints
  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  
  // Stops the horn until the new settings are saved
  dpSetWait(\"AlertHorn.Enabled:_original.._value\",0);
  
  
  ////////////// If high and low Priority  //////////////////////////////////////////////////
  if (chkPriority.state(0))
  {
	dpSet(\"AlertHorn.DistinguishTwoPrios:_original.._value\", 1);
    
    if (iPriority != 0) dpSet(\"AlertHorn.HighPrioLimit:_original.._value\", iPriority);
  }
  else dpSet(\"AlertHorn.DistinguishTwoPrios:_original.._value\", 0,
  			 \"AlertHorn.HighPrioLimit:_original.._value\", 0);
  
  dpSetWait(\"AlertHorn.HornSilenceTime:_original.._value\", iAlertDelay,
  			\"AlertHorn.HighPrioLimit:_original.._value\",   iPriority);		
  
  
  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  // CASE BEEPER:
  if (chkMode.state(0)) dpSetWait(\"AlertHorn.BeeperProps.LowPrioIntervalTime:_original.._value\",  iPause,
                                  \"AlertHorn.BeeperProps.LowPrioFrequency:_original.._value\", 	  iFreq,
                                  \"AlertHorn.BeeperProps.LowPrioBeepTime:_original.._value\",	  iBeepDuration,
                                  \"AlertHorn.BeeperProps.HighPrioIntervalTime:_original.._value\", iPause2,
                                  \"AlertHorn.BeeperProps.HighPrioFrequency:_original.._value\",	  iFreq2,
                                  \"AlertHorn.BeeperProps.HighPrioBeepTime:_original.._value\",	  iBeepDuration2,
                                  \"AlertHorn.BeeperProps.Enabled:_original.._value\", 			  1);
  
  // CASE !BEEPER -SETS DP TO \"\"
  if (!chkMode.state(0)) dpSetWait(\"AlertHorn.BeeperProps.LowPrioIntervalTime:_original.._value\",  0,
			  					   \"AlertHorn.BeeperProps.LowPrioFrequency:_original.._value\",     0,
		      					   \"AlertHorn.BeeperProps.LowPrioBeepTime:_original.._value\",	   0,
		      					   \"AlertHorn.BeeperProps.HighPrioIntervalTime:_original.._value\", 0,
		      					   \"AlertHorn.BeeperProps.HighPrioFrequency:_original.._value\",    0,
		      					   \"AlertHorn.BeeperProps.HighPrioBeepTime:_original.._value\",	   0,
		      					   \"AlertHorn.BeeperProps.Enabled:_original.._value\", 			   0);
		      
		      
  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  // CASE .WAV SOUND:
  if (chkMode.state(1))
  {	
	dpSetWait(\"AlertHorn.WavProps.LowPrioWavFile:_original.._value\",  sFileName1,
	   		  \"AlertHorn.WavProps.HighPrioWavFile:_original.._value\", sFileName2,
	   		  \"AlertHorn.WavProps.Enabled:_original.._value\", 		  1);   
	    
	if (WavContCyc.number == 1) 
	{
	  getValue(\"wavCyclMode\",\"text\",iWavSec);
	  dpSetWait(\"AlertHorn.WavProps.LowPrioCycleInterval:_original.._value\", iWavSec,
	   		 	\"AlertHorn.WavProps.LowPrioContinous:_original.._value\",	 0);
    }   	  

    if (WavContCyc.number == 0) dpSetWait(\"AlertHorn.WavProps.LowPrioCycleInterval:_original.._value\", \"\",
    									  \"AlertHorn.WavProps.LowPrioContinous:_original.._value\",	   1);
	     
    if (WavContCyc2.number == 1)
    {
      getValue(\"wavCyclMode2\",\"text\",iWavSec2);
      dpSetWait(\"AlertHorn.WavProps.HighPrioCycleInterval:_original.._value\", iWavSec2,
      			\"AlertHorn.WavProps.HighPrioContinous:_original.._value\",	  0);
    }
    if (WavContCyc2.number == 0) dpSet(\"AlertHorn.WavProps.HighPrioCycleInterval:_original.._value\", \"\",
    								   \"AlertHorn.WavProps.HighPrioContinous:_original.._value\",	 1);
  }
  // CASE !WAV -SETS DP TO \"\"
  if (!chkMode.state(1)) dpSetWait(\"AlertHorn.WavProps.LowPrioWavFile:_original.._value\",  \"\",
	    		  				   \"AlertHorn.WavProps.HighPrioWavFile:_original.._value\", \"\",
	    		  				   \"AlertHorn.WavProps.Enabled:_original.._value\", 		   0);
  
  
  
  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  // CASE DPE:
  if (chkMode.state(2)) dpSetWait(\"AlertHorn.ExternalProps.LowPrioExternalHornDPE:_original.._value\",  sDpeValue,
                                  \"AlertHorn.ExternalProps.HighPrioExternalHornDPE:_original.._value\", sDpeValue2,
                                  \"AlertHorn.ExternalProps.Enabled:_original.._value\", 				   1); 
  
  // CASE !DPE -SETS DP TO \"\"
  if (!chkMode.state(2)) dpSetWait(\"AlertHorn.ExternalProps.LowPrioExternalHornDPE:_original.._value\",  \"\",
    		  					   \"AlertHorn.ExternalProps.HighPrioExternalHornDPE:_original.._value\", \"\",
    		  					   \"AlertHorn.ExternalProps.Enabled:_original.._value\", 			    0); 
    		  						 
    		  						 


  // Everyting is saved, so the horn can be restarted with the new settings.
  dpSet(\"AlertHorn.Enabled:_original.._value\",0);  
  dpSet(\"AlertHorn.Enabled:_original.._value\",1);  
  
  setValue(\"txt_info\",\"visible\",1);
  delay(3);
  
  setValue(\"txt_info\",\"visible\",0);
}" 0

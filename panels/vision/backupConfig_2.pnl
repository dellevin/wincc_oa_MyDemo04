	   if ( sPath!=\"\" && _WIN32 && substr(sPath,strlen(sPath)-1)!=\"\\\\\")
      sPath+=\"\\\\\"; 
	   if ( sPath!=\"\" && _UNIX && substr(sPath,strlen(sPath)-1)!=\"/\")
      sPath+=\"/\"; 

    dp_path_invis.text = sPath;  

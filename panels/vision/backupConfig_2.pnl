  strreplace( sPath, \"\\\\\", \"/\" );

  iErr = folderSelector( sPath );

  if(( iErr >= 1 ) && ( strlen( sPath ) > 0 ))
  {
    if (strpos(sPath+\"/\",sPath2) >=0 )  // 
    {
      ChildPanelOnCentralModal(\"vision/MessageWarning\", \"\", makeDynString(sErrorMsg)); 

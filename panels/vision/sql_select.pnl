  getValue( \"statisticFunction\", \"selectedPos\", iPos );
  getValue( \"konfig\",            \"text\",        sKonfig );
  getValue( \"statFct\",           \"state\", 0,    bState );

  if( !bState )
  {
    // No statistic statement
      getValue( \"konfig\",    \"text\", sKonfig );
      setValue( \"selection\", \"text\", sKonfig );
  }
  else
  {
    // One statistic statement
    switch( iPos )
    {
      case 1:
        getValue( \"konfig\",    \"text\", sKonfig );
        sKonfig = \"COUNT(\" + sKonfig + \")\" ;
        setValue( \"selection\", \"text\", sKonfig );
      break;

      case 2:
        getValue( \"konfig\",    \"text\", sKonfig );
        sKonfig = \"MIN(\" + sKonfig + \")\" ;
        setValue( \"selection\", \"text\", sKonfig );
      break;


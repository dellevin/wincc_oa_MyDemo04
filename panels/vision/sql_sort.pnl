E "main()
{
int    iModus;
int    iElement;
int    iDetail;
string sDetail;

 	
  getMultiValue( \"sortModus\",   \"number\",      iModus,
                 \"sortElement\", \"selectedPos\", iElement,
                 \"sortDetails\", \"number\",      iDetail );

  // ASC and DESC
  if( iDetail == 1 )
    sDetail = \" ASC\";
  else if( iDetail == 2 )
    sDetail = \" DESC\";
  else
    sDetail = \"\";

  // Modus
  if( iModus == 1 )
  {
    // Sort by time
    setValue( \"group\", \"state\", 0, false );
    sortElement.enabled	  = false;
    sortDetails.enabled   = false;
    group.enabled         = true;
    groupTimeUnit.visible = true;
    g_sSort = \"0\";
    sortElement.text	  = \"\";
    g_sGroup = \"\";
  }
  else if( iModus == 2 )
  {
    // Sort by dp element
    if( dynlen( g_dsSelect ) > 0 )
    {
      setValue( \"sortElement\", \"items\", g_dsSelect,
                               \"text\",  \"\" );
      iElement = 1;
      group.enabled       = false;
      sortElement.enabled = true;
    }
    else
    {
      ChildPanelOnCentral( \"vision/MessageWarning\",
                           getCatStr( \"general\", \"warning\" ),
                           makeDynString( getCatStr( \"sql\", \"selectStateEmpty\" )));
      return;
    }
    
    setValue( \"group\", \"state\", 0, false );
    sortDetails.enabled	  = true;
    groupTimeUnit.visible = false;
    g_sSort  = \"\";
    g_sGroup = \"\";
  }
  else
  {
    // Do not sort
    setValue( \"group\", \"state\", 0, false );
    sortElement.enabled	  = false;
    sortDetails.enabled	  = false;
    group.enabled         = false;
    groupTimeUnit.visible = false;
    g_sSort               = \"\";
    sortElement.text	  = \"\";
    g_sGroup              = \"\";
  }
  groupDetails.enabled  = false;
}" 0


 E E "main()
{
  dpConnect(\"zeige\", \"_DataManager.AlertDataSetChange.SwitchTime:_online.._value\");
}

zeige(string dp, time zeit)
{
  int i, secs = period(zeit);
  
  setValue(\"a_days\",    \"text\", i = (secs/86400)); secs -= i;
  setValue(\"a_hours\",   \"text\", i = (secs/3600));  secs -= i;
  setValue(\"a_minutes\", \"text\", i = (secs/60));
}" 0


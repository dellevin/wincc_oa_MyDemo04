LANG:10027 0 
"main()
{
  int tag, std, min;

  getMultiValue(\"e_days\",    \"text\", tag,
                \"e_hours\",   \"text\", std,
                \"e_minutes\", \"text\", min);
                
  dpSet(\"_DataManager.DataSetChange.SwitchTime:_original.._value\",
        tag*86400 + std*3600 + min*60);
}" 0
 E E "main()
{
  dpConnect(\"zeige\", \"_DataManager.DataSetChange.SwitchTime:_online.._value\");
}

zeige(string dp, time zeit)
{
  int i, secs = period(zeit);
  
  setValue(\"e_days\",    \"text\", i = (secs/86400)); secs -= i;
  setValue(\"e_hours\",   \"text\", i = (secs/3600));  secs -= i;
  setValue(\"e_minutes\", \"text\", i = (secs/60));
}" 0


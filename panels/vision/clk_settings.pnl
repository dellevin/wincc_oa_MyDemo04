"main() 
{ 
string sysname, dpn_tf, mode, dpn;
string sMode;
dpn_tf = $CLKM; 
strreplace(dpn_tf,\"clk_Mgr_\",\"clk_\");       


  getValue( \"preSelection\", \"text\", sMode );
  
  if( sMode == getCatStr(\"clkmsg\",\"always0\"))
  {
    dpSetWait($CLKM+\".mode:_original.._value\",0);
    dpSetWait($CLKM+\".result:_original.._value\",0);
  }
  else if( sMode == getCatStr(\"clkmsg\",\"always1\"))
  {
    dpSetWait($CLKM+\".mode:_original.._value\",1);
    dpSetWait($CLKM+\".result:_original.._value\",1);
  }
  else if( sMode == getCatStr(\"clkmsg\",\"auto\"))
  {
    dpSetWait($CLKM+\".mode:_original.._value\",2);
  }
  else
    return;


  // the bool-function clk_checkAndSettings() return 0 for 'successful' !
  if( !clk_checkAndSettings( $CLKM ))
  {
    clk_transferToTimed( $CLKM );
    
    if( sMode == getCatStr(\"clkmsg\",\"auto\"))
    {
      // To prevent multi-launches of the same time-switch
      if (dpExists(dpn_tf)) 
      { 
        dpSet(dpn_tf+\".validUntil:_original.._value\",getCurrentTime()); 
        dpSet(dpn_tf+\".validUntil:_original.._value\",makeTime(2035,1,1));
      }
      
      dpSet(\"TimeClkCom.transferName:_original.._value\",dpn_tf);
      clk_startTimedFuncWork(dpn_tf);
    }
  }

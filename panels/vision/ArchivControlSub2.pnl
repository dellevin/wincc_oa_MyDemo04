"bool isConnected;

main()
{
  string dataSet;
  isConnected = false;  
  
  if ( isLightRedundant() || $host == \"\" )
  {
    if (      $fileType == \"_Event\" )
      dataSet = \"_DataSet\";
    else if ( $fileType == \"_Alert\" )
      dataSet = \"_AlertDataSet\";

    if ( $fileType != \"_LA\"  )
    {
      getMediaInfo( $fileType, $host );
      
      if ( dpConnect(\"refresh\", true, $fileType + \"ArchivControl\" + $host + \".Media.CycleRefresh12:_online.._value\" ) == -1 ) 
        setValue( \"\", \"backCol\", \"_dpdoesnotexist\", \"foreCol\", \"_dpdoesnotexist\" );

      if ( dpConnect(\"workNewDir\", true, dataSet + $host + \".List.States:_online.._value\" ) == -1 )
        setValue( \"\", \"backCol\", \"_dpdoesnotexist\", \"foreCol\", \"_dpdoesnotexist\" );

      if ( dpConnect(\"workConnect\", true, $fileType + \"ArchivControl\" + $host + \".Media.Descript:_online.._value\" ) == -1 )
        setValue( \"\", \"backCol\", \"_dpdoesnotexist\", \"foreCol\", \"_dpdoesnotexist\" );
 
      if ( dpConnect(\"workStatus\", true, $fileType + \"ArchivControl\" + $host +  \".StatusMedia:_online.._value\" ) == -1 )
        setValue( \"\", \"backCol\", \"_dpdoesnotexist\" ,\"foreCol\",\" _dpdoesnotexist\" );

      if ( dpConnect(\"doDiscUsage\", true, \"_ArchivDisk\" + $host + \".AvailPerc:_online.._value\" ) == -1 )
        setValue( \"\", \"backCol\",\" _dpdoesnotexist\", \"foreCol\", \"_dpdoesnotexist\" );
    }
  }
}

refresh( string     dp,
         dyn_string ds )
{
  string r;
  int    p,
         h,
         m,
         c,
         cx;
  
  r = ds[1];
  sscanf( ds[2], \"%2d\", p );
  sscanf( ds[3], \"%2d\", h );
  sscanf( ds[4], \"%2d\", m );
  sscanf( ds[5], \"%5d\", cx );

  getValue( \"Media_CycleTime_Count\", \"text\", c );
  refreshMediaCycle( c, h, m );
  setMultiValue( \"Media_CycleTime_Resolution\", \"text\", r,
                 \"Media_CycleTime_C\",          \"text\", p );
  displayMediaFields( $fileType, p );
}
workStatus(string dp, bool wert)
{
  // IM 112404 - modification to read settings for the timed-function
  bool b_validTF;
  time t_from, t_until;
  
  dpGet($fileType + \"FileArchiv\" + $host + \".validFrom\",t_from,
        $fileType + \"FileArchiv\" + $host + \".validUntil\",t_until);
  
  //DebugTN(__LINE__,\"ArchivControlSub1 - check filechange timed-function\",t_from,t_until);
  
  if(t_from < getCurrentTime() && t_until > getCurrentTime())
  {
    b_validTF = 1;
    //DebugTN(__LINE__,\"ArchivControlSub1 - check filechange timed-function\",b_validTF);
  }
  else if(t_from == 0 && t_until == 0)
  {
    b_validTF = 1;
    //DebugTN(__LINE__,\"ArchivControlSub1 - check filechange timed-function\",b_validTF);
  }
  else
  {
    b_validTF = 0;
    //DebugTN(__LINE__,\"ArchivControlSub1 - check filechange timed-function\",b_validTF);
  }
  
  if(wert && b_validTF == 1)
  {
    setMultiValue(\"StatusMedia\",\"backCol\",\"Green\");
  }
  else
  {
    setMultiValue(\"StatusMedia\",\"backCol\",\"Red\");

    // IM 112404 - if timed-function is active but status shell be set inactive deactivate the timed function
    if(b_validTF == 1 && wert == 0)
    {
      dpSetWait($fileType + \"FileArchiv\" + $host + \".validFrom\",0,
                $fileType + \"FileArchiv\" + $host + \".validUntil\",1);
    }
  }
}

doDiscUsage( string dp,
             int    wert )
{
  string s;

  wert = 100 - wert;
  sprintf( s, \"%3d%\", wert );
  setValue( \"Disk_Usage\", \"text\", s );
}

workNewMediaDir( string   dp,
                 dyn_time wert )
{
  getMediaDir( $fileType, $host );
}

workNewDir( string    dp,
            dyn_bit32 wert )
{
  getSystemDir( $fileType, $host );
}

workConnect( string     dp,
             dyn_string wert )
{
  dyn_string ds;
  string     str;

  if( dynlen( wert ) > 1 )
  {
    str = wert[1];
    ds  = dpNames( $fileType + \"ArchivDir\" + str );
    if( dynlen( ds ) != 0 && str != \"\" && isConnected == true )
    {
      dpDisconnect( \"workNewMediaDir\" ,$fileType + \"ArchivDir\" + wert[1] + \".File:_online.._value\" );
      isConnected = false;
    }  
    str = wert[2];
    if ( str != \"\" )
    {
      dpConnect( \"workNewMediaDir\", true, $fileType + \"ArchivDir\" + wert[2] + \".File:_online.._value\" );
      isConnected = true;
    }
    else
    {
      setMultiValue( \"Media_Table\", \"deleteAllLines\",
                     \"Media_Usage\", \"text\", \"\" );
    }
    setValue( \"Media_Descript\", \"text\", str );
  }
}






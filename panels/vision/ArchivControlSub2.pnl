"main()
{
  dpConnect( \"getwert\",\"_MediaConfig.Name:_online.._value\");
  // IM 112396 - change wantsAnswer = true to call refreshMedia() to set the correct information when opening the panel
  dpConnect( \"refreshMedia\", true, $fileType + \"ArchivControl\" + $host + \".Media.Refresh:_online.._value\" );
}

refreshMedia(string dp, bool neu)
{
  int        i,
             p,
             d,
             h,
             m,
             err;
  string     wert;
  dyn_uint   siz,
             dir,
             hou,
             min,
             cyc;
  dyn_string sel,
             dev,
             mou,
             typ,
             day;

  dpGet( \"_MediaConfig.Name:_online.._value\",                                 sel,
         $fileType + \"ArchivControl\" + $host + \".Media.Name:_online.._value\", wert );
  i = 1;
  p = -1;

  while ( i <= dynlen( sel ) && p == -1 )
  {
    if ( sel[i] == wert )
      p = i;
    else
      i++;
  }

  if ( p > 0 )
  {
    dpGet( \"_MediaConfig.Device:_original.._value\",   dev,
           \"_MediaConfig.MountDir:_original.._value\", mou,
           \"_MediaConfig.Type:_original.._value\",     typ,
           \"_MediaConfig.Size:_original.._value\",     siz );

    setValue( \"Media_Device\",   \"text\", dev[p] );
    setValue( \"Media_MountDir\", \"text\", mou[p] );

    // azure dev ops server issue #116923
    displayMediaTypeFields(typ[p]);

    setValue( \"Media_Type\", \"text\", typ[p] );
    setValue( \"Media_Size\", \"text\", siz[p] );
  }
}

getwert(string dp, dyn_string wert )
{
  int i;
  string s_config;

  setValue( \"Media_Name\", \"deleteAllItems\" );

  // azure dev ops server issue #116923
  if ( dynlen( wert ) > 0 )
    setMultiValue( \"Media_Name\", \"items\", wert,
                   \"Media_Name\", \"selectedPos\", 1);

  s_config = Media_Name.selectedText;

  i = 1;

  while ( i <= dynlen( wert ) )
  {
    if ( wert[i] == \"--\" )
    {
      setValue( \"Media_Name\", \"deletePos\", i );
      getValue( \"Media_Name\", \"items\",     wert );
    }
    else
      i++;
  }

  // IM 102665 -->
  // Whenever a new configuration is added the media name combobox
  // (Media_Name) is updated. To make it possible to initialize
  // all parameters have to be provided to all other shapes!
  dyn_uint   siz;
  dyn_string dev, mou, typ;

  dpGet( \"_MediaConfig.Device\",   dev,
         \"_MediaConfig.MountDir\", mou,
         \"_MediaConfig.Type\",     typ,
         \"_MediaConfig.Size\",     siz );

  // Don't initialize if there is no configuration yet
  if ( dynlen(dev) == 0 )
    return;

  // Initialize to first configuration in list if failure to find
  // otherwise initialize to the (combobox-)selected configuration
  int index = dynContains(wert, s_config);

  //DebugTN(__LINE__,\"index of current configuration\",index,wert,s_config);

  if (index < 1 || index > dynlen(typ))
  {
    index = 1;
  }
  else
  {
    //set again the name for the current configuration if it is part of the configuration list
    Media_Name.text = s_config;
  }

  setValue( \"Media_Device\",   \"text\", dev[index] );
  setValue( \"Media_MountDir\", \"text\", mou[index] );

  // azure dev ops server issue #116923
  displayMediaTypeFields(typ[index]);

  setValue( \"Media_Type\", \"text\", typ[index] );
  setValue( \"Media_Size\", \"text\", siz[index] );
  // --> IM 102665
}







"main()
{
  int        i, j, k, iPos, row, col;
  string     sCol, display, group, s32, s;
  dyn_int    diManType, diGroupId;
  dyn_float  df;
  dyn_bit32  dbPermission;
  dyn_string ds, dsDisplayName;

  if ( !getUserPermission(4) )
  {
    ChildPanelOnCentralModal( \"vision/MessageWarning\",
                              getCatStr( \"general\", \"warning\" ),
                              makeDynString( getCatStr( \"general\", \"permission\" )));
    return;
  }
        
  getValue( \"tabelle\", \"currentCell\", row, col);
  if ( row < 0 )
  {
    ChildPanelOnCentralModal( \"vision/MessageWarning\",
                              getCatStr( \"general\", \"warning\" ),
                              makeDynString( getCatStr(\"userMan\",\"nodisplayselected\")));
    return;
  }
  
  id.text = tabelle.cellValueRC(row, tabelle.columnToName(0));
  cmbGroup.text = tabelle.cellValueRC(row, tabelle.columnToName(1));
  s32 = tabelle.cellValueRC(row, \"permission\");
  strreplace(s32, \" \", \"\");
  for ( i = 0; i < strlen(s32); i++ )
  {
    k = i + 2; sCol = \"#\" + k;
    setValue(\"permissions\", \"cellValueRC\", 0, sCol, s32[i]);
  }

  s = getCatStr(\"userMan\",\"suretodeletesetting\");
  strreplace(s, \"%id\", id.text);
  strreplace(s, \"%group\", cmbGroup.text);
  ChildPanelOnCentralModalReturn( \"vision/MessageInfo\",
                                  getCatStr( \"general\", \"warning\" ),
                                  makeDynString(s,
                                                getCatStr(\"para\",\"yes\"),
                                                getCatStr(\"para\",\"no\")),
                                  df, ds);
  if ( dynlen(df) < 1 || !df[1] )
    return;

  dpGet(\"_WsPermission.DisplayName:_original.._value\", dsDisplayName,
        \"_WsPermission.ManType:_original.._value\", diManType,
        \"_WsPermission.GroupId:_original.._value\", diGroupId,
        \"_WsPermission.Permission:_original.._value\", dbPermission);
  
  dynRemove(dsDisplayName, row+1);
  dynRemove(diManType, row+1);
  dynRemove(diGroupId, row+1);
  dynRemove(dbPermission, row+1);

  dpSet(\"_WsPermission.DisplayName:_original.._value\", dsDisplayName,
        \"_WsPermission.ManType:_original.._value\", diManType,
        \"_WsPermission.GroupId:_original.._value\", diGroupId,
        \"_WsPermission.Permission:_original.._value\", dbPermission);

  setValue(\"tabelle\", \"deleteLineN\", row);
  id.text = \"\";
  cmbGroup.text = \"*\";
  for ( j = 0; j < 32; j++ )
  {
    k = j + 2; sCol = \"#\" + k;
    setValue(\"permissions\", \"cellValueRC\", 0, sCol, \"0\");
  }
}

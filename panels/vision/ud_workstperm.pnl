"main()
{
  int            i32, i, j, k, l, iPos;
  bit32          b32;
  string         sBit, sCol, sG, sPerm, sPath, sUnknown = \"\";
  dyn_int        diGroupId, diManType;
  dyn_bit32      dbPermission;
  dyn_string     dsDisplayName, dsGroupId, gNames;
  dyn_langString desliste;

  m_um = UserManagement::getInstance();
  setFirstRowBitsVisible(true);

  if ( !getUserPermission(4) )
  {
    RootPanelOn(\"vision/MessageWarning\",getCatStr(\"para\",\"warning\"),
      getCatStr(\"general\",\"permission\"));
  }

  sPath = getPath(CONFIG_REL_PATH + \"config\");
  cmdUnknown.visible = (paCfgReadValue(sPath, \"general\", \"permUnknownWS\", sUnknown) == 0 && sUnknown != \"\");

  dpGet(\"_WsPermission.DisplayName:_original.._value\", dsDisplayName,
        \"_WsPermission.ManType:_original.._value\", diManType,
        \"_WsPermission.GroupId:_original.._value\", diGroupId,
        \"_WsPermission.Permission:_original.._value\", dbPermission);

  permissions.tableMode (TABLE_SELECT_BROWSE);
  permissions.selectByClick (TABLE_SELECT_LINE_COLUMN);
  permissions.sortOnClick(false);
  permissions.deleteAllLines();
  permissions.appendLine(\"#1\", \"\");
  tabelle.tableMode (TABLE_SELECT_BROWSE);
  tabelle.selectByClick (TABLE_SELECT_LINE);
  tabelle.sortOnClick(false);
  tabelle.selectColors (\"_ButtonShadow\", \"_Window\");
  tabelle.deleteAllLines();

  desliste = m_um.getAllPermissionDescriptions();
  dyn_anytype groups = m_um.getAllGroups();
  for (i = 1; i <= dynlen(groups); i++)
  {
    diGroups[i] = groups[i].getId();
    gNames[i] = groups[i].getName();
    dsGroupNames[i] = groups[i].getFullName();
    if ( strrtrim(strltrim(dsGroupNames[i])) == \"\" )
      dsGroupNames[i] = gNames[i];
  }

  dynInsertAt(dsGroupNames, \"*\",1);
  dynInsertAt(diGroups, \"-1\",1);
  cmbGroup.items = dsGroupNames;
  cmbGroup.selectedPos = 1;

  for ( i = 1; i <= dynlen(dsDisplayName); i++ )
  {
    sPerm = \"\";
    for ( k = 1; k < 33; k++ )
    {
      sPerm += (dbPermission[i][k-1])?\"1\":\"0\";
      if ( k==8 || k==16 || k==24 ) sPerm +=\" \";
    }
    if ( diGroupId[i] == -1 )
      sG = \"*\";
    else
    if ( (iPos=dynContains(diGroups, diGroupId[i])) < 1 )
      sG = \"???\";
    else
      sG = dsGroupNames[iPos];
    setValue(\"tabelle\", \"updateLine\", 2, \"display\",    dsDisplayName[i],
                                         \"groupid\",    diGroupId[i],
                                         \"group\",      sG,
                                         \"permission\", sPerm,
                                         \"mantype\",    diManType[i]);
  }
  permissions.deleteAllLines();
  permissions.appendLine(\"#1\", \"\");
  for ( j = 1; j <= 32; j++ )
  {
    sCol = \"#\" + (j+1);

    if ( j==32)
      sBit = 0;
    else
      sBit = 1;

   setValue(\"permissions\", \"cellValueRC\", 0, sCol, sBit);
   if (dynlen(desliste) >= j)
     setValue (\"permissions\", \"columnToolTipText\", j, desliste[j] + \"(\"+j+\")\");
  }
}

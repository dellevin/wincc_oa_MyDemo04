"main()
{
  int        i, j, k, iPos, mantype = UI_MAN; // UI
  bit32      b32;
  string     display = id.text, sPerm;
  dyn_int    diManType, diGroupId;
  dyn_float  df;
  dyn_bit32  dbPermission;
  dyn_string ds, dsDisplayName;

  if ( !getUserPermission(4) )
  {
    ChildPanelOnCentralModal( \"vision/MessageWarning\",
                              getCatStr( \"general\", \"warning\" ),
                              makeDynString( getCatStr( \"general\", \"permission\" )));
    return;
  }

  getMultiValue( \"id\",\"text\",display); display = strltrim(strrtrim(display));
  if ( display == \"\" )
  {
    ChildPanelOnCentralModal( \"vision/MessageWarning\",
                              getCatStr( \"general\", \"warning\" ),
                              makeDynString( getCatStr(\"userMan\",\"nodisplaydefined\")));
    return;
  }
  if ( !dummyPing(display) )
  {
    ChildPanelOnCentralModalReturn( \"vision/MessageInfo\",
                                    getCatStr( \"general\", \"warning\" ),
                                    makeDynString(getCatStr(\"userMan\",\"displaynotexists\"),
                                                  getCatStr(\"para\",\"yes\"),
                                                  getCatStr(\"para\",\"no\")),
                                    df, ds);
    if ( dynlen(df) < 1 || !df[1] )
      return;
  }

  getValue(\"permissions\", \"getLineN\", 0, ds);
  for ( j = 2; j <= dynlen(ds); j++ )
  {
    k = ds[j];
    setBit (b32, j-2, k);
  }

  sPerm = \"\";
  for ( k = 1; k < 33; k++ )
  {
    sPerm += (b32[k-1])?\"1\":\"0\";
    if ( k==8 || k==16 || k==24 ) sPerm +=\" \";
  }

  dpGet(\"_WsPermission.DisplayName:_original.._value\", dsDisplayName,
        \"_WsPermission.ManType:_original.._value\", diManType,
        \"_WsPermission.GroupId:_original.._value\", diGroupId,
        \"_WsPermission.Permission:_original.._value\", dbPermission);

  iPos = dynlen(dsDisplayName)+1;
  for ( i = 1; i <= dynlen(dsDisplayName); i++ )
  {
    if ( dsDisplayName[i] == display &&
         diManType[i]     == mantype &&
         diGroupId[i]     == diGroups[cmbGroup.selectedPos] )
    {
      iPos = i;
      break;
    }
  }
  dsDisplayName[iPos] = display;
  diManType[iPos]     = mantype;
  diGroupId[iPos]     = diGroups[cmbGroup.selectedPos];
  dbPermission[iPos]  = b32;

  dpSet(\"_WsPermission.DisplayName:_original.._value\", dsDisplayName,
        \"_WsPermission.ManType:_original.._value\", diManType,
        \"_WsPermission.GroupId:_original.._value\", diGroupId,
        \"_WsPermission.Permission:_original.._value\", dbPermission);

//  umSetManagerPermission(getUserId(), 0, display, diGroupId[iPos]);
  setValue(\"tabelle\", \"updateLine\", 2, \"display\",    display,
                                       \"groupid\",    diGroups[cmbGroup.selectedPos],
                                       \"group\",      cmbGroup.text,
                                       \"permission\", sPerm,
                                       \"mantype\",    mantype);
}

bool dummyPing(string display)
{
  return(true);
}



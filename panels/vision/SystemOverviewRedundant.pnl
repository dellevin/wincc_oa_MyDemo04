synchronized setButtonEnabled(string dp, dyn_int di, string dp_2, dyn_int di_2)//IM 111221
{
  bool isInSplitmode;
  bool bSSS =false;

  // IM 109425
  // IM 98329 use $System for getting the SSS system name.
  string sSys;
  sSys = \"\";
  if (isDollarDefined(\"$System\")) {
    sSys = $System;
  }

  //IM 98056: Splitmode is not allowed on SSS in case of 2x2 Redundancy
  string sSSS_SystemName;
  if(dpExists(sSys + \"_2x2Redu.settings.SSS_System\") == TRUE)  // If 2x2 Redundand System
  {
    dpGet(sSys + \"_2x2Redu.settings.SSS_System\",sSSS_SystemName);  //get System Name of SSS System

    if(strrtrim(sSSS_SystemName) != \"\" && isDollarDefined(\"$System\"))
    {
      if(sSSS_SystemName == $System)  //if the Systemoverviewpanel shows the SSS System, no Splitmode is allowed
        bSSS = true;
    }
  }

  // button to enter split mode is disabled if it is the SSS in 2x2 Redundancy
  if ( bSSS )
  {
    setMultiValue(\"buSplitmode\",\"enabled\",false,
                  \"buRedundancymode\",\"enabled\",true);
    return;
  }


  isSplitModeActive(isInSplitmode,sSys);

  // button to enter split mode is disabled if the system is already in split mode
  if ( isInSplitmode )
  {
    setMultiValue(\"buSplitmode\",\"enabled\",false,
                  \"buRedundancymode\",\"enabled\",true);
  }
  // button to enter redundancy mode is disabled if system is not in split mode
  else
  {
    setMultiValue(\"buSplitmode\",\"enabled\",true,
                  \"buRedundancymode\",\"enabled\",false);
  }
}


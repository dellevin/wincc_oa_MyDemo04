disableTime(string dp, time t)
{
  if (t - tOld < 10 && iThreadId != 0)
    stopThread(iThreadId) ;
  iThreadId = startThread(\"disableTimeDelay\");


}

disableTimeDelay()
{
  bool buSplitmodeEnabled = buSplitmode.enabled();
  bool buRedumodeEnabled  = buRedundancymode.enabled();
  bool bSSS = false;

  // IM 109425
  // IM 98329 use $System for getting the SSS system name.
  string sSys;
  sSys = \"\";
  if (isDollarDefined(\"$System\")) {
    sSys = $System;
  }

  //IM 98056: Splitmode is not allowed on SSS in case of 2x2 Redundancy
  string sSSS_SystemName;
  if(dpExists(sSys + \"_2x2Redu.settings.SSS_System\") == TRUE)  // If 2x2 Redundand System
  {
    dpGet(sSys + \"_2x2Redu.settings.SSS_System\",sSSS_SystemName);  //get System Name of SSS System

    if(strrtrim(sSSS_SystemName) != \"\" && isDollarDefined(\"$System\"))
    {
      if(sSSS_SystemName == $System)  //if the Systemoverviewpanel shows the SSS System, no Splitmode is allowed
        bSSS = true;
    }
  }


  setMultiValue(\"buPrefered_L\",\"enabled\",FALSE,
                \"buPrefered_R\",\"enabled\",FALSE,
                \"buActive_L\",\"enabled\", FALSE,
                \"buActive_R\",\"enabled\", FALSE,
                \"buRedundancymode\",\"enabled\", FALSE,
                \"buSplitmode\", \"enabled\", FALSE);
  delay(10);
  iThreadId = 0;
  setMultiValue(\"buPrefered_L\",\"enabled\",TRUE,
                \"buPrefered_R\",\"enabled\",TRUE,
                \"buActive_L\",\"enabled\", TRUE,
                \"buActive_R\",\"enabled\", TRUE,
                \"buRedundancymode\",\"enabled\", buRedumodeEnabled,
                \"buSplitmode\", \"enabled\", (buSplitmodeEnabled && !bSSS));


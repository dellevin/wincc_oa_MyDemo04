threadCheckHostConnection(int iSystemNumber)
{
  bool isDistSystem = iSystemNumber != getSystemId();

  int iEventManIdHost1 = convManIdToInt(EVENT_MAN, 0, iSystemNumber);
  int iEventManIdHost2 = convManIdToInt(EVENT_MAN, 0, iSystemNumber, 2);

  bool bVisibleHost1=TRUE, bVisibleHost2=TRUE;
  bool bConnectionToHost1, bConnectionToHost2;

  while(1)
  {
    getConnectionState(iSystemNumber, isDistSystem, bConnectionToHost1, bConnectionToHost2, iEventManIdHost1, iEventManIdHost2);

    if ( bVisibleHost1 != bConnectionToHost1 )
      setHostVisible(bConnectionToHost1, 1);

    if ( bVisibleHost2 != bConnectionToHost2 )
      setHostVisible(bConnectionToHost2, 2);

    bVisibleHost1 = bConnectionToHost1;
    bVisibleHost2 = bConnectionToHost2;

    delay(0,200);
  }
}

/**
  @author jhercher

  Function has been added to show state of redudist systems correctly in the system overview panel (TFS 4824)
  in case of local system isConnOpen can still be used but in case of a dist system
  the dp _DistConnections.Dist.ManNums is used, if it contains a number bigger than zero it has a connection to
  the first redupartner of dist system if i contains a number smaller than zero it has a connection to
  the second redupartner of dist system. it can also contain both if a connection with both is established.

  @param distSysId ... int system id of dist system
  @param isDistSystem ... bool TRUE if the systemoverview panel shows a dist system, FALSE if it shows local system
  @param bConnectionToHost1 ... bool return parameter TRUE if connection to first redupartner ist established, FALSE if not
  @param bConnectionToHost2 ... bool return parameter TRUE if connection to second redupartner ist established, FALSE if not
  @param iEventManIdHost1 ... int with the EventmanagerId for Host1
  @param iEventManIdHost2 ... int with the EventmanagerId for Host2

  */
void getConnectionState(int distSysId, bool isDistSystem, bool &bConnectionToHost1, bool &bConnectionToHost2,
                        int iEventManIdHost1, int iEventManIdHost2)
{
  bConnectionToHost1 = FALSE;
  bConnectionToHost2 = FALSE;
  if(!isDistSystem)
  {
    bConnectionToHost1 = isConnOpen(iEventManIdHost1);
    bConnectionToHost2 = isConnOpen(iEventManIdHost2);
  }
  else
  {
    dyn_int manNums, manNums2;
    dpGet(\"_DistConnections.Dist.ManNums\", manNums,
          \"_DistConnections_2.Dist.ManNums\", manNums2);

    dynAppend(manNums, manNums2);
    for(int i = 1; i <= dynlen(manNums); i++)
    {
      if(manNums[i] > 0 && distSysId == manNums[i])
      {
        bConnectionToHost1 = TRUE;
      }
      else if(manNums[i] < 0 && distSysId == abs(manNums[i]))
      {
        bConnectionToHost2 = TRUE;
      }
    }
  }
}


"main()
{
  bool       ok;
  int        err;
  int        iloff,
             iva,
             cc,
             ch,
             cd,
             cm,
             cr,
             smd,
             smo,
             swd,
             sd,
             sh,
             sm,
             _swd,
             _smd,
             _smo,
             jahr;
  time       t,
             checkDate;
  dyn_string rt;
  dyn_float  rf;
  string     teile,
             cn;
  int        ss; 

  ok = true;
  getMultiValue( \"Media_CycleTime_Count\",   \"text\",        cc,
                 \"Media_CycleTime_Hours\",   \"text\",        ch,
                 \"Media_CycleTime_Minutes\", \"text\",        cm,
                 \"Media_CycleTime_Days\",    \"text\",        cd,
                 \"Media_CycleTime_C\",       \"text\",        cr,
                 \"Media_SyncTime_MonthDay\", \"selectedPos\", smd,
                 \"Media_SyncTime_Month\",    \"selectedPos\", smo,
                 \"Media_SyncTime_WeekDay\",  \"selectedPos\", swd,       
                 \"Media_SyncTime_Hour\",     \"text\",        sh,
                 \"Media_SyncTime_Minute\",   \"text\",        sm,
                 \"Client_Name\",             \"text\",        cn,
                 \"Save_Destination\",        \"selectedPos\", sd,
                 \"Save_Selection\",          \"selectedPos\", ss ); 

/*
getMultiValue(\"Save_Selection\", \"selectedPos\", iPosSicherung, 
              \"Client_Name\", \"text\", sClientName ); 
*/
 
  if( ss > 1 && cn == \"\" )  
    doError(319);  
  else
  {
    if( daylightsaving( getCurrentTime() ) &&
        ( cr == 3 || cr == 4 ) ) {
      teile = getCatStr( \"archivControl\", \"synchron\" );
      strreplace( teile, \"§\", \"\\n\" );
      ChildPanelOnCentralModalReturn( \"vision/MessageInfo\",
                                      getCatStr( \"archivControl\", \"message\" ),
                                      makeDynString( teile,
                                                     getCatStr( \"archivControl\", \"ja\" ),
                                                     getCatStr( \"archivControl\", \"nein\" ) ),
                                      rf,
                                      rt );
      if( dynlen( rf ) > 0 && rf[1] == 0 )
        ok = false;
    }

    if( cr == 1 ) {
      jahr = year( getCurrentTime() );
      checkDate = makeTime( jahr, smo, smd );
      if( jahr != year( checkDate ) ||
          smo  != month( checkDate ) ||
          smd  != day(   checkDate ) ) {
        doError( 318 );
        ok = false;
      }
    }  

    if( ok ) {
      if(      $fileType == \"_Event\" ) { iloff = 0;  }
      else if( $fileType == \"_Alert\" ) { iloff = 30; }
      else if( $fileType == \"_LA\"    ) { iloff = 60; }
      infoLine( iloff + 106, -1, $host );

      if( (err = setArchivDpMedia( $fileType, $host )) != 0 ) {
        strreplace( teile, \"§\", \"\\n\" );
        doError( err );
      }
      else { 
        switch( cr ) {
          case 1:
            iva  = 365 * 24 * 60 * 60 * 2;
            _smo = smo;
            _smd = smd;
            _swd = -1;
            break;
          case 2:
            iva  =  31 * 24 * 60 * 60 * 2;
            _smo = -1;
            _smd = smd;
            _swd = -1;
            break;
          case 3:
            iva  = 7 * 24 * 60 * 60;  
            _smo = 1;   
            _smd = 1;   
            _swd = swd;
            break;
          case 4:
            iva  = 24 * 60 * 60;  
            _smo = 1; 
            _smd = 1;   
            _swd = 1;   
            break;
          case 5:
            iva  = ch * 3600 + cm * 60 + cd * 3600 * 24;
            _smo = -1;
            _smd = -1;
            _swd = -1;
            break;
        }    
  
        dpSet( $fileType + \"FileArchiv\" + $host + \".validFrom:_original.._value\",   setPeriod(t,0),
               $fileType + \"FileArchiv\" + $host + \".validUntil:_original.._value\",  setPeriod(t,1));
        dpSet( $fileType + \"FileArchiv\" + $host + \".validFrom:_original.._value\",   setPeriod(t,0),
               $fileType + \"FileArchiv\" + $host + \".validUntil:_original.._value\",  setPeriod(t,0),
               $fileType + \"FileArchiv\" + $host + \".time:_original.._value\",        makeDynInt(),
               $fileType + \"FileArchiv\" + $host + \".weekDay:_original.._value\",     makeDynInt(),
               $fileType + \"FileArchiv\" + $host + \".monthDay:_original.._value\",    makeDynInt(),
               $fileType + \"FileArchiv\" + $host + \".month:_original.._value\",       makeDynInt(),
               $fileType + \"FileArchiv\" + $host + \".interval:_original.._value\",    iva,
               $fileType + \"FileArchiv\" + $host + \".syncTime:_original.._value\",    sm * 60 + sh * 60 * 60,
               $fileType + \"FileArchiv\" + $host + \".syncWeekDay:_original.._value\", _swd,
               $fileType + \"FileArchiv\" + $host + \".syncDay:_original.._value\",     _smd,
               $fileType + \"FileArchiv\" + $host + \".syncMonth:_original.._value\",   _smo,
               $fileType + \"FileArchiv\" + $host + \".delay:_original.._value\",       0 );
  
        dpSet( $fileType + \"ArchivControl\" + $host + \".StatusMedia:_original.._value\",                         1,
               $fileType + \"ArchivControl\"         + \".ExecuteClientAction.ClientHost:_original.._value\",      cn,
               $fileType + \"ArchivControl\"         + \".ExecuteClientAction.saveDestination:_original.._value\", sd );
      }
    infoLine( 0, -1, $host );
    }
  }
}" 0

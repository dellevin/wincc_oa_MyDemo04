"main()
{ 
  dpConnect( \"getwert\",             \"_MediaConfig.Name:_online.._value\");
  dpConnect( \"refreshMedia\", false, $fileType + \"ArchivControl\" + $host + \".Media.Refresh:_online.._value\" );
}

refreshMedia(
  string dp,
  bool   neu )
{
  int        i,
             p,
             d,
             h,
             m,
             err;
  string     wert;
  dyn_uint   siz,
             dir,
             hou,
             min,
             cyc;
  dyn_string sel,
             dev,
             mou,
             typ,
             day;

  dpGet( \"_MediaConfig.Name:_online.._value\",                                 sel, 
         $fileType + \"ArchivControl\" + $host + \".Media.Name:_online.._value\", wert );
  i = 1;
  p = -1;
  while( i <= dynlen( sel ) && p == -1 ) {
    if( sel[i] == wert ) p = i;
    else                 i++;
  }    
  if( p > 0 ) {
    dpGet( \"_MediaConfig.Device:_original.._value\",   dev,
           \"_MediaConfig.MountDir:_original.._value\", mou,
           \"_MediaConfig.Type:_original.._value\",     typ,
           \"_MediaConfig.Size:_original.._value\",     siz );
    setValue( \"Media_Device\",   \"text\", dev[p] );
    setValue( \"Media_MountDir\", \"text\", mou[p] );
    if( typ[p] == \"DAT\" || typ[p] == \"QIC\" ) {
      setMultiValue( \"Media_One\",            \"visible\", 1,
                     \"Media_Two\",            \"visible\", 1,
                     \"Media_One\",            \"text\",    \"Rewind\",
                     \"Media_Two\",            \"text\",    \"Eject\" );
      if( _WIN32 ) setMultiValue( \"Media_Device_Title\", \"text\", \"Device:\" );
      else         setMultiValue( \"Media_Device_Title\", \"text\", \"Device:\" );
    }
    else
      if( _WIN32 )
        setMultiValue( \"Media_One\",            \"visible\", 0,
                       \"Media_Two\",            \"visible\", 0,
                       \"Media_One\",            \"text\",    \"Mount\",
                       \"Media_Two\",            \"text\",    \"Unmount\",
                       \"Media_Device_Title\",   \"text\",    \"Backup-Verzeichnis:\" );
      else
        setMultiValue( \"Media_One\",            \"visible\", 1,
                       \"Media_Two\",            \"visible\", 1,
                       \"Media_One\",            \"text\",    \"Mount\",
                       \"Media_Two\",            \"text\",    \"Unmount\",
                       \"Media_Device_Title\",   \"text\",    \"Device:\" );

    setValue( \"Media_Type\", \"text\", typ[p] );
    setValue( \"Media_Size\", \"text\", siz[p] );
  }
}

getwert(
  string     dp,
  dyn_string wert )
{ 
  int i;
  
  setValue( \"\", \"deleteAllItems\" );
  if( dynlen( wert ) > 0 )
    setValue( \"\", \"items\", wert );
  
  i = 1;  
  while( i <= dynlen( wert ) ) {
    if( wert[i] == \"--\" ) {  
      setValue( \"\", \"deletePos\", i ); 
      getValue( \"\", \"items\",     wert );
    }
    else i++;
  }
}

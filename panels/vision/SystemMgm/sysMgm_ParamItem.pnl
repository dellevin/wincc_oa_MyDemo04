private string makeRelativePanelPath(string absolutePathFileName)
{
  absolutePathFileName = makeUnixPath(absolutePathFileName);
  int panelNamePos = strpos(absolutePathFileName, PANELS_REL_PATH);

  if (panelNamePos < 0)
    return \"\";

  return substr(absolutePathFileName, panelNamePos + strlen(PANELS_REL_PATH));
}

bool selectPanelFileName(string &fileName)
{
  string newFileName = fileName;

  int result = fileSelector(newFileName, \"\", true, \"*\", true);

  if (result != 0)
    return false;
  else
  {
    // succeeded, no error occured in fileSelector()
    if (newFileName == \"\") // \"Cancel\" button was clicked
      return false;

    fileName = makeRelativePanelPath(newFileName);
    return true;
  }
}" 0

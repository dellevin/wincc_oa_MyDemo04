void closePanel()
{
  PanelOffReturn(makeDynFloat(0),makeDynString(\"\"));
}

setDollarTab()
{
  dyn_string dsDollarsFromPanel, dsDollarFromDb, dsValuesFromDb;
  dyn_string dsDollars, dsValues;
  dyn_string ds;
  string filename, dollar, actionScript;
  
  filename = txtItemFilename.text;
  //actionScript = txtItemActionScript.text;
  dollar = txtItemDollar.text;
  
  if (filename!=\"\" && getPath(PANELS_REL_PATH, filename)!=\"\")
    dsDollarsFromPanel = getDollarParamsFromPanel(filename);
  else
    dsDollarsFromPanel = makeDynString();
  dollarStringToDollarValue(dollar, dsDollarFromDb, dsValuesFromDb);
  
  //Zu einer Liste zusammenführen
  dynAppend(dsDollars, (dyn_string)dsDollarsFromPanel);
  dynAppend(dsDollars, (dyn_string)dsDollarFromDb);
  dynSortAsc(dsDollars);
  dynUnique(dsDollars);
  
  for (int i=dynlen(dsDollars); i>=1; i--)
  {
    int iDb = dynContains(dsDollarFromDb, dsDollars[i]);
    if (iDb>=1) dsValues[i] = dsValuesFromDb[iDb];
    else dsValues[i] = \"\";
  }
  
  //Tabelle befüllen
  tabDollarParam.deleteAllLines();
  for (int i=1; i<= dynlen(dsDollars); i++)
    ds[i] = \" ...\";
  tabDollarParam.appendLines(dynlen(dsDollars), 
                             \"dollarParams\",    dsDollars,  
                             \"dpSelector\",      ds,
                             \"panelSelector\",   ds,
                             \"colorSelector\",   ds,
                             \"value\",           dsValues);
}

dollarStringToDollarValue(string dollar, dyn_string &dsDollars, dyn_string &dsValues)
{
  dyn_string ds;
  
  dynClear(dsDollars);
  dynClear(dsValues);
  
  //$dollar1:value1|$dollar2:value2|...
  ds = strsplit(dollar, \"|\");
  for (int i=1; i<=dynlen(ds); i++)
  {
    dyn_string ds2;
    if (strpos(ds[i], \":\")<0)
      continue;
    //$dollar1:value1
    ds2 = strsplit(ds[i], \":\");
    dsDollars[i] = ds2[1];  //$dollar1
    dsValues[i] = ds2[2];   //value1
  }
}

string dollarValueToDollarString(dyn_string dsDollars, dyn_string dsValues)
{
  string dollar;
  
  for (int i=1; i<=dynlen(dsDollars); i++)
  {
    if (dsValues[i]!=\"\")
      dollar+=(dollar!=\"\"?\"|\":\"\")+dsDollars[i]+\":\"+dsValues[i];
  }
  return dollar;
}

string selectPicture(string sPicture)
{
  dyn_string ds;
  string s, sPath;
  int ret;
  
  //Aus Filename Filepfad holen
  s = getPath(PICTURES_REL_PATH, sPicture);
  if (s==\"\" || s==PROJ_PATH) s=PROJ_PATH+PICTURES_REL_PATH;  //Wenn nicht gefunden Default verwenden
  
  //Aus Filepfad nur Pfad extrahieren
  strreplace(s, \"\\\\\", \"/\");  //sicherheitshalber
  if (s[strlen(s)-1] != \"/\")  //Nur wenn File angegeben und nicht Pfad
  {
    ds = strsplit(s, \"/\");
    for (int i=1; i<dynlen(ds); i++)
      sPath+=ds[i]+\"/\";
  }
  else
    sPath = s;
  
  //File-Selector öffnen
  ret = fileSelector(s, sPath, TRUE, \"*.png\", TRUE);
  
  //Aus Filepfad nur Filename extrahieren
  for (int i=1; i<=SEARCH_PATH_LEN; i++)
    strreplace(s, getPath(PICTURES_REL_PATH, \"\", getActiveLang(), i), \"\");
  
  if (ret>=0 && s!=\"\")
    sPicture = s;
  
  return sPicture;
}

loadPreview(string sShape, string path)
{
  shape object;
  string s;
  
  object = getShape(sShape);
  s = getPath(PICTURES_REL_PATH, path);
  
  if (path != \"\" && s != \"\" && patternMatch(\"*.png\", s))
  {
    dyn_errClass derr;
    object.fill(\"[pattern,[tile,png,\"+s+\"]]\");
    derr = getLastError();
    if (dynlen(derr)>=1)
      object.fill(\"[solid]\");
  }
  else
    object.fill(\"[solid]\");

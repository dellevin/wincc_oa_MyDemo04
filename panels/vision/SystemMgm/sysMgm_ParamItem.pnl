E"main(int row, string column, string value)
{
  dyn_string ds, dsDollars, dsValues;
  string s;
  int ret;
  
  if (value!=\" ...\" && column!=\"value\") return;
  
  switch (column)
  {
    case \"dpSelector\":
      ret = dpSelector(s, true);
      if(s!=\"\")
      {
        strreplace(s, \"System1:\", \"\");               //NoCheckWarning
        if (strpos(s,\".\")<1) s+=\".\";
      }
      break;
    case \"panelSelector\": ret = panelSelector(s); break;
    case \"colorSelector\": ret = colorEditor(s); break;
    case \"value\": break;
    default: return;
  }
  
  //Wert in Tabelle setzen
  if (s!=\"\")
    setValue(\"\",\"cellValueRC\",row,\"value\",s);
  
  //Alle Werte holen und in Dyns schreiben
  for (int i=1; i<=tabDollarParam.lineCount; i++)
  {
    string sDollar, sValue;
    getValue(\"\",\"cellValueRC\", i-1, \"dollarParams\", sDollar);
    getValue(\"\",\"cellValueRC\", i-1, \"value\", sValue);
    
    if (sValue!=\"\")  //Nur wenn was angegeben ist
    {
      dynAppend(dsDollars, sDollar);
      dynAppend(dsValues, sValue);
    }
  }
  
  //Aus Dyn String machen
  s = dollarValueToDollarString(dsDollars, dsValues);
  
  //In Textfeld schreiben
  txtItemDollar.text = s;
}" 0

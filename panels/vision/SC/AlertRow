"// up

main()
{
  int line, count;
  
  getValue(\"table\", \"lineVisible\", line,
                    \"lineCount\", count);

  if ( line == -1 ) line = count - 1;  // -1 means last line

  if ( line > 0  )
    setMultiValue(\"table\", \"lineVisible\", line - 1,
                  \"ar_currentLine\", \"text\", line);  // \"current line\" indicator is 1 based
}" 0

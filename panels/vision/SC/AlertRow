"// down

main()
{
  int lines, line;
  
  getValue(\"table\", \"lineCount\", lines,
                    \"lineVisible\", line);

  if ( line == -1 ) line = lines - 1;  // -1 means last line

  if ( line < (lines - 1) )
    setMultiValue(\"table\", \"lineVisible\", line + 1,
                  \"ar_currentLine\", \"text\", line + 1 + 1);   // \"current line\" -indicator is 1 based
}" 0

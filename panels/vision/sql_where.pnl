"main()
{
  int        iPos;
  int        iCount;
  string     sWhere;
  string     sOperator;
  dyn_string dsTemp;
  int        n;
  getValue( \"whereList\",         \"selectedPos\", iPos );
  getValue( \"whereListOperator\", \"itemCount\",   iCount );
  getValue( \"where\",             \"text\",        sWhere );
  getValue( \"whereOperator\",     \"text\",        sOperator );
	
  if( iPos > 0 )
  {
    if(( iPos > 1 ) && ( iPos > iCount ))
    {
      // Last element, take the operator before
      setValue( \"whereList\",         \"deletePos\", iPos );
      setValue( \"whereListOperator\", \"deletePos\", iPos -1 );
      
      dsTemp = strsplit( g_dsWhere[iPos -1], \" \" );
      g_dsWhere[iPos -1] = \"\";
      for( n = 1; n <= dynlen( dsTemp ) -1; n ++ )
      {
        g_dsWhere[iPos -1] += dsTemp[n];
        if( n != dynlen( dsTemp ) -1 )
          g_dsWhere[iPos -1] += \" \";
      }
      
      key.enabled           = false;
      operator.enabled      = true;
      whereOperator.enabled = true;
      
      dynRemove( g_dsWhere, iPos );
    }
    else if(( iPos <= 1 ) && ( iPos > iCount ))
    {
      // First and only element, no operator available
      setValue( \"whereList\", \"deletePos\", iPos );
      setValue( \"whereList\", \"selectedPos\", 0 );
      
      key.enabled           = true;
      operator.enabled      = false;
      whereOperator.enabled = false;
      
      dynRemove( g_dsWhere, iPos );
    }
    else
    {
      setValue( \"whereList\",         \"deletePos\", iPos );
      setValue( \"whereListOperator\", \"deletePos\", iPos );
      
      key.enabled           = false;
      operator.enabled      = true;
      whereOperator.enabled = true;
      
      dynRemove( g_dsWhere, iPos );
    }
    
    // In any case
    comp.enabled         = false;
    details.enabled      = false;
    dpSelector.enabled   = false;
    dpSelector.visible   = false;
    select.enabled       = false;
    valueComp.enabled    = false;
    valueCompare.enabled = false;
    where.text         = \"\";
    key.text           = \"\";
    operator.text      = \"\";

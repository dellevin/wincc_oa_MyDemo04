getSelectedItem()
{
  int        iPos;
  string     sWhere;
  dyn_string dsWhereListOperator;
  dyn_string dsWhereParts;
  string     sCaseValueCompare;
  dyn_string dsKey = makeDynString(\"_SYS\",
                                   \"_DP\",
                                   \"_EL\",
                                   \"_DPT\",
                                   \"_ELC\",
                                   \"_LEAF\",
                                   \"valueCompare\");
  
  dyn_string dsComp = makeDynString(\"=\", \"!=\", \"LIKE\", \"NOT LIKE\"); //IM 99702
  getMultiValue( \"whereList\",         \"selectedPos\",   iPos,
                 \"whereList\",         \"selectedText\",  sWhere,
                 \"whereListOperator\", \"items\",         dsWhereListOperator );
  // Decide: with selected row compare value or compare keyword
  strreplace(sWhere,\"NOT LIKE\",\"NOT_LIKE\");
  dsWhereParts = strsplit( sWhere, \" \" );
  
  if ( dynlen(dsWhereParts) )
  {
  
    if(dynlen(dsWhereParts)>1)
    {
      if(dsWhereParts[2] == \"NOT_LIKE\")
      {
        strreplace(dsWhereParts[2], \"_\", \" \");
        strreplace(sWhere, \"NOT_LIKE\", \"NOT LIKE\");
      }
    }
    //IM 99702 -->
    int keyPos = dynContains(dsKey,dsWhereParts[1]);
    if(keyPos > 4)
    {
      dynRemove(dsComp,4);
      dynRemove(dsComp,3);
    }
    comp.items = dsComp;
    //--> IM 99702
    
    sCaseValueCompare = substr( dsWhereParts[1], 1, strlen( dsWhereParts[1] ) -2 );
  
    if( dynContains( g_dsSelect, sCaseValueCompare ) <= 0 )
    {
      // Compare Keyword
      setMultiValue( \"where\",        \"text\", sWhere,
                     \"key\",          \"text\", keyPos?key.items[keyPos]:\"\",
                     \"keyField\",     \"text\", dsWhereParts[1] );
      key.enabled = true;
    
      if( dynlen( dsWhereParts) > 1 )
      {
        setMultiValue( \"comp\",         \"text\", dsWhereParts[2],
                       \"compField\",    \"text\", dsWhereParts[2],
                       \"details\",      \"text\", dsWhereParts[3],
                       \"detailsField\", \"text\", dsWhereParts[3] );
        comp.enabled         = true;
        details.enabled      = true;
        select.enabled       = false;
        valueComp.enabled    = false;
        valueCompare.enabled = false;
        select.text          = \"\";
        valueComp.text       = \"\";
        valueCompare.text    = \"\";
        // If dp elements selected, show dp selector
        if( dsWhereParts[1] == \"_EL\" )
        {
          dpSelector.visible = true;
          dpSelector.enabled = true;
        }
        else
          dpSelector.visible = false;
      }
    }
    else
    {
      // Compare value
      setMultiValue( \"where\",             \"text\", sWhere,
                     \"key\",               \"selectedPos\", 7,
                     \"keyField\",          \"text\", \"valueCompare\",
                     \"valueComp\",         \"text\", dsWhereParts[2],
                     \"valueCompField\",    \"text\", dsWhereParts[2],
                     \"select\",            \"text\", sCaseValueCompare,
                     \"selectField\",       \"text\", dsWhereParts[1],
                     \"valueCompare\",      \"text\", dsWhereParts[3],
                     \"valueCompareField\", \"text\", dsWhereParts[3] );
      key.enabled          = true;
      select.enabled       = true;
      valueComp.enabled    = true;
      valueCompare.enabled = true;
      comp.enabled         = false;
      details.enabled      = false;
      comp.text            = \"\";
      details.text         = \"\";
      dpSelector.visible   = false;
    }
  
    if(( iPos > dynlen( dsWhereListOperator )) && ( iPos > 1 ))
    {
      // Last element, set operator before available
      setValue( \"whereOperator\",     \"text\",        dsWhereListOperator[iPos -1] );
      setValue( \"whereListOperator\", \"selectedPos\", iPos -1 );
      operator.enabled      = false;
      whereOperator.enabled = false;
    }
    else if(( iPos > dynlen( dsWhereListOperator )) && ( iPos <= 1 ))
    {
      // First an only element, no operator available
      operator.enabled      = false;
      whereOperator.enabled = false;
    }
    else
    {
      // Any other
      setValue( \"whereOperator\", \"text\", dsWhereListOperator[iPos] );
      setValue( \"whereListOperator\", \"selectedPos\", iPos );
      setValue( \"operator\", \"text\", dsWhereListOperator[iPos] );
      operator.enabled      = true;
      whereOperator.enabled = true;
    }
  }
}

initDataBGCheck()
{
  if(isRedundant())
    dpConnect(\"workDataBG\",TRUE,\"_DataManager_2.MultiUserMode.Status:_original.._value\",
                                \"_ReduManager_2.Status.Active:_original.._value\");
      
   dpConnect(\"workDataBG\",TRUE,\"_DataManager.MultiUserMode.Status:_original.._value\",
                               \"_ReduManager.Status.Active:_original.._value\");
}

workDataBG(string dp1, bool bConnDataBG, string dp2, bool bActive)
{
  if(isReduDp(dp2))
  {
    bActiveHost2 = bActive;
    bDataBGisRunning2 = bConnDataBG;
  }
  else if(isRedundant())
  {
    bActiveHost1 = bActive;
    bDataBGisRunning1 = bConnDataBG;
  }
  else
  {
    bActiveHost1 = TRUE;//nur einer da, also aktiv
    bDataBGisRunning1 = bConnDataBG;
  }
}

bool checkDataBG()
{
  //Linux does not need this
  if(!_WIN32)
    return TRUE;
	
  if(isRedundant())
    return bActiveHost2?bDataBGisRunning2:bDataBGisRunning1;// keine Berücksichtigung des Splitmodes
  else
    return bDataBGisRunning1;
}
//IM 108050 End

lockDpList()
{
  return;
}

buildHostConnectString(string &answer, bool bOther = false, bool bSplitModeActive = false)
{
  dyn_string dsEventHosts = eventHost();
  dyn_string dsDataHosts = dataHost();
  unsigned udataPort = dataPort();
  unsigned ueventPort = eventPort();
  int hostNum =  1;
  
  if ( bOther )
  {
    if ( hostNum == 1)
      hostNum = 2;
  }
  
  if ( bSplitModeActive )
    hostNum = myReduHostNum();

  answer = \" -data \"+dsDataHosts[hostNum]+ \":\"+udataPort+\" -event \"+dsEventHosts[hostNum]+\":\"+ueventPort;
}

//IM 99785 -->
getSubTypes(dyn_string &dpt)
{
  dyn_dyn_string dds;  
  dyn_string ds,sub;
  
  for(int i = 1; i <= dynlen(dpt); i++)
  {
    dds = dpGetDpTypeRefs(dpt[i]);
    ds = getDynString(dds,1);
    dynUnique(ds);
    dynAppend(sub,ds);
  }
  dynAppend(sub,dpt);
  dpt = sub;
  dynUnique(dpt);
}

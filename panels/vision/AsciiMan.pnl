                \"pwrConfigs\", \"visible\", ((n||m)&&b), // -->I M 99785
                \"linkedDps\", \"visible\", ((n||m)&&c)
                );
 

if ( !young)  {                                              // -> Format 3 or 4 sedelect Filter 

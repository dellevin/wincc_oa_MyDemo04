{
  int      i;
  bool     p,ph, bAll = true;

  if ( button == 0 )
  {
    this.state(0) = state;
    this.state(1) = state;
    this.state(2) = state;
    this.state(3) = state;
    this.state(4) = state;
    this.state(5) = state;
    this.state(6) = state;
    this.state(7) = state;
    subTypen.state(0) = state; // IM 99785 -->
    subTypen.visible = state;
    pwrConfigs.state(0) = state;
    pwrConfigs.visible = state; // --> IM 99785
    linkedDps.state(0) = state;
    linkedDps.visible = state;
    
    // set global variables
    b_typeRef = state;
    b_pwrConfigs = state;
    b_linkedCNS = state;
    
    db_checkBoxStates[1] = state;
    db_checkBoxStates[2] = state;
    db_checkBoxStates[3] = state;
    db_checkBoxStates[4] = state;
    db_checkBoxVisibility[1] = state;
    db_checkBoxVisibility[2] = state;
  }
  else
  {
    for ( i = 1; i <= 7; i++ )
      bAll &= this.state(i);
    
    if(bAll && b_typeRef && b_pwrConfigs && b_linkedCNS)
      this.state(0) = 1;
    else
      this.state(0) = 0;
    
    // IM 99785 -->
    if(button == 1) // DPT
    {
      subTypen.visible = this.state(1);
      subTypen.state(0) = b_typeRef;
    }
    if(button == 2) // DP
    {
      pwrConfigs.visible = this.state(2);
      pwrConfigs.state(0) = b_pwrConfigs;
    }
    // --> IM 99785
    if(button == 7) // CNS
    {
      linkedDps.visible = this.state(7);
      linkedDps.state(0) = b_linkedCNS;
    }
  }
  
  // IM 111157 -->
  if ( this.state(1) == FALSE ) // DPT
    subTypen.state(0) = FALSE;
  if ( this.state(2) == FALSE ) // DP
    pwrConfigs.state(0) = FALSE;
  // --> IM 111157
  if ( this.state(7) == FALSE ) // CNS
    linkedDps.state(0) = FALSE;
  
  getValue(\"\",\"state\",5,p,
              \"state\",6,ph);

  p = p || ph;

  setMultiValue(\"rk\",\"visible\",p,
                \"konfig\",\"visible\",p,
                \"konfigliste\",\"visible\",p,
                \"kadd\",\"visible\",p,
                \"kdel\",\"visible\",p);
}









  lockDpList();
}



void checkCnsDps(bool state)
{
  dyn_string dsD, dsC, dsDC;
  string spr = getCatStr(\"general\", \"all\");
  
  // IM 118172 - variable to store the dplist
  dyn_string ds_dplist;
  
  getMultiValue(\"dp\",     \"items\", dsD,
                \"cnsSel\", \"items\", dsC);  
  
  //IM 118879 initialize global states if not done yet
  if(dynlen(db_checkBoxStates) <= 0)
  {
    db_checkBoxStates[1] = FilterOptionen.state(1);
    db_checkBoxStates[2] = FilterOptionen.state(2);
    db_checkBoxStates[3] = subTypen.state(0);
    db_checkBoxStates[4] = pwrConfigs.state(0);
    db_checkBoxVisibility[1] = subTypen.visible;
    db_checkBoxVisibility[2] = pwrConfigs.visible;
  }

  if ( state )
  {
    // IM 118172 - save the settings for the check boxes
    db_checkBoxStates[1] = FilterOptionen.state(1);
    db_checkBoxStates[2] = FilterOptionen.state(2);
    db_checkBoxStates[3] = subTypen.state(0);
    db_checkBoxStates[4] = pwrConfigs.state(0);

    db_checkBoxVisibility[1] = subTypen.visible;
    db_checkBoxVisibility[2] = pwrConfigs.visible;
    //DebugTN(\"db_checkBoxStates\",db_checkBoxStates);
    //DebugTN(\"db_checkBoxVisibility\",db_checkBoxVisibility);
    
    FilterOptionen.state(1,true);
    FilterOptionen.state(2,true);
    subTypen.visible(true);
    pwrConfigs.visible(true);
    subTypen.state(0,true);
    pwrConfigs.state(0,true);
    
    ds_dplist = g_dsNoCnsDp;
    
    if ( dsC[1] != spr )//not all
    {
      
      for ( int i = 1; i <= dynlen(dsC); i++)
      {       
        dyn_string dsTemp; 
        if ( dsC[i][0] == \".\" )
        {
          cnsGetIdSet(cnsSubStr(dsC[i], CNSSUB_SYS)+dsC[i]+\"*\", CNS_SEARCH_ALL_NAMES, CNS_SEARCH_ALL_LANGUAGES, CNS_DATATYPE_DATAPOINT, dsTemp);
        }
        else
        {
          cnsGetIdSet(dsC[i]+\"*\", CNS_SEARCH_ALL_NAMES, CNS_SEARCH_ALL_LANGUAGES, CNS_DATATYPE_DATAPOINT, dsTemp);
        }
        dynAppend(dsDC,dsTemp);
      }
    }
    else//all
    {
      cnsGetIdSet(\"*\", CNS_SEARCH_ALL_NAMES, CNS_SEARCH_ALL_LANGUAGES, CNS_DATATYPE_DATAPOINT, dsDC);
    }

    if ( dynlen(dsD) > 1 && dsD[1] != spr )
      dp.deleteAllItems();

    if ( dsD[1] == spr && dynlen(dsDC) )
      setValue(\"dp\", \"deleteAllItems\");
  
    if ( dynlen(dsDC) )
    {
      for ( int i = 1; i <= dynlen(dsDC); i++)
      {
        dynAppend(ds_dplist,dpSubStr(dsDC[i], DPSUB_DP));
      }
      dynUnique(ds_dplist);
      setValue(\"dp\",\"items\",ds_dplist);
    }
  }
  else
  {
    setValue(\"dp\", \"deleteAllItems\");

    // IM 118172 - restore the settings for the check boxes
    FilterOptionen.state(1,db_checkBoxStates[1]);
    FilterOptionen.state(2,db_checkBoxStates[2]);
    subTypen.visible(db_checkBoxVisibility[1]);
    pwrConfigs.visible(db_checkBoxVisibility[2]);
    subTypen.state(0,db_checkBoxStates[3]);
    pwrConfigs.state(0,db_checkBoxStates[4]);

    //DebugTN(\"g_dsNoCnsDp\",g_dsNoCnsDp);
    
    if ( !dynlen(g_dsNoCnsDp) )
      setValue(\"dp\", \"appendItem\", spr);
    else
      setValue(\"dp\",\"items\",g_dsNoCnsDp);
//      for ( int i = 1; i <= dynlen(g_dsNoCnsDp); i++ )
//        setValue(\"dp\", \"appendItem\", g_dsNoCnsDp[i]);
  }
}







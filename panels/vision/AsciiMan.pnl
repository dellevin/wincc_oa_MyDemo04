//IM 108050

bool bDataBGisRunning1 = FALSE;
bool bDataBGisRunning2 = FALSE;
bool bActiveHost1 = FALSE;
bool bActiveHost2 = FALSE;

// IM 118172 - save the settings for the check boxes
dyn_bool db_checkBoxStates;
dyn_bool db_checkBoxVisibility;

bool b_typeRef;

void outversClicked()
{
  string lang;
  int num;
  bool n, m, l, a, b, c;
  bool young, young2;

  getValue(\"outvers\",\"number\",num);
  n = (num == 0);  // regular output
  m = (num == 1);  // ASCII Format2
  l = (num > 1);   // Powerconfig
  a = FilterOptionen.state(1); // --> IM 99785; subtypes
  b = FilterOptionen.state(2); // --> IM 99785; powerconfigs

					FilterOptionen.state(5) = FALSE;
					FilterOptionen.state(6) = FALSE;
					FilterOptionen.state(7) = FALSE;
					subTypen.state(0)       = FALSE; // --> IM 99785
          pwrConfigs.state(0)     = FALSE; // --> IM 99785
          linkedDps.state(0)      = FALSE;
					
    
           setMultiValue(\"rk\",\"visible\",0,
                        \"konfig\",\"visible\",0,
                        \"konfigliste\",\"visible\",0,
                        \"kadd\",\"visible\",0,
                        \"kdel\",\"visible\",0,
                        \"subTypen\",\"visible\", 0,//-->IM 99785
                        \"pwrConfigs\",\"visible\", 0//-->IM 99785
                        );
                        
                        
        }
 
  if (young) 
    getValue(\"younger\",\"state\",0,young2);
  young &= young2;

  setMultiValue(\"YY\",\"visible\",young,
                \"TJ\",\"visible\",young,
                \"MM\",\"visible\",young,
                \"TM\",\"visible\",young,
                \"DD\",\"visible\",young,
                \"TT\",\"visible\",young,
                \"hh\",\"visible\",young,
                \"Ts\",\"visible\",young,
                \"mm\",\"visible\",young,
                \"Tm\",\"visible\",young,
                \"jetzt\",\"visible\",young);
  
  
  
  
  
  if (l)    // set only Masterdps 
  {
    dyn_string ds;
    int i;
    dyn_string dsTypes;
    
    dsTypes = dpNames(\"_mp_*\");
    
    for ( i= 1; i<= dynlen(dsTypes); i++)

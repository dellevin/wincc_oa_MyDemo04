"main()
{
  bool filter,outall,outdptyp,outdp,outalias,outoriginal,outpara,outhist,outCNS;
  bool inyes,ininactivate, inNoAlertConfigHist;
  bool lokal,einspr,young,typrefs,pwrconf, nodedps;
  string datei,confdp,name,cname,spr;
  dyn_string dp, dpt,sprachen,conf,confdpl,konfig,cnsPaths,cnsViews;
  dyn_dyn_anytype confliste, liste;
  int x,inout,dpinout,c, pos,i,o;
  string mod;
  unsigned Y,M,D,h,m;
  time reftime;

  getValue(\"confliste\",\"text\",cname,
                       \"items\",conf);
  getValue(\"confdp\",\"items\",confdpl);
  getMultiValue(\"Datei\",\"text\",datei,
           \"InOut\",\"number\",inout);
  getMultiValue(\"FilterOptionen\",\"state\",0,outall,
                \"FilterOptionen\",\"state\",1,outdptyp,
                \"FilterOptionen\",\"state\",2,outdp,
                \"FilterOptionen\",\"state\",3,outalias,
                \"FilterOptionen\",\"state\",4,outoriginal,
                \"FilterOptionen\",\"state\",5,outpara,
                \"FilterOptionen\",\"state\",6,outhist,
                \"FilterOptionen\",\"state\",7,outCNS,
                \"subTypen\",\"state\",0,typrefs,
                \"pwrConfigs\",\"state\",0,pwrconf,
                \"linkedDps\",\"state\",0,nodedps);
  getValue(\"infilter\",\"state\",0,inyes,
                      \"state\",1,ininactivate,
                      \"state\",2,inNoAlertConfigHist);

  getValue(\"lokalzeit\",\"state\",0,lokal);
  getMultiValue(\"outvers\",   \"number\", einspr,
                \"listelang\", \"items\",  sprachen);
  getMultiValue(\"dp\",\"items\",dp,
                \"dptypsel\",\"items\",dpt,
                \"cnsSel\",\"items\",cnsPaths,
                //\"cnsViewsSel\",\"items\",cnsViews,
                \"konfig\",\"items\",konfig);
  getMultiValue(\"younger\",\"state\",0,young,
                \"YY\",\"text\",Y,
                \"MM\",\"text\",M,
                \"DD\",\"text\",D,
                \"hh\",\"text\",h,
                \"mm\",\"text\",m);
  reftime=makeTime(Y,M,D,h,m);

  // Only save typerefs or powerconfigs if DPT or DP is checked
  if (!outdptyp) typrefs = false;
  if (!outdp) pwrconf = false;
  // for CNS
  if (!outCNS) nodedps = false;
  
//------------------------------------------------------
  pos = -1;
  for (c=1;c<=dynlen(conf);c++)
  {
    if (cname==conf[c])
    {
      pos = c;
      c = dynlen(conf) +1;
    }
  }
    
  if (pos < 0)
  {
    if (inout == 0)
    {
      for (i=1;i<=999;i++)
      {
        confdp=\"i_\"+i;
        if (dpExists(confdp)==0)
        {
          dpCreate(confdp,\"_AsciiConfig\");
          i=1000;
        }  
      }
    }
    else
    {
      for (o=1;o<=999;o++)
      {
        confdp=\"o_\"+o;
        if (dpExists(confdp)==0)
        {
          dpCreate(confdp,\"_AsciiConfig\");
          o=1000;
        }  
      }
    }

  }
  else
  {
    confdp=confdpl[pos];
  }
      
//----------------------------------------------------
  dpSet(confdp + \".Name:_original.._value\",cname);
  if (inout == 0)
  {
   dpSet(confdp + \".InFilter.yes:_original.._value\",inyes,
         confdp + \".InFilter.inactivate:_original.._value\",ininactivate,
         confdp + \".InFilter.noAlertConfigHist:_original.._value\",inNoAlertConfigHist,
         confdp + \".Lokalzeit:_original.._value\",lokal,
         confdp + \".Sprachen.Sprachen:_original.._value\",\"alle\");
   dpSet(confdp + \".outputVersion:_original.._value\",outvers.number);

  }
  else
  {
    spr=getCatStr(\"general\",\"all\");
    if (sprachen[1]==spr)
      sprachen=makeDynString(\"alle\");
    if (dp[1]==spr)
      dp=makeDynString(\"alle\");
    if (dpt[1]==spr)
      dpt=makeDynString(\"alle\");	//02.03.2000 xg #4736: im if hat ein \"=\" gefehlt
    if (konfig[1]==spr)
      konfig=makeDynString(\"alle\");
    if (cnsPaths[1]==spr)
      cnsPaths=makeDynString(\"alle\");
//     if (cnsViews[1]==spr)
//       cnsViews=makeDynString(\"alle\");
    
    dpSet(confdp + \".ASCII_Datei:_original.._value\",datei,
          confdp + \".OutFilter.DPTypen:_original.._value\",outdptyp,
          confdp + \".OutFilter.DP:_original.._value\",outdp,
          confdp + \".OutFilter.Alias:_original.._value\",outalias,
          confdp + \".OutFilter.Originalwerte:_original.._value\",outoriginal,
          confdp + \".OutFilter.Parametrierung:_original.._value\",outpara,
          confdp + \".OutFilter.Historisch:_original.._value\",outhist,
          confdp + \".OutFilter.Typreferenzen:_original.._value\",typrefs,
          confdp + \".OutFilter.PwrConfEinst:_original.._value\",pwrconf,
          confdp + \".OutFilter.CNS:_original.._value\",outCNS,
          confdp + \".OutFilter.LinkedDps:_original.._value\",nodedps,
          confdp + \".Lokalzeit:_original.._value\",lokal,
          confdp + \".Sprachen.Mehrsprachig:_original.._value\",einspr,
          confdp + \".Sprachen.Sprachen:_original.._value\",sprachen,
          confdp + \".DP:_original.._value\",dp,
          confdp + \".DPTypen:_original.._value\",dpt,
          confdp + \".Younger.Younger:_original.._value\",young,
          confdp + \".Younger.Zeit:_original.._value\",reftime,     
          confdp + \".Konfig:_original.._value\",konfig,
          //confdp + \".CNSViews:_original.._value\",cnsViews,
          confdp + \".CNSPaths:_original.._value\",cnsPaths);
 
    dpSet(confdp + \".outputVersion:_original.._value\",outvers.number);

  }

  setValue(\"confliste\",\"deleteAllItems\");
  setValue(\"confdp\",\"deleteAllItems\");
  if (inout==0)
    dpQuery(\"SELECT '_original.._value' FROM 'i*.Name' WHERE _DPT = \\\"_AsciiConfig\\\"\",confliste);
  else
    dpQuery(\"SELECT '_original.._value' FROM 'o*.Name' WHERE _DPT = \\\"_AsciiConfig\\\"\",confliste);


  pos = -1;
  for (x=2;x<=dynlen(confliste);x++)
  {
    mod=dpSubStr(confliste[x][1],DPSUB_DP);
    setValue(\"confliste\",\"appendItem\",confliste[x][2]);
    setValue(\"confdp\",\"appendItem\",mod);
    
    if( confliste[x][2] == cname )
      pos = x-1;
  }
  if( pos > -1 )
    setMultiValue( \"confliste\", \"selectedPos\", pos,
                   \"confdp\",    \"selectedPos\", pos );
  
 // setValue(\"confliste\",\"text\",\"\");
}







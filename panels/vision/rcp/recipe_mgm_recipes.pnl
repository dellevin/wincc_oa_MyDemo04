loadSelctedRecept()
{
  // Show all recipes to the selected type.
  int        i,rez_c;
  time       lastmodified;
  langString ls;
  string     rct,rcp,unit,valid,author,user;
  dyn_float  df;
  dyn_string elementtypes,elements,elementunits,rez,desc,ds,name;
  // Show all recipes of the selected type
  getValue(\"cboRecipeType\",\"text\",rct);
  rcp_getAllRecipes(rct,name);
  
  setMultiValue(\"selRcp\",\"items\",       name,
                \"selRcp\",\"selectedPos\", 1,
                \"txtRezcount\",\"text\",   rez_c);
          
  getValue(\"selRcp\",\"selectedText\",rcp);
  // If there is a recipe show its values
  if (rcp != \"\")
  {
    dpGet(rcp +\".description:_online.._value\",  desc,
          rcp +\".author:_online.._value\",       author,
          rcp +\".valid:_online.._value\",        valid,
          rcp +\".user:_online.._value\",         user, 
          rcp +\".lastModified:_online.._value\", lastmodified,
          rct +\".recipes:_online.._value\",      rez);
  
    rez_c = dynlen(rez);
    ls = desc;
  
    // Set colors and stati as needed
    if (valid == TRUE) 
    {
      valid = \"VALID\";
      elpRct.backCol = \"Green\";
    }
    else 
    {
      valid = \"INVALID\";
      elpRct.backCol = \"Red\";
    }
  
    setMultiValue(\"txtDesc\",\"text\",         ls[getActiveLang()],
                  \"txtAuthor\",\"text\",       author,
                  \"txtStatus\",\"text\",       valid,
                  \"txtUser\",\"text\",         user,
                  \"txtLastModified\",\"text\", lastmodified,
                  \"txtRcp\",\"text\",          rcp,
                  \"txtRezcount\",\"text\",     rez_c);
  }
}


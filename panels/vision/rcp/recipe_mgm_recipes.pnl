"main()
{
  //This calls the import- function.
  //After importing, the new recipe(s) will be shown in the panel.
  time     lastmodified;
  int     rez_c;
  langString ls;
  bool     overwrite;
  string    fileName,err,select,desc,author,valid,user,rcp,rct,pattern,search;
  dyn_string name,ds,rez;
  dyn_float   df;
  
  getMultiValue(\"cboRecipeType\",\"text\",select,
          \"txtFieldFile\",\"text\", fileName);
  
  pattern = getPath(DATA_REL_PATH,\"rcp\");
  pattern = strrtrim(pattern);             
  strreplace(pattern, \"\\\\\",\"/\");  
  pattern = pattern + \"/\" + fileName;  
        
  if(access(pattern, F_OK) == -1) 
  {
    string sMessageText=getCatStr(\"general\",\"noFile\");
    ChildPanelOnCentralReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
    return;
  }
  
  // Check selection and permission
  if (!getUserPermission(4))
  {
    string sMessageText=getCatStr(\"STD_Symbols\",\"Text20\");
    ChildPanelOnCentralReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
    return;
  }
  
  if(fileName == \"\")
  {
    string sMessageText=getCatStr(\"Recipe\",\"nofileselected\");
    ChildPanelOnCentral(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText));
    return;
  }
  
  openProgressBar(\"Rezepte\",\"\",getCatStr(\"Recipe\",\"csv_reading\"), \"\", \"\", 1);
  rcp_readRecipesFromFile(fileName,err,overwrite);
  
  if(err == -1)    // Needed type does not exist
  {
    string sMessageText=getCatStr(\"Recipe\",\"rct_error_act\");
    closeProgressBar();
    ChildPanelOnCentralReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
    return;
  }
  
  if(err == -2)    // Already exists
  {
    string sMessageText=getCatStr(\"Recipe\",\"rct_already_exists_overwrite\");
    closeProgressBar();
    ChildPanelOnCentralReturn(\"vision/MessageWarning2\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText,
                  \"$2:\"+getCatStr(\"general\",\"OK\"),\"$3:\"+getCatStr(\"general\",\"cancel\")),df,ds);
    openProgressBar(\"Rezepte\",\"\",getCatStr(\"Recipe\",\"csv_reading\"), \"\", \"\", 1);
    if (df[1]== 0) // Don't overwrite
    {
      if(select != \"\")
      {
        dpGet(select+\".recipes:_original.._value\",name);
        selRcp.items = name;
        closeProgressBar();
        return;
      }
    }
    else      // Overwrite 
    {
      overwrite = 1;
      if (err == \"-2\") err = \"0\";
      rcp_readRecipesFromFile(fileName,err,overwrite);
    }
  }
  closeProgressBar();
  
  if(err == -3)    // Set to invalid
  {
    string sMessageText=getCatStr(\"Recipe\",\"rcp_common_error\");
  if(select != \"\")
    {
      dpGet(select+\".recipes:_original.._value\",name);
      selRcp.items = name;
    }
    ChildPanelOnCentral(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText));
  } 
  
  if (err == 0)    // OK
  {
    string sMessageText=getCatStr(\"Recipe\",\"rcp_csv_imported\");
    if(select != \"\")
    {
      dpGet(select+\".recipes:_original.._value\",name);
      selRcp.items = name;
    }
    ChildPanelOnCentralReturn(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText),df,ds);
  }
  
  // Show new values in the panel
  if(select != \"\")
  {
    dpGet(select+\".recipes:_original.._value\",name);
    
    selRcp.items = name;   
    selRcp.selectedPos = selRcp.itemCount;
    getMultiValue(\"selRcp\",\"selectedText\",rcp,
            \"cboRecipeType\", \"text\",rct);
    
    if (rcp != \"\")
    {
      dpGet(rcp +\".description:_online.._value\",  desc,
          rcp +\".author:_online.._value\",      author,
          rcp +\".valid:_online.._value\",      valid,
          rcp +\".user:_online.._value\",      user, 
          rcp +\".lastModified:_online.._value\",  lastmodified,
          rct +\".recipes:_online.._value\",    rez);
  
      ls = desc;
      rez_c = dynlen(rez);
    
      // Set colors and stati as needed
      if (valid == TRUE) 
      {
        valid = \"VALID\";
        elpRct.backCol = \"Green\";
      }
      else 
      {
        valid = \"INVALID\";
        elpRct.backCol = \"Red\";
      }
  
      setMultiValue(\"txtDesc\",\"text\",       ls[getActiveLang()],
              \"txtAuthor\",\"text\",     author,
              \"txtStatus\",\"text\",     valid,
              \"txtUser\",\"text\",      user,
              \"txtLastModified\",\"text\",  lastmodified,
              \"txtRcp\",\"text\",      rcp,
              \"txtRezcount\",\"text\",    rez_c);    
    }
  }
}


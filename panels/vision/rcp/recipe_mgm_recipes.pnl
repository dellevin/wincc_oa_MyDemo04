 E "main()
{
  // IM 117020, TFS 4337
  if ( globalExists(\"g_bRcpChangedFromRecipePanel\") )
    g_bRcpChangedFromRecipePanel = TRUE;
  
  //This opens the edit recipe -panel.
  //After editing the new values will be displayed in the fields.
  time     lastmodified;
  langString ls;
  int      rez_c;
  string    rcp,rct,unit,valid,author,user;
  dyn_string ds,desc,rez;
  dyn_float  df;
  
  getMultiValue(\"cboRecipeType\",\"text\", rct,
                \"selRcp\",\"selectedText\",rcp);
  
  // Check selection and permission
  if(rcp==\"\")
  {
    string sMessageText=getCatStr(\"para\",\"nolineselected\");
    ChildPanelOnCentral(\"vision/MessageInfo1\",getCatStr(\"general\",\"information\"),makeDynString(sMessageText));
    return;
  }
  
  if (!dpExists(rcp)) 
  {
    string sMessageText=getCatStr(\"Recipe\", \"recipe_not_exists\");
    ChildPanelOnCentralReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
    return;
  }
  
  if (!getUserPermission(4))
  {
    string sMessageText=getCatStr(\"STD_Symbols\",\"Text20\");
    ChildPanelOnCentralReturn(\"vision/MessageWarning\",getCatStr(\"general\",\"warning\"),makeDynString(sMessageText),df,ds);
    return;
  }
  
  
  // Open the recipe panel
  ChildPanelOnCentralModalReturn(\"vision/rcp/recipe_recipes.pnl\",
  getCatStr(\"Recipe\",\"title_of_rcp_edit\"), makeDynString(\"$dpt:\" + rcp, \"$erw:\" + \"\"), df, ds);
  
  
  // Show new values in the panel
  dpGet(rcp +\".description:_online.._value\",  desc,
        rcp +\".author:_online.._value\",      author,
        rcp +\".valid:_online.._value\",      valid,
        rcp +\".user:_online.._value\",      user, 
        rcp +\".lastModified:_online.._value\",  lastmodified,
        rct +\".recipes:_online.._value\",    rez);
  
  ls = desc;
  rez_c = dynlen(rez);
  
  // Set colors and stati as needed
  if (valid == TRUE) 
  {
    valid = \"VALID\";
    elpRct.backCol = \"Green\";
  }
  else 
  {
    valid = \"INVALID\";
    elpRct.backCol = \"Red\";
  }
  
  setMultiValue(\"txtDesc\",\"text\",       ls[getActiveLang()],
          \"txtAuthor\",\"text\",     author,
          \"txtStatus\",\"text\",     valid,
          \"txtUser\",\"text\",      user,
          \"txtLastModified\",\"text\",   lastmodified,
          \"txtRcp\",\"text\",      rcp,
          \"txtRezcount\",\"text\",    rez_c);
}





